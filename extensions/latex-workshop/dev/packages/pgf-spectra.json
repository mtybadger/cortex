{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "pgfspectra"
    },
    {
      "name": "pgfspectra",
      "arg": {
        "format": "[]",
        "snippet": "pgfspectra[${1:options%keyvals}]",
        "keys": [
          "\\pgfspectra,\\pgfspectraStyle,\\pgfspectraplotshade"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "wlcolor",
      "arg": {
        "format": "{}",
        "snippet": "wlcolor{${1:wavelength}}"
      }
    },
    {
      "name": "pgfspectraStyle",
      "arg": {
        "format": "[]",
        "snippet": "pgfspectraStyle[${1:options%keyvals}]",
        "keys": [
          "\\pgfspectra,\\pgfspectraStyle,\\pgfspectraplotshade"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfspectraStyleReset"
    },
    {
      "name": "pgfspectrashade",
      "arg": {
        "format": "(){}",
        "snippet": "pgfspectrashade(${2:start},${3:end}){${1:name}}"
      }
    },
    {
      "name": "pgfspectrashade",
      "arg": {
        "format": "[](){}",
        "snippet": "pgfspectrashade[${2:h|v}](${3:start},${4:end}){${1:name}}"
      }
    },
    {
      "name": "usepgfspectralibrary",
      "arg": {
        "format": "{}",
        "snippet": "usepgfspectralibrary{${1:libraries%keyvals}}",
        "keys": [
          "\\usepgfspectralibrary#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfspectradata",
      "arg": {
        "format": "{}",
        "snippet": "pgfspectradata{${1:options%keyvals}}",
        "keys": [
          "\\pgfspectradata"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfspectradata",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfspectradata[${2:name}]{${1:options%keyvals}}",
        "keys": [
          "\\pgfspectradata"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "pgfspectratable"
    },
    {
      "name": "pgfspectratable",
      "arg": {
        "format": "{}",
        "snippet": "pgfspectratable{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectratable",
      "arg": {
        "format": "[]",
        "snippet": "pgfspectratable[${1:options%keyvals}]",
        "keys": [
          "\\pgfspectratable"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfspectratable",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfspectratable[${2:options%keyvals}]{${1:dataset names}}",
        "keys": [
          "\\pgfspectratable"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfspectrawrite"
    },
    {
      "name": "pgfspectrawrite",
      "arg": {
        "format": "{}",
        "snippet": "pgfspectrawrite{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectrawrite",
      "arg": {
        "format": "[]",
        "snippet": "pgfspectrawrite[${1:filename}]"
      }
    },
    {
      "name": "pgfspectrawrite",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfspectrawrite[${2:filename}]{${1:dataset names}}"
      }
    },
    {
      "name": "pgfspectraplotmap",
      "arg": {
        "format": "{}",
        "snippet": "pgfspectraplotmap{${1:name}}"
      }
    },
    {
      "name": "pgfspectraplotmap",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfspectraplotmap[${2:l|h}]{${1:name}}"
      }
    },
    {
      "name": "pgfspectraplotshade",
      "arg": {
        "format": "{}",
        "snippet": "pgfspectraplotshade{${1:name}}"
      }
    },
    {
      "name": "pgfspectraplotshade",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfspectraplotshade[${2:options%keyvals}]{${1:name}}",
        "keys": [
          "\\pgfspectraplotshade"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "tempercolor",
      "arg": {
        "format": "{}",
        "snippet": "tempercolor{${1:Kelvin}}"
      }
    },
    {
      "name": "pgfspectrarainbow",
      "arg": {
        "format": "{}",
        "snippet": "pgfspectrarainbow{${1:radius}}"
      }
    },
    {
      "name": "pgfspectrarainbow",
      "arg": {
        "format": "(){}",
        "snippet": "pgfspectrarainbow(${2:options%keyvals}){${1:radius}}"
      }
    },
    {
      "name": "pgfspectrarainbow",
      "arg": {
        "format": "[](){}",
        "snippet": "pgfspectrarainbow[${2:TikZ options}](${3:options%keyvals}){${1:radius}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pgf-spectra#c": [
      "LSE",
      "NIST"
    ],
    "\\pgfspectra,\\pgfspectraStyle,\\pgfspectraplotshade": [
      "use visible shading",
      "width=",
      "height=",
      "element=${1:symbol(s)}",
      "charge=${1:<integer> or all}",
      "Imin=${1:factor}",
      "relative intensity",
      "relative intensity threshold=${1:factor}",
      "line intensity=${1:number}",
      "gamma=${1:number}",
      "brightness=${1:factor}",
      "back=",
      "backIRUV=",
      "IRcolor=",
      "UVcolor=",
      "lines={${1:wavelengths}}",
      "line width=",
      "begin=${1:wavelength}",
      "end=${1:wavelength}",
      "absorption",
      "axis",
      "axis step=${1:integer}",
      "axis ticks=${1:integer}",
      "axis unit=${1|nm,micron,A|}",
      "axis unit precision=${1:integer}",
      "axis label",
      "axis label text={${1:text}}",
      "axis label position=${1|left,center,right|}",
      "axis color=",
      "axis font=${1:font commands}",
      "axis font color=",
      "label",
      "label position=${1|north west,north,north east,west,east,south west,south,south east|}",
      "label font=${1:font commands}",
      "label font color=",
      "label before text=${1:text}",
      "label after text=${1:text}",
      "redshift=${1:value}",
      "show redshift value"
    ],
    "\\usepgfspectralibrary#c": [
      "data",
      "pgfplots",
      "tempercolor",
      "rainbow"
    ],
    "\\pgfspectradata": [
      "element=${1:symbol(s)}",
      "charge=${1:<integer> or all}",
      "Imin=${1:factor}",
      "redshift=${1:value}",
      "begin=${1:wavelength}",
      "end=${1:wavelength}",
      "relative intensity",
      "precision=${1:integer}",
      "unit=${1|nm,micron,A|}"
    ],
    "\\pgfspectratable": [
      "title={${1:text}}",
      "back color=",
      "data back color=",
      "text color=",
      "width=",
      "elements column width="
    ],
    "\\pgfspectraplotshade": [
      "shade begin=${1:value}",
      "shade end=${1:value}",
      "shade opacity=${1:factor}",
      "shade opacity color=",
      "logarithmic"
    ],
    "\\pgfspectrarainbow": [
      "rainbow start=${1:fraction}",
      "rainbow knock out=${1:value}",
      "rainbow fade=${1:fading}",
      "rainbow transparency=${1:factor}",
      "rainbow background="
    ]
  },
  "args": [
    "\\usepackage/pgf-spectra#c"
  ]
}