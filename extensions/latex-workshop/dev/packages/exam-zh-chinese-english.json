{
  "deps": [
    {
      "name": "tcolorbox"
    },
    {
      "name": "tcolorboxlibrarymost"
    },
    {
      "name": "varwidth"
    }
  ],
  "macros": [
    {
      "name": "linelistset",
      "arg": {
        "format": "{}",
        "snippet": "linelistset{${1:list}}"
      }
    },
    {
      "name": "linelistset",
      "arg": {
        "format": "[]{}",
        "snippet": "linelistset[${2:键值列表%keyvals}]{${1:list}}",
        "keys": [
          "\\begin{lineto},\\linelistset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "lineconnect",
      "arg": {
        "format": "{}",
        "snippet": "lineconnect{${1:list}}"
      }
    },
    {
      "name": "lineconnect",
      "arg": {
        "format": "[]{}",
        "snippet": "lineconnect[${2:TikZ keys}]{${1:list}}"
      }
    },
    {
      "name": "zhu",
      "arg": {
        "format": "{}",
        "snippet": "zhu{${1:注释%text}}"
      }
    },
    {
      "name": "zhu",
      "arg": {
        "format": "[]{}",
        "snippet": "zhu[${2:数}]{${1:注释%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "select"
    },
    {
      "name": "select",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]",
        "keys": [
          "\\begin{select}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "lineto"
    },
    {
      "name": "lineto",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]",
        "keys": [
          "\\begin{lineto},\\linelistset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "material"
    },
    {
      "name": "material",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]",
        "keys": [
          "\\begin{material}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "poem"
    },
    {
      "name": "poem",
      "arg": {
        "format": "[]",
        "snippet": "[${1:键值列表%keyvals}]",
        "keys": [
          "\\begin{poem}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "writingbox"
    },
    {
      "name": "writingbox",
      "arg": {
        "format": "[]",
        "snippet": "[${1:tcolorbox keys}]"
      }
    }
  ],
  "keys": {
    "\\examsetup,\\ExamPrintAnswerSet": [
      "select={${1:键值列表}}",
      "select/mark-symbol=${1:symbol}",
      "select/show-mark",
      "select/mark-position=${1|top,above,below,bottom,left,right|}",
      "select/mark-xshift=",
      "select/mark-yshift=",
      "select/separator=${1:symbol}",
      "select/pre-content=${1:content}",
      "select/post-content=${1:content}",
      "lineto={${1:键值列表}}",
      "lineto/xsep=",
      "lineto/ysep=",
      "lineto/name=${1:name}",
      "material={${1:键值列表}}",
      "material/title=${1:标题}",
      "material/title-format=${1:格式}",
      "material/author=${1:作者}",
      "material/author-format=${1:格式}",
      "material/title-author-sep=",
      "material/format=${1:格式}",
      "material/source=${1:来源出处}",
      "material/source-format=${1:格式}",
      "material/top-sep=",
      "material/bottom-sep=",
      "material/title-material-sep=",
      "poem={${1:键值列表}}",
      "poem/title=${1:标题}",
      "poem/title-format=${1:格式}",
      "poem/author=${1:作者}",
      "poem/author-format=${1:格式}",
      "poem/title-author-sep=",
      "poem/format=${1:格式}",
      "poem/align=${1|l,c,r|}",
      "poem/top-sep=",
      "poem/bottom-sep=",
      "poem/title-poem-sep=",
      "poem/type=${1|minipage,chinese|}",
      "poem/zhu-circlednumber-base=${1|tikz,font|}"
    ],
    "\\begin{select}": [
      "mark-symbol=${1:symbol}",
      "show-mark",
      "mark-position=${1|top,above,below,bottom,left,right|}",
      "mark-xshift=",
      "mark-yshift=",
      "separator=${1:symbol}",
      "pre-content=${1:content}",
      "post-content=${1:content}"
    ],
    "\\begin{lineto},\\linelistset": [
      "xsep=",
      "ysep=",
      "name=${1:name}"
    ],
    "\\begin{material}": [
      "title=${1:标题}",
      "title-format=${1:格式}",
      "author=${1:作者}",
      "author-format=${1:格式}",
      "title-author-sep=",
      "format=${1:格式}",
      "source=${1:来源出处}",
      "source-format=${1:格式}",
      "top-sep=",
      "bottom-sep=",
      "title-material-sep="
    ],
    "\\begin{poem}": [
      "title=${1:标题}",
      "title-format=${1:格式}",
      "author=${1:作者}",
      "author-format=${1:格式}",
      "title-author-sep=",
      "format=${1:格式}",
      "align=${1|l,c,r|}",
      "top-sep=",
      "bottom-sep=",
      "title-poem-sep=",
      "type=${1|minipage,chinese|}",
      "zhu-circlednumber-base=${1|tikz,font|}"
    ]
  },
  "args": []
}