{
  "deps": [
    {
      "name": "CJKutf8"
    },
    {
      "name": "cjkutf8-nanummjhanja"
    },
    {
      "name": "cjkutf8-josa"
    },
    {
      "name": "kolabels-utf"
    },
    {
      "name": "ulem",
      "if": "usedotemph"
    },
    {
      "name": "CJKfntef",
      "if": "usedotemph"
    },
    {
      "name": "konames-utf",
      "if": "hangul"
    }
  ],
  "macros": [
    {
      "name": "dotemph",
      "if": "usedotemph",
      "arg": {
        "format": "{}",
        "snippet": "dotemph{${1:text}}"
      }
    },
    {
      "name": "dotemphchar",
      "if": "usedotemph",
      "unusual": true
    },
    {
      "name": "dotemphraise",
      "if": "usedotemph",
      "unusual": true
    },
    {
      "name": "CJKscale",
      "arg": {
        "format": "{}",
        "snippet": "CJKscale{${1:factor}}"
      }
    },
    {
      "name": "lowerCJKchar",
      "arg": {
        "format": "{}",
        "snippet": "lowerCJKchar{${1:length}}"
      }
    },
    {
      "name": "CJKpostmathglue",
      "unusual": true
    },
    {
      "name": "HangulGlue",
      "unusual": true
    },
    {
      "name": "HangulPenalty",
      "unusual": true
    },
    {
      "name": "cancelCJKscale",
      "unusual": true
    },
    {
      "name": "cancellowerCJKchar",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/cjkutf8-ko#c": [
      "hangul",
      "hanja",
      "usecjkt1font",
      "usedotemph",
      "nojosa",
      "truetype",
      "mj=${1:font}",
      "gt=${1:font}"
    ]
  },
  "args": [
    "\\usepackage/cjkutf8-ko#c"
  ]
}