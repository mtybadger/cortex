      "name": "setyagenumeratekeys",
      "arg": {
        "format": "{}",
        "snippet": "setyagenumeratekeys{${1:keyvals}}",
        "keys": [
          "\\setyagusylokeys,\\defdingname,\\setyagenumeratekeys",
          "\\newenumpattern,\\begin{yagenumerate},\\setyagenumeratekeys"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "yagnumber",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "yagnumber{${1:symfam}}{${2:counter}}{${3:color}}"
