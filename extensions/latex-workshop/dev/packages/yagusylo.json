      "name": "setyagitemize",
      "arg": {
        "format": "{}",
        "snippet": "setyagitemize{${1:symfam1, number1, color1. symfam2, number2, color2. ...}}"
      }
    },
    {
      "name": "setyagitemize*",
      "arg": {
        "format": "{}",
        "snippet": "setyagitemize*{${1:dingname1. dingname2. ...}}"
      }
