  "envs": [
    {
      "name": "yagitemize",
      "arg": {
        "format": "{}",
        "snippet": "{${1:char-num}}"
      }
    },
    {
      "name": "yagitemize",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:char-num}}[${2:color}]"
      }
    },
    {
      "name": "yagitemize",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:symfam%keyvals}]{${1:char-num}}",
        "keys": [
          "\\yagding,\\defdingname,\\begin{yagitemize}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "yagitemize",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:symfam%keyvals}]{${1:char-num}}[${3:color}]",
        "keys": [
          "\\yagding,\\defdingname,\\begin{yagitemize}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "yagitemize*"
    },
    {
      "name": "yagitemize*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:ding-name}]"
      }
    },
    {
      "name": "yagenumerate"
    },
    {
      "name": "yagenumerate",
      "arg": {
        "format": "[]",
        "snippet": "[${1:keyvals}]",
        "keys": [
          "\\newenumpattern,\\begin{yagenumerate},\\setyagenumeratekeys"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "notyagenum"
    }
  ],
  "keys": {
    "\\usepackage/yagusylo#c": [
      "info=${1|mute,normal,verbose|}",
      "onerror=${1|nice,tough|}",
      "color",
      "XcolorOptions={${1:xcolor options}}",
      "configfile"
    ],
    "\\setyagusylokeys,\\defdingname,\\setyagenumeratekeys": [
      "*"
    ],
    "\\setyagusylokeys,\\yagfill,\\yagfill*,\\yagfill+,\\yagline,\\yagline*,\\yagline+": [
      "symfam=${1|marvosym,fourier,wasysym,bbding,dingbat,ark,ifsym,ifsymgeo,ifsymgeonarrow,ifsymgeowide,ifsymclock,ifsymweather,pifont|}",
      "symcolor=",
      "leadtype=${1|l,c,x|}",
      "symplace=${1|c,r,l,a,n|}",
      "sympos=${1:integer 0-1000}",
      "boxwidth=",
      "before=",
      "after=",
      "head=",
      "tail="
    ],
    "\\yagding,\\defdingname,\\begin{yagitemize}": [
      "marvosym",
      "fourier",
      "wasysym",
      "bbding",
      "dingbat",
      "ark",
      "ifsym",
      "ifsymgeo",
      "ifsymgeonarrow",
      "ifsymgeowide",
      "ifsymclock",
      "ifsymweather",
      "pifont"
    ],
    "\\newenumpattern,\\begin{yagenumerate},\\setyagenumeratekeys": [
      "symfam=${1|marvosym,fourier,wasysym,bbding,dingbat,ark,ifsym,ifsymgeo,ifsymgeonarrow,ifsymgeowide,ifsymclock,ifsymweather,pifont|}",
      "symcolor=",
      "firstitemnum=${1:integer 0-255}",
      "enumlength=${1:integer}",
      "enumpattern=${1:name}"
    ]
  },
  "args": [
    "\\usepackage/yagusylo#c"
  ]
}