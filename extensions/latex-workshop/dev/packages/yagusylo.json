    {
      "name": "yagline",
      "arg": {
        "format": "{}",
        "snippet": "yagline{${1:char-num}}"
      }
    },
    {
      "name": "yagline",
      "arg": {
        "format": "[]{}",
        "snippet": "yagline[${2:options%keyvals}]{${1:char-num}}",
        "keys": [
          "\\setyagusylokeys,\\yagfill,\\yagfill*,\\yagfill+,\\yagline,\\yagline*,\\yagline+"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "yagline*",
      "arg": {
        "format": "{}",
        "snippet": "yagline*{${1:ding-name}}"
      }
    },
    {
      "name": "yagline*",
      "arg": {
        "format": "[]{}",
        "snippet": "yagline*[${2:options%keyvals}]{${1:ding-name}}",
        "keys": [
          "\\setyagusylokeys,\\yagfill,\\yagfill*,\\yagfill+,\\yagline,\\yagline*,\\yagline+"
        ],
        "keyPos": 0
      }
    },
