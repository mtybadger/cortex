{
  "deps": [
    {
      "name": "ifpdf"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "CJK"
    },
    {
      "name": "CJKutf8"
    },
    {
      "name": "CJKspace"
    },
    {
      "name": "CJKpunct"
    }
  ],
  "macros": [
    {
      "name": "setCJKmainfont",
      "arg": {
        "format": "{}",
        "snippet": "setCJKmainfont{${1:字体文件名}}"
      }
    },
    {
      "name": "setCJKmainfont",
      "arg": {
        "format": "{}[]",
        "snippet": "setCJKmainfont{${1:字体文件名}}[${2:选项}]"
      }
    },
    {
      "name": "setCJKromanfont",
      "arg": {
        "format": "{}",
        "snippet": "setCJKromanfont{${1:字体文件名}}"
      }
    },
    {
      "name": "setCJKromanfont",
      "arg": {
        "format": "{}[]",
        "snippet": "setCJKromanfont{${1:字体文件名}}[${2:选项}]"
      }
    },
    {
      "name": "setCJKsansfont",
      "arg": {
        "format": "{}",
        "snippet": "setCJKsansfont{${1:字体文件名}}"
      }
    },
    {
      "name": "setCJKsansfont",
      "arg": {
        "format": "{}[]",
        "snippet": "setCJKsansfont{${1:字体文件名}}[${2:选项}]"
      }
    },
    {
      "name": "setCJKmonofont",
      "arg": {
        "format": "{}",
        "snippet": "setCJKmonofont{${1:字体文件名}}"
      }
    },
    {
      "name": "setCJKmonofont",
      "arg": {
        "format": "{}[]",
        "snippet": "setCJKmonofont{${1:字体文件名}}[${2:选项}]"
      }
    },
    {
      "name": "setCJKfamilyfont",
      "arg": {
        "format": "{}{}",
        "snippet": "setCJKfamilyfont{${1:族}}{${2:字体文件名}}"
      }
    },
    {
      "name": "setCJKfamilyfont",
      "arg": {
        "format": "{}{}[]",
        "snippet": "setCJKfamilyfont{${1:族}}{${2:字体文件名}}[${3:选项}]"
      }
    },
    {
      "name": "CJKrmdefault",
      "unusual": true
    },
    {
      "name": "CJKsfdefault",
      "unusual": true
    },
    {
      "name": "CJKttdefault",
      "unusual": true
    },
    {
      "name": "CJKfamilydefault",
      "unusual": true
    },
    {
      "name": "CJKslant",
      "unusual": true
    },
    {
      "name": "CJKboldslant",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/zhmCJK#c": [
      "encoding=${1:编码}",
      "pdffakebold",
      "AutoFakeBold=${1:数字}",
      "AutoFakeSlant=${1:数字}",
      "cmap=${1:CMap 值}",
      "embed"
    ]
  },
  "args": [
    "\\usepackage/zhmCJK#c"
  ]
}