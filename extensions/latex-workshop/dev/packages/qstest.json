{
  "deps": [
    {
      "name": "makematch"
    },
    {
      "name": "verbatim"
    }
  ],
  "macros": [
    {
      "name": "IncludeTests",
      "arg": {
        "format": "{}",
        "snippet": "IncludeTests{${1:pattern list}}"
      }
    },
    {
      "name": "TestErrors",
      "arg": {
        "format": "{}",
        "snippet": "TestErrors{${1:pattern list}}"
      }
    },
    {
      "name": "LogTests",
      "arg": {
        "format": "{}{}{}",
        "snippet": "LogTests{${1:file ext}}{${2:keywordlist1}}{${3:keywordlist2}}"
      }
    },
    {
      "name": "LogClose",
      "arg": {
        "format": "{}",
        "snippet": "LogClose{${1:file ext}}"
      }
    },
    {
      "name": "Expect",
      "arg": {
        "format": "{}{}",
        "snippet": "Expect{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "Expect*",
      "arg": {
        "format": "{}{}",
        "snippet": "Expect*{${1:arg1}}{${2:arg2}}"
      }
    },
    {
      "name": "Expect*",
      "arg": {
        "format": "{}*{}",
        "snippet": "Expect*{${1:arg1}}*{${2:arg2}}"
      }
    },
    {
      "name": "ExpectIfThen",
      "arg": {
        "format": "{}",
        "snippet": "ExpectIfThen{${1:condition}}"
      }
    },
    {
      "name": "InRange",
      "arg": {
        "format": "{}{}",
        "snippet": "InRange{${1:dimen1%l}}{${2:dimen2%l}}"
      }
    },
    {
      "name": "NearTo",
      "arg": {
        "format": "{}",
        "snippet": "NearTo{${1:dimen%l}}"
      }
    },
    {
      "name": "NearTo",
      "arg": {
        "format": "[]{}",
        "snippet": "NearTo[${2:opt dimen%l}]{${1:dimen%l}}"
      }
    },
    {
      "name": "SaveValueFile",
      "arg": {
        "format": "{}",
        "snippet": "SaveValueFile{${1:file name%file}}"
      }
    },
    {
      "name": "CloseValueFile"
    },
    {
      "name": "SaveValue",
      "arg": {
        "format": "{}{}",
        "snippet": "SaveValue{${1:cmd}}{${2:arg}}"
      }
    },
    {
      "name": "SaveValue",
      "arg": {
        "format": "{}*{}",
        "snippet": "SaveValue{${1:cmd}}*{${2:arg}}"
      }
    },
    {
      "name": "InternalSetValue"
    },
    {
      "name": "SavedValue"
    },
    {
      "name": "CalledName"
    }
  ],
  "envs": [
    {
      "name": "qstest",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:test name}}{${2:keywordlist}}"
      }
    },
    {
      "name": "ExpectCallSequence",
      "arg": {
        "format": "{}",
        "snippet": "{${1:call sequence}}"
      }
    }
  ],
  "keys": {},
  "args": []
}