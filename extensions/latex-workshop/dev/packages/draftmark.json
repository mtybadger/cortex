{
  "deps": [
    {
      "name": "atbegshi"
    },
    {
      "name": "etextools"
    },
    {
      "name": "fix-cm"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ltxnew"
    },
    {
      "name": "picture"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "xifthen"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "draftmarksetup",
      "arg": {
        "format": "{}",
        "snippet": "draftmarksetup{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/draftmark#c,\\draftmarksetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/draftmark#c": [
      "draft",
      "final",
      "allpages",
      "evenpages",
      "oddpages",
      "pages=${1:num1-num2}",
      "page=${1:num}"
    ],
    "\\usepackage/draftmark#c,\\draftmarksetup": [
      "mark=${1:text}",
      "angle=${1:degrees}",
      "scale=${1:factor}",
      "grayness=${1:factor}",
      "color=",
      "fontfamily=${1:family}",
      "fontsize=",
      "xcoord=${1:number}",
      "ycoord=${1:number}",
      "coordunit=${1:unit}",
      "showcenter",
      "acceptrange"
    ]
  },
  "args": [
    "\\usepackage/draftmark#c",
    "\\usepackage/draftmark#c,\\draftmarksetup"
  ]
}