{
  "deps": [
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "sectionbreak"
    },
    {
      "name": "sectionbreak",
      "arg": {
        "format": "[]",
        "snippet": "sectionbreak[${1:mark}]"
      }
    },
    {
      "name": "sectionbreakmark",
      "arg": {
        "format": "{}",
        "snippet": "sectionbreakmark{${1:mark}}"
      }
    },
    {
      "name": "asterism"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/sectionbreak#c": [
      "preskip=",
      "postskip=",
      "skip=",
      "style=${1:font commands}",
      "mark=${1:chars}",
      "asterism",
      "disable"
    ]
  },
  "args": [
    "\\usepackage/sectionbreak#c"
  ]
}