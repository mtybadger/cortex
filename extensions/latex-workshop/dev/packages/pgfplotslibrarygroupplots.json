{
  "deps": [
    {
      "name": "tikzlibrarycalc"
    }
  ],
  "macros": [
    {
      "name": "nextgroupplot"
    },
    {
      "name": "nextgroupplot",
      "arg": {
        "format": "[]",
        "snippet": "nextgroupplot[${1:options%keyvals}]"
      }
    }
  ],
  "envs": [
    {
      "name": "groupplot"
    },
    {
      "name": "groupplot",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\begin{axis}#c,\\begin{loglogaxis}#c,\\begin{semilogxaxis}#c,\\begin{semilogyaxis}#c,\\addplot#c,\\addplot3#c,\\addplot+#c,\\pgfplotsset#c,\\begin{groupplot}#c,\\nextgroupplot#c": [
      "group style={${1:options}}",
      "group/group size=${1:columns} by ${2:rows}",
      "group/columns=${1:integer}",
      "group/rows=${1:integer}",
      "group/horizontal sep=",
      "group/vertical sep=",
      "group/every plot/.style={${1:TikZ keys}}",
      "group/xlabels at=${1|all,edge top,edge bottom|}",
      "group/ylabels at=${1|all,edge left,edge right|}",
      "group/xticklabels at=${1|all,edge top,edge bottom|}",
      "group/yticklabels at=${1|all,edge left,edge right|}",
      "group/x descriptions at=${1|all,edge top,edge bottom|}",
      "group/y descriptions at=${1|all,edge left,edge right|}",
      "group/group name=${1:name}",
      "group/empty plot/.style={${1:TikZ keys}}"
    ]
  },
  "args": []
}