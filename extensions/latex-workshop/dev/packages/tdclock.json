{
  "deps": [
    {
      "name": "hyperref"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "initclock"
    },
    {
      "name": "tdclock"
    },
    {
      "name": "tdtime"
    },
    {
      "name": "tddate"
    },
    {
      "name": "tdday"
    },
    {
      "name": "tdmonth"
    },
    {
      "name": "tdyear"
    },
    {
      "name": "tdhours"
    },
    {
      "name": "tdminutes"
    },
    {
      "name": "tdseconds"
    },
    {
      "name": "crono"
    },
    {
      "name": "cronohours"
    },
    {
      "name": "cronominutes"
    },
    {
      "name": "cronoseconds"
    },
    {
      "name": "resetcrono",
      "arg": {
        "format": "{}",
        "snippet": "resetcrono{${1:button text}}"
      }
    },
    {
      "name": "toggleclock",
      "arg": {
        "format": "{}",
        "snippet": "toggleclock{${1:button text}}"
      }
    },
    {
      "name": "factorclockfont",
      "arg": {
        "format": "{}",
        "snippet": "factorclockfont{${1:factor}}"
      }
    },
    {
      "name": "hhmmss"
    },
    {
      "name": "hhmm"
    },
    {
      "name": "timeseparator",
      "unusual": true
    },
    {
      "name": "ddmmyyyy"
    },
    {
      "name": "mmddyyyy"
    },
    {
      "name": "dateseparator",
      "unusual": true
    },
    {
      "name": "pdfslash",
      "unusual": true
    },
    {
      "name": "pdfcolon",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/tdclock#c": [
      "font=${1|Helv,HelvB,HelvI,HelvBI,Times,TimesB,TimesI,TimesBI,Cour,CourB,CourI,CourBI|}",
      "timeinterval=${1:integer}",
      "timeduration=${1:minutes}",
      "timewarningfirst=${1:integer 0-100}",
      "timewarningsecond=${1:integer 0-100}",
      "timedeath=${1:0|1}",
      "colorwarningfirst=",
      "colorwarningsecond=",
      "fillcolorwarningfirst=",
      "fillcolorwarningsecond=",
      "resetatpages=${3|none,all,${1:integer},{${2:list of integers}}|}",
      "pageresetcontrol=${1|pdfpagelabels,pdfpagenumbers|}"
    ]
  },
  "args": [
    "\\usepackage/tdclock#c"
  ]
}