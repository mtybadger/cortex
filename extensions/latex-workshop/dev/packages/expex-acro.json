{
  "deps": [
    {
      "name": "expex"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xspace"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "acro"
    },
    {
      "name": "enumitem"
    }
  ],
  "macros": [
    {
      "name": "exref",
      "arg": {
        "format": "{}",
        "snippet": "exref{${1:tag}}"
      }
    },
    {
      "name": "exref",
      "arg": {
        "format": "[]{}",
        "snippet": "exref[${2:tag1}]{${1:tag2}}"
      }
    },
    {
      "name": "exref",
      "unusual": true,
      "arg": {
        "format": "[][]{}",
        "snippet": "exref[${2:range}][${3:tag1}]{${1:tag2}}"
      }
    },
    {
      "name": "exrefnil",
      "arg": {
        "format": "{}",
        "snippet": "exrefnil{${1:tag}}"
      }
    },
    {
      "name": "exrefnil",
      "arg": {
        "format": "[]{}",
        "snippet": "exrefnil[${2:tag1}]{${1:tag2}}"
      }
    },
    {
      "name": "mexref",
      "arg": {
        "format": "{}",
        "snippet": "mexref{${1:tag1,tag2,...}}"
      }
    },
    {
      "name": "mexref",
      "arg": {
        "format": "[]{}",
        "snippet": "mexref[${2:separator}]{${1:tag1,tag2,...}}"
      }
    },
    {
      "name": "gl",
      "arg": {
        "format": "{}",
        "snippet": "gl{${1:id}}"
      }
    },
    {
      "name": "newGlossingAbbrev",
      "arg": {
        "format": "{}{}",
        "snippet": "newGlossingAbbrev{${1:id}}{${2:long form}}"
      }
    },
    {
      "name": "glossingAbbrevsList"
    },
    {
      "name": "obj",
      "arg": {
        "format": "{}",
        "snippet": "obj{${1:text}}"
      }
    },
    {
      "name": "qu",
      "arg": {
        "format": "{}",
        "snippet": "qu{${1:text}}"
      }
    },
    {
      "name": "rc",
      "arg": {
        "format": "{}",
        "snippet": "rc{${1:text}}"
      }
    },
    {
      "name": "ort",
      "arg": {
        "format": "{}",
        "snippet": "ort{${1:text}}"
      }
    },
    {
      "name": "pnt",
      "arg": {
        "format": "{}",
        "snippet": "pnt{${1:text}}"
      }
    },
    {
      "name": "pnm",
      "arg": {
        "format": "{}",
        "snippet": "pnm{${1:text}}"
      }
    },
    {
      "name": "dbqu",
      "arg": {
        "format": "{}",
        "snippet": "dbqu{${1:text}}"
      }
    },
    {
      "name": "ungr",
      "arg": {
        "format": "{}",
        "snippet": "ungr{${1:text}}"
      }
    },
    {
      "name": "bad",
      "arg": {
        "format": "{}",
        "snippet": "bad{${1:text}}"
      }
    },
    {
      "name": "lxm",
      "arg": {
        "format": "{}",
        "snippet": "lxm{${1:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/expex-acro#c": [
      "abbrevs=${1|leipzig,all|}",
      "refmode=${1|expex,latex|}"
    ]
  },
  "args": [
    "\\usepackage/expex-acro#c"
  ]
}