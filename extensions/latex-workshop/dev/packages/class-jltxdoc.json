{
  "deps": [
    {
      "name": "platex"
    },
    {
      "name": "class-ltxdoc"
    }
  ],
  "macros": [
    {
      "name": "Lcount",
      "arg": {
        "format": "{}",
        "snippet": "Lcount{${1:text%plain}}"
      }
    },
    {
      "name": "Lopt",
      "arg": {
        "format": "{}",
        "snippet": "Lopt{${1:option}}"
      }
    },
    {
      "name": "NFSS"
    },
    {
      "name": "dst"
    },
    {
      "name": "file",
      "arg": {
        "format": "{}",
        "snippet": "file{${1:file name}}"
      }
    },
    {
      "name": "mlineplus",
      "arg": {
        "format": "{}",
        "snippet": "mlineplus{${1:number}}"
      }
    },
    {
      "name": "pstyle",
      "arg": {
        "format": "{}",
        "snippet": "pstyle{${1:text%plain}}"
      }
    }
  ],
  "envs": [
    {
      "name": "tsample",
      "arg": {
        "format": "{}",
        "snippet": "{${1:height}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/jltxdoc#c": [
      "nocfg",
      "doc2",
      "a4paper",
      "b5paper",
      "letterpaper",
      "legalpaper",
      "executivepaper",
      "landscape",
      "10pt",
      "11pt",
      "12pt",
      "oneside",
      "twoside",
      "draft",
      "final",
      "titlepage",
      "notitlepage",
      "onecolumn",
      "twocolumn",
      "leqno",
      "fleqn",
      "openbib"
    ]
  },
  "args": [
    "\\documentclass/jltxdoc#c"
  ]
}