{
  "deps": [
    {
      "name": "pgf"
    },
    {
      "name": "expkv-opt"
    }
  ],
  "macros": [
    {
      "name": "cistercian",
      "arg": {
        "format": "{}",
        "snippet": "cistercian{${1:counter}}"
      }
    },
    {
      "name": "cisterciannum",
      "arg": {
        "format": "{}",
        "snippet": "cisterciannum{${1:integer}}"
      }
    },
    {
      "name": "cisterciannumE",
      "arg": {
        "format": "{}",
        "snippet": "cisterciannumE{${1:integer expression}}"
      }
    },
    {
      "name": "cisterciansetup",
      "arg": {
        "format": "{}",
        "snippet": "cisterciansetup{${1:options%keyvals}}",
        "keys": [
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cistercianredraw",
      "unusual": true
    },
    {
      "name": "cistercianredrawlazy",
      "unusual": true
    },
    {
      "name": "cistercianstyle",
      "arg": {
        "format": "{}{}",
        "snippet": "cistercianstyle{${1:keyname%specialDef}}{${2:options%keyvals}}",
        "keys": [
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "cistercianstyle*",
      "arg": {
        "format": "{}{}",
        "snippet": "cistercianstyle*{${1:keyname%specialDef}}{${2:options%keyvals}}",
        "keys": [
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
          "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c": [
      "width=",
      "wd=",
      "height=",
      "ht=",
      "bound-x=${1:factor}",
      "bx=${1:factor}",
      "bound-y=${1:factor}",
      "by=${1:factor}",
      "baseline=${1:factor}",
      "bs=${1:factor}",
      "stroke-v=",
      "sv=",
      "stroke-h=",
      "sh=",
      "stroke-du=",
      "sdu=",
      "stroke-dd=",
      "sdd=",
      "strokes=",
      "s=",
      "fraction=${1:fraction}",
      "fr=${1:fraction}",
      "orientation=${1|horizontal,h,vertical,v|}",
      "o=${1|horizontal,h,vertical,v|}",
      "horizontal",
      "vertical",
      "alternate-5=${1:choice}",
      "5=${1:choice}",
      "alternate-6=${1:choice}",
      "6=${1:choice}",
      "alternate-9=${1:choice}",
      "9=${1:choice}",
      "font={${1:font setup}}",
      "f={${1:font setup}}",
      "%cisterciankey"
    ],
    "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*": [
      "redraw={${1:feature list}}",
      "redraw +={${1:feature list}}",
      "redraw",
      "redraw!",
      "r={${1:feature list}}",
      "r +={${1:feature list}}",
      "r",
      "r!"
    ],
    "\\usepackage/xistercian#c": [
      "debug"
    ]
  },
  "args": [
    "\\cisterciansetup,\\cistercianstyle,\\cistercianstyle*,\\usepackage/xistercian#c",
    "\\usepackage/xistercian#c"
  ]
}