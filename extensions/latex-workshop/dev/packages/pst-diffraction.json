{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-3dplot"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psdiffractionRectangle"
    },
    {
      "name": "psdiffractionRectangle",
      "arg": {
        "format": "[]",
        "snippet": "psdiffractionRectangle[${1:options%keyvals}]",
        "keys": [
          "\\psdiffractionRectangle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "psdiffractionCircular"
    },
    {
      "name": "psdiffractionCircular",
      "arg": {
        "format": "[]",
        "snippet": "psdiffractionCircular[${1:options%keyvals}]",
        "keys": [
          "\\psdiffractionCircular"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "psdiffractionTriangle"
    },
    {
      "name": "psdiffractionTriangle",
      "arg": {
        "format": "[]",
        "snippet": "psdiffractionTriangle[${1:options%keyvals}]",
        "keys": [
          "\\psdiffractionTriangle"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psdiffractionRectangle": [
      "a=${1:area}",
      "Alpha=${1:number}",
      "colorMode=${1|0,1,2,3|}",
      "contrast=${1:number}",
      "f=${1:focal length}",
      "IIID",
      "k=${1:height}",
      "lambda=${1:wavelength in nm}",
      "pixel=${1:factor}",
      "s=${1:distance}",
      "twoSlit"
    ],
    "\\psdiffractionCircular": [
      "colorMode=${1|0,1,2,3|}",
      "contrast=${1:number}",
      "d=${1:distance}",
      "f=${1:focal length}",
      "IIID",
      "lambda=${1:wavelength in nm}",
      "pixel=${1:factor}",
      "r=${1:radius}",
      "twoHole"
    ],
    "\\psdiffractionTriangle": [
      "colorMode=${1|0,1,2,3|}",
      "contrast=${1:number}",
      "f=${1:focal length}",
      "h=${1:height}",
      "scale=${1:factor}",
      "IIID",
      "lambda=${1:wavelength in nm}"
    ]
  },
  "args": []
}