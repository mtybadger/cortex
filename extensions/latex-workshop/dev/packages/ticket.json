{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "calc"
    }
  ],
  "macros": [
    {
      "name": "ticketdefault"
    },
    {
      "name": "ticket",
      "arg": {
        "format": "{}",
        "snippet": "ticket{${1:content%text}}"
      }
    },
    {
      "name": "ticketreset"
    },
    {
      "name": "backside"
    },
    {
      "name": "ticketskip",
      "arg": {
        "format": "{}",
        "snippet": "ticketskip{${1:number}}"
      }
    },
    {
      "name": "ticketNumbers",
      "arg": {
        "format": "{}{}",
        "snippet": "ticketNumbers{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "ticketWidth"
    },
    {
      "name": "ticketHeight"
    },
    {
      "name": "ticketSize",
      "arg": {
        "format": "{}{}",
        "snippet": "ticketSize{${1:width}}{${2:height}}"
      }
    },
    {
      "name": "ticketDistance",
      "arg": {
        "format": "{}{}",
        "snippet": "ticketDistance{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "ticketToUse",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ticket#c": [
      "${1:.tdf file}",
      "boxed",
      "crossmark",
      "circlemark",
      "emptycrossmark",
      "cutmark",
      "rowmode"
    ]
  },
  "args": [
    "\\usepackage/ticket#c"
  ]
}