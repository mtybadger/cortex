{
  "deps": [
    {
      "name": "class-scrartcl"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "leadsheets"
    },
    {
      "name": "scrlayer-scrpage"
    },
    {
      "name": "translations"
    },
    {
      "name": "zref-totpages"
    }
  ],
  "macros": [
    {
      "name": "instruction",
      "arg": {
        "format": "{}",
        "snippet": "instruction{${1:text}}"
      }
    },
    {
      "name": "choir",
      "arg": {
        "format": "{}",
        "snippet": "choir{${1:text}}"
      }
    },
    {
      "name": "lsenparen",
      "arg": {
        "format": "{}",
        "snippet": "lsenparen{${1:text}}"
      }
    },
    {
      "name": "mkinstruction",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "mkinstruction{${1:text}}"
      }
    },
    {
      "name": "mkchoir",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "mkchoir{${1:text}}"
      }
    },
    {
      "name": "mklsenparens",
      "unusual": true
    },
    {
      "name": "mklsenparen",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "mklsenparen{${1:open paren}}{${2:closing paren}}{${3:text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "prechorus"
    },
    {
      "name": "prechorus",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{prechorus}"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\begin{prechorus}": [
      "format={${1:code}}",
      "label-format={${1:code}}",
      "class={${1:class name}}",
      "after-label={${1:code}}",
      "name=${1:name}",
      "template=${1:template}",
      "numbered",
      "named",
      "recall-chords={${1:env-class}}",
      "verses-format={${1:code}}",
      "verses-label-format={${1:code}}",
      "verses-after-label={${1:code}}"
    ]
  },
  "args": []
}