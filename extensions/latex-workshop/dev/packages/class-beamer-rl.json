{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "ifluatex"
    },
    {
      "name": "class-beamer"
    },
    {
      "name": "babel"
    },
    {
      "name": "fontspec"
    }
  ],
  "macros": [
    {
      "name": "localfootnote",
      "if": "layout=footnotes",
      "arg": {
        "format": "{}",
        "snippet": "localfootnote{${1:text}}"
      }
    },
    {
      "name": "mainfootnote",
      "if": "layout=footnotes",
      "arg": {
        "format": "{}",
        "snippet": "mainfootnote{${1:text}}"
      }
    },
    {
      "name": "localfootnotetext",
      "if": "layout=footnotes",
      "unusual": true
    },
    {
      "name": "mainfootnotetext",
      "if": "layout=footnotes",
      "unusual": true
    },
    {
      "name": "blacktriangleright",
      "detail": "▶ (\"wasysym\" command)",
      "doc": "\\blacktriangleright (fourier -mathabx), (large) right triangle, filled"
    },
    {
      "name": "blacktriangleleft",
      "detail": "◀ (\"wasysym\" command)",
      "doc": "\\blacktriangleleft (fourier -mathabx), (large) left triangle, filled"
    },
    {
      "name": "redefbeamertemplate",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "redefbeamertemplate{${1:element}}{${2:option}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/beamer-rl#c": [
      "arabic",
      "arabic={${1:\\babelprovide options}}",
      "arabic-dz",
      "arabic-dz={${1:\\babelprovide options}}",
      "arabic-tn",
      "arabic-tn={${1:\\babelprovide options}}",
      "arabic-ma",
      "arabic-ma={${1:\\babelprovide options}}",
      "arabic-eg",
      "arabic-eg={${1:\\babelprovide options}}",
      "arabic-sa",
      "arabic-sa={${1:\\babelprovide options}}",
      "arabic-iq",
      "arabic-iq={${1:\\babelprovide options}}",
      "arabic-sy",
      "arabic-sy={${1:\\babelprovide options}}",
      "arabic-lb",
      "arabic-lb={${1:\\babelprovide options}}",
      "arabic-ps",
