{
  "deps": [],
  "macros": [
    {
      "name": "lcmd",
      "arg": {
        "format": "{}{}{}",
        "snippet": "lcmd{${1:command}}{${2:prefix}}{${3:list}}"
      }
    },
    {
      "name": "lmake",
      "arg": {
        "format": "[]",
        "snippet": "lmake[${1:keyvals}]",
        "keys": [
          "\\lmake#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\lmake#c": [
      "p=${1:pattern}",
      "c=${1:separator}",
      "n=%{last index%>",
      "1=${1:first index}",
      "2=${1:second index}",
      "d=${1:dots}",
      "l={${1:sym1,sym2,...}}"
    ]
  },
  "args": []
}