{
  "deps": [
    {
      "name": "amsmath"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibraryshapes.geometric"
    },
    {
      "name": "etoolbox"
    }
  ],
  "macros": [
    {
      "name": "pascal",
      "arg": {
        "format": "{}",
        "snippet": "pascal{${1:order}}"
      }
    },
    {
      "name": "pascal",
      "arg": {
        "format": "[]{}",
        "snippet": "pascal[${2:options%keyvals}]{${1:order}}",
        "keys": [
          "\\pascal,\\pascalset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pascalset",
      "arg": {
        "format": "{}",
        "snippet": "pascalset{${1:options%keyvals}}",
        "keys": [
          "\\pascal,\\pascalset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "binomc",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "binomc{${1:arg1}}{${2:arg2}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pascal,\\pascalset": [
      "shape=${1|iso,rt|}",
      "radius=",
      "fontsize=${1:fontsize command}",
      "withnum",
      "binom",
      "fillr=${1:integer}",
      "fillc=${1:integer}"
    ]
  },
  "args": []
}