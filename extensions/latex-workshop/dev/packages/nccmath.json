{
  "deps": [
    {
      "name": "amsmath"
    }
  ],
  "macros": [
    {
      "name": "intertext",
      "arg": {
        "format": "[]{}",
        "snippet": "intertext[${2:distance%l}]{${1:word or phrase%text}}"
      }
    },
    {
      "name": "dmulticolumn",
      "arg": {
        "format": "{}{}{}",
        "snippet": "dmulticolumn{${1:count}}{${2:preamble}}{${3:formula}}"
      }
    },
    {
      "name": "useshortskip"
    },
    {
      "name": "nr"
    },
    {
      "name": "nr",
      "arg": {
        "format": "[]",
        "snippet": "nr[${1:dist%l}]"
      }
    },
    {
      "name": "nr*"
    },
    {
      "name": "nr*",
      "arg": {
        "format": "[]",
        "snippet": "nr*[${1:dist%l}]"
      }
    },
    {
      "name": "mrel",
      "arg": {
        "format": "{}",
        "snippet": "mrel{${1:formula}}"
      }
    },
    {
      "name": "underrel",
      "arg": {
        "format": "{}{}",
        "snippet": "underrel{${1:base}}{${2:bottom}}"
      }
    },
    {
      "name": "medmath",
      "arg": {
        "format": "{}",
        "snippet": "medmath{${1:formula}}"
      }
    },
    {
      "name": "medop",
      "arg": {
        "format": "{}",
        "snippet": "medop{${1:operator}}"
      }
    },
    {
      "name": "medint",
      "arg": {
        "format": "{}",
        "snippet": "medint{${1:operator}}"
      }
    },
    {
      "name": "medintcorr",
      "arg": {
        "format": "{}",
        "snippet": "medintcorr{${1:length}}"
      }
    },
    {
      "name": "mfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "mfrac{${1:num}}{${2:den}}"
      }
    },
    {
      "name": "mbinom",
      "arg": {
        "format": "{}{}",
        "snippet": "mbinom{${1:above}}{${2:below}}"
      }
    },
    {
      "name": "eq",
      "arg": {
        "format": "{}",
        "snippet": "eq{${1:formula}}"
      }
    },
    {
      "name": "eq*",
      "arg": {
        "format": "{}",
        "snippet": "eq*{${1:formula}}"
      }
    },
    {
      "name": "eqs",
      "arg": {
        "format": "{}",
        "snippet": "eqs{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqs",
      "arg": {
        "format": "[]{}",
        "snippet": "eqs[${2:dist%l}]{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqs*",
      "arg": {
        "format": "{}",
        "snippet": "eqs*{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqs*",
      "arg": {
        "format": "[]{}",
        "snippet": "eqs*[${2:dist%l}]{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqalign",
      "arg": {
        "format": "{}",
        "snippet": "eqalign{${1:formulas%formula}}"
      }
    },
    {
      "name": "eqalign*",
      "arg": {
        "format": "{}",
        "snippet": "eqalign*{${1:formulas%formula}}"
      }
    }
  ],
  "envs": [
    {
      "name": "fleqn"
    },
    {
      "name": "fleqn",
      "arg": {
        "format": "[]",
        "snippet": "[${1:margin}]"
      }
    },
    {
      "name": "ceqn"
    },
    {
      "name": "darray",
      "arg": {
        "format": "{}",
        "snippet": "{${1:cols}}"
      }
    },
    {
      "name": "darray",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:pos%keyvals}]{${1:cols}}",
        "keys": [
          "\\begin{darray}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "medsize"
    },
    {
      "name": "mmatrix"
    }
  ],
  "keys": {
    "\\usepackage/nccmath#c": [
      "mediummath",
      "intlimits",
      "nointlimits",
      "sumlimits",
      "nosumlimits",
      "namelimits",
      "nonamelimits",
      "leqno",
      "reqno",
      "centertags",
      "tbtags",
      "cmex10",
      "fleqn",
      "alignedleftspaceyes",
      "alignedleftspaceno",
      "alignedleftspaceyesifneg"
    ],
    "\\begin{darray}": [
      "l",
      "c",
      "r"
    ]
  },
  "args": [
    "\\usepackage/nccmath#c"
  ]
}