      "prephead=${1:text}",
      "hinthead=${1:text}",
      "continuationhead=${1:text}",
      "continuationfoot=${1:text}",
      "preparationtime=${1:text}",
      "bakingtime=${1:text}",
      "bakingtemperature=${1:text}",
      "portion=${1:text}",
      "portionvalue=${1:text}",
      "calory=${1:text}",
      "source=${1:text}"
