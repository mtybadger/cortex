    {
      "name": "issuenumber",
      "arg": {
        "format": "{}",
        "snippet": "issuenumber{${1:number(${2:s})}}"
      }
    },
    {
      "name": "manuscriptid",
      "arg": {
        "format": "{}",
        "snippet": "manuscriptid{${1:id}}"
      }
    },
    {
      "name": "onlinedate",
      "arg": {
        "format": "{}",
        "snippet": "onlinedate{${1:date}}"
      }
    },
    {
      "name": "papernumber",
      "arg": {
        "format": "{}",
        "snippet": "papernumber{${1:number}}"
      }
    },
    {
      "name": "paperUrl",
      "arg": {
        "format": "{}",
        "snippet": "paperUrl{${1:URL}}"
      }
    },
    {
      "name": "prevpaper",
      "arg": {
        "format": "{}",
        "snippet": "prevpaper{${1:previous paper%definition}}"
      }
    },
    {
      "name": "publicationmonth",
      "arg": {
        "format": "[]{}",
        "snippet": "publicationmonth[${2:abbrev month(${3:s})}]{${1:month(${4:s})}}"
      }
    },
    {
      "name": "publicationmonth",
      "arg": {
        "format": "{}",
        "snippet": "publicationmonth{${1:month(${2:s})}}"
      }
    },
    {
      "name": "publicationyear",
      "arg": {
        "format": "{}",
        "snippet": "publicationyear{${1:year}}"
      }
    },
    {
      "name": "rpdefault",
      "unusual": true
    },
    {
      "name": "startpage",
      "arg": {
        "format": "{}",
        "snippet": "startpage{${1:number}}"
      }
    },
    {
      "name": "TC",
      "arg": {
        "format": "[]{}",
        "snippet": "TC[${2:level}]{${1:text}}"
      }
    },
    {
      "name": "TC",
      "arg": {
        "format": "{}",
        "snippet": "TC{${1:text}}"
      }
    },
    {
      "name": "TCSelect",
      "arg": {
        "format": "[]{}",
        "snippet": "TCSelect[${2:color}]{${1:level1,level2,...}}"
      }
    },
    {
      "name": "TCSelect",
      "arg": {
        "format": "{}",
        "snippet": "TCSelect{${1:level1,level2,...}}"
      }
    },
    {
      "name": "titlenote",
      "arg": {
        "format": "{}",
        "snippet": "titlenote{${1:note%text}}"
      }
    },
    {
      "name": "volumenumber",
      "arg": {
        "format": "{}",
        "snippet": "volumenumber{${1:number(${2:s})}}"
      }
    }
  ],
  "envs": [
    {
      "name": "notes",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/resphilosophica#c": [
      "8pt",
      "9pt",
      "10pt",
      "11pt",
      "12pt",
      "manuscript",
      "screen",
      "preprint",
      "forthcoming",
      "bodyfont=${1|auto,sabon,cm|}",
      "rpfont=${1|auto,imprintshadow,cm|}",
      "natbib"
    ]
  },
  "args": [
    "\\documentclass/resphilosophica#c"
  ]
}