    {
      "name": "lastpage"
    },
    {
      "name": "collect"
    },
    {
      "name": "footmisc"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "mathdesign"
    },
    {
      "name": "lsabon"
    },
    {
      "name": "natbib"
    }
  ],
  "macros": [
    {
      "name": "AddtoEndMatter",
      "arg": {
        "format": "{}",
        "snippet": "AddtoEndMatter{${1:bibliography commands}}"
      }
    },
    {
      "name": "authornote",
      "arg": {
        "format": "{}",
        "snippet": "authornote{${1:note%text}}"
      }
    },
    {
      "name": "copyrightnote",
      "arg": {
        "format": "{}",
        "snippet": "copyrightnote{${1:copyright statement%text}}"
      }
    },
    {
      "name": "copyrightyear",
      "arg": {
        "format": "{}",
        "snippet": "copyrightyear{${1:year}}"
      }
    },
    {
      "name": "doinumber",
      "arg": {
        "format": "{}",
        "snippet": "doinumber{${1:DOI}}"
      }
    },
    {
      "name": "ECSelect",
      "arg": {
        "format": "{}",
        "snippet": "ECSelect{${1:level1,level2,...}}"
      }
    },
    {
      "name": "EditorialComment",
      "arg": {
        "format": "[]{}",
        "snippet": "EditorialComment[${2:level}]{${1:comments%text}}"
      }
    },
    {
      "name": "EditorialComment",
      "arg": {
        "format": "{}",
        "snippet": "EditorialComment{${1:comments%text}}"
      }
    },
    {
      "name": "endpage",
      "arg": {
        "format": "{}",
        "snippet": "endpage{${1:number}}"
      }
