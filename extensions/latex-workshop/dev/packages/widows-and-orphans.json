{
  "deps": [
    {
      "name": "underscore"
    },
    {
      "name": "l3keys2e"
    }
  ],
  "macros": [
    {
      "name": "WaOsetup",
      "arg": {
        "format": "{}",
        "snippet": "WaOsetup{${1:options%keyvals}}",
        "keys": [
          "\\WaOsetup,\\usepackage/widows-and-orphans#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "WaOparameters"
    },
    {
      "name": "WaOignorenext"
    }
  ],
  "envs": [],
  "keys": {
    "\\WaOsetup,\\usepackage/widows-and-orphans#c": [
      "check=${1|warning,error,info,none|}",
      "orphans=${1|prevent,avoid,default|}",
      "widows=${1|prevent,avoid,default|}",
      "prevent-all",
      "avoid-all",
      "default-all"
    ]
  },
  "args": [
    "\\WaOsetup,\\usepackage/widows-and-orphans#c"
  ]
}