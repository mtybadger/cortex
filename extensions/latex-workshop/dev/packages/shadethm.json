    {
      "name": "color"
    }
  ],
  "macros": [
    {
      "name": "newshadetheorem",
      "arg": {
        "format": "{}{}",
        "snippet": "newshadetheorem{${1:envname}}{${2:label text}}"
      }
    },
    {
      "name": "newshadetheorem",
      "arg": {
