      "unusual": true
    },
    {
      "name": "saveparindent",
      "unusual": true
    },
    {
      "name": "shadedtextwidth",
      "unusual": true
    },
    {
      "name": "shadeleftshift",
      "unusual": true
    },
    {
      "name": "shaderightshift",
      "unusual": true
    },
    {
      "name": "shadesavebox",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "shadebox",
      "unusual": true
    }
  ],
  "keys": {
    "\\usepackage/shadethm#c": [
