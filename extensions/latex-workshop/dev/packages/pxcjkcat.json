{
  "deps": [
    {
      "name": "uplatex"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "cjkcategorymode",
      "arg": {
        "format": "{}",
        "snippet": "cjkcategorymode{${1:mode%keyvals}}",
        "keys": [
          "\\cjkcategorymode#c,\\usepackage/pxcjkcat#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cjkcategory",
      "arg": {
        "format": "{}{}",
        "snippet": "cjkcategory{${1:block}}{${2:category}}"
      }
    },
    {
      "name": "showcjkcategory",
      "unusual": true
    },
    {
      "name": "getcjktokenmode"
    },
    {
      "name": "thecjktokenmode"
    },
    {
      "name": "setcjktokenmode",
      "arg": {
        "format": "{}",
        "snippet": "setcjktokenmode{${1:value}}"
      }
    },
    {
      "name": "withcjktokendisabled",
      "arg": {
        "format": "{}",
        "snippet": "withcjktokendisabled{${1:code}}"
      }
    },
    {
      "name": "withcjktokenenabled",
      "arg": {
        "format": "{}",
        "snippet": "withcjktokenenabled{${1:code}}"
      }
    },
    {
      "name": "withcjktokenforced",
      "arg": {
        "format": "{}",
        "snippet": "withcjktokenforced{${1:code}}"
      }
    },
    {
      "name": "withcjktokendisabledex",
      "arg": {
        "format": "{}",
        "snippet": "withcjktokendisabledex{${1:code}}"
      }
    },
    {
      "name": "withcjktokenenabledex",
      "arg": {
        "format": "{}",
        "snippet": "withcjktokenenabledex{${1:code}}"
      }
    },
    {
      "name": "withcjktokenforcedex",
      "arg": {
        "format": "{}",
        "snippet": "withcjktokenforcedex{${1:code}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pxcjkcat#c": [
      "ccv1",
      "ccv2",
      "ccv3",
      "ccv4",
      "ccv+",
      "real",
      "nomode"
    ],
    "\\cjkcategorymode#c,\\usepackage/pxcjkcat#c": [
      "forcecjk",
      "default",
      "prefercjk",
      "prefercjkvar",
      "japanese",
      "japanesevar",
      "prefernoncjk"
    ]
  },
  "args": [
    "\\usepackage/pxcjkcat#c",
    "\\cjkcategorymode#c,\\usepackage/pxcjkcat#c"
  ]
}