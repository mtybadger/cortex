  "envs": [
    {
      "name": "sseq",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:x-range}}{${2:y-range}}"
      }
    },
    {
      "name": "sseq",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:options%keyvals}]{${1:x-range}}{${2:y-range}}",
        "keys": [
          "\\begin{sseq}"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/luasseq#c": [
      "debug"
    ],
    "\\begin{sseq}": [
      "grid=${1|none,crossword,go,dots,chess|}",
      "gridstroke=",
      "gapsize=",
      "xgapsize=",
      "ygapsize=",
      "entrysize=",
      "labels=${2|none,numbers,{${1:x1;x2;...}}|}",
      "xlabels=${2|none,numbers,{${1:x1;x2;...}}|}",
      "ylabels=${2|none,numbers,{${1:x1;x2;...}}|}",
      "labelstep=${1:integer}",
      "xlabelstep=${1:integer}",
      "ylabelstep=${1:integer}",
      "leak=",
      "xleak=",
      "yleak=",
      "arrows=${1:arrow type}",
      "packing=${1|auto,horizontal,vertical,diagonal|}"
    ],
    "\\ssdrop,\\ssdropextension": [
      "circled",
      "boxed",
      "color=",
      "name=${1:name}"
    ],
    "\\ssdroplabel": [
      "color=",
      "pos=${1|U,LU,L,LD,D,RD,R,RU|}",
      "U",
      "LU",
      "L",
      "LD",
      "D",
      "RD",
      "R",
      "RU"
    ],
    "\\ssstroke,\\ssline,\\ssarrow,\\ssvoidline": [
      "color=",
      "curve=${1:value}",
      "dashed",
      "dashed=${1:dashing type}",
      "dotted",
      "dotted=${1:dashing type}",
      "arrowfrom",
      "arrowfrom=${1:dashing type}",
      "arrowto",
      "arrowto=${1:dashing type}",
      "void"
    ]
  },
  "args": [
    "\\usepackage/luasseq#c"
  ]
