{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "ulem"
    },
    {
      "name": "underscore"
    }
  ],
  "macros": [
    {
      "name": "gap",
      "arg": {
        "format": "{}",
        "snippet": "gap{${1:text}}"
      }
    },
    {
      "name": "gap",
      "arg": {
        "format": "[]{}",
        "snippet": "gap[${2:style%keyvals}]{${1:text}}",
        "keys": [
          "\\gap,\\gap*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "gap*",
      "arg": {
        "format": "{}",
        "snippet": "gap*{${1:text}}"
      }
    },
    {
      "name": "gap*",
      "arg": {
        "format": "[]{}",
        "snippet": "gap*[${2:style%keyvals}]{${1:text}}",
        "keys": [
          "\\gap,\\gap*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "TeacherModeOn",
      "unusual": true
    },
    {
      "name": "TeacherModeOff",
      "unusual": true
    },
    {
      "name": "dashundergapssetup",
      "arg": {
        "format": "{}",
        "snippet": "dashundergapssetup{${1:options%keyvals}}",
        "keys": [
          "\\dashundergapssetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "thegapnumber",
      "unusual": true
    },
    {
      "name": "thetotalgapnumber",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\gap,\\gap*": [
      "u",
      "d",
      "w",
      "b",
      "-",
      "."
    ],
    "\\dashundergapssetup": [
      "teacher-mode",
      "gap-mode",
      "teachermode",
      "gap-format=${1|underline,double-underline,dash,dot,wave,blank|}",
      "gap-format-adjust",
      "teacher-gap-format=${1|underline,double-underline,dash,dot,wave,blank|}",
      "gap-font=",
      "dash",
      "dot",
      "gap-numbers",
      "gap-number-format=",
      "numbers",
      "display-total-gaps",
      "displaynbgaps",
      "gap-widen",
      "gap-extend-minimum=",
      "gap-extend-percent=",
      "widen"
    ]
  },
  "args": []
}