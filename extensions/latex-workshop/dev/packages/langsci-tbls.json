{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "tcolorbox"
    },
    {
      "name": "tcolorboxlibrarybreakable"
    },
    {
      "name": "tcolorboxlibraryskins"
    },
    {
      "name": "mdframed",
      "if": "mdframed"
    }
  ],
  "macros": [
    {
      "name": "langscisymbol",
      "if": "mdframed",
      "unusual": true
    },
    {
      "name": "trennlinie",
      "if": "mdframed",
      "unusual": true
    },
    {
      "name": "tblsboxcolor",
      "if": "mdframed",
      "unusual": true
    },
    {
      "name": "tblslinecolour",
      "if": "mdframed",
      "unusual": true
    },
    {
      "name": "tblsfillcolour",
      "if": "mdframed",
      "unusual": true
    },
    {
      "name": "tblssy",
      "if": "mdframed",
      "arg": {
        "format": "{}{}{}",
        "snippet": "tblssy{${1:symbol}}{${2:title%text}}{${3:text}}"
      }
    },
    {
      "name": "tblssy",
      "if": "mdframed",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "tblssy[${4:color}]{${1:symbol}}{${2:title%text}}{${3:text}}"
      }
    },
    {
      "name": "tblsli",
      "if": "mdframed",
      "arg": {
        "format": "{}{}{}",
        "snippet": "tblsli{${1:linewidth%l}}{${2:title%text}}{${3:text}}"
      }
    },
    {
      "name": "tblsli",
      "if": "mdframed",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "tblsli[${4:color}]{${1:linewidth%l}}{${2:title%text}}{${3:text}}"
      }
    },
    {
      "name": "tblsfi",
      "if": "mdframed",
      "arg": {
        "format": "{}{}",
        "snippet": "tblsfi{${1:title%text}}{${2:text}}"
      }
    },
    {
      "name": "tblsfi",
      "if": "mdframed",
      "arg": {
        "format": "[]{}{}",
        "snippet": "tblsfi[${3:color}]{${1:title%text}}{${2:text}}"
      }
    },
    {
      "name": "tblsfr",
      "if": "mdframed",
      "arg": {
        "format": "{}{}{}",
        "snippet": "tblsfr{${1:symbol}}{${2:title%text}}{${3:text}}"
      }
    },
    {
      "name": "tblsfr",
      "if": "mdframed",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "tblsfr[${4:color}]{${1:symbol}}{${2:title%text}}{${3:text}}"
      }
    },
    {
      "name": "tblsfd",
      "if": "mdframed",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "tblsfd{${1:color}}{${2:linewidth%l}}{${3:title%text}}{${4:content%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "tblslineshorizontal",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "tblslineshorizontal",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:title%text}}[${2:width}]"
      }
    },
    {
      "name": "tblslineshorizontal",
      "arg": {
        "format": "{}[][]",
        "snippet": "{${1:title%text}}[${2:width}][${3:color}]"
      }
    },
    {
      "name": "tblsfilled",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "tblsfilled",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:title%text}}[${2:color}]"
      }
    },
    {
      "name": "tblsframed",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "tblsframed",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:title%text}}[${2:width}]"
      }
    },
    {
      "name": "tblsframed",
      "arg": {
        "format": "{}[][]",
        "snippet": "{${1:title%text}}[${2:width}][${3:color}]"
      }
    },
    {
      "name": "tblsfilledsymbol",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:title%text}}{${2:PDF file%file}}"
      }
    },
    {
      "name": "tblsfilledsymbol",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:title%text}}[${3:color}]{${2:PDF file%file}}"
      }
    },
    {
      "name": "tblsframedsymbol",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:title%text}}{${2:PDF file%file}}"
      }
    },
    {
      "name": "tblsframedsymbol",
      "arg": {
        "format": "{}[]{}",
        "snippet": "{${1:title%text}}[${3:color}]{${2:PDF file%file}}"
      }
    },
    {
      "name": "tblsframedsymbol",
      "arg": {
        "format": "{}[][]{}",
        "snippet": "{${1:title%text}}[${3:color}][${4:width}]{${2:PDF file%file}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/langsci-tbls#c": [
      "mdframed",
      "tcolorbox"
    ]
  },
  "args": [
    "\\usepackage/langsci-tbls#c"
  ]
}