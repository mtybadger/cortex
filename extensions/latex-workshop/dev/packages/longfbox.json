{
  "deps": [
    {
      "name": "options"
    },
    {
      "name": "longbox"
    },
    {
      "name": "pict2e"
    },
    {
      "name": "ellipse"
    }
  ],
  "macros": [
    {
      "name": "lfbox",
      "arg": {
        "format": "{}",
        "snippet": "lfbox{${1:content%text}}"
      }
    },
    {
      "name": "lfbox",
      "arg": {
        "format": "[]{}",
        "snippet": "lfbox[${2:options%keyvals}]{${1:content%text}}",
        "keys": [
          "\\lfbox#c,\\begin{longfbox}#c,\\newfboxstyle#c,\\fboxset#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "newfboxstyle",
      "arg": {
        "format": "{}{}",
        "snippet": "newfboxstyle{${1:name%specialDef}}{${2:options%keyvals}}",
        "keys": [
          "\\lfbox#c,\\begin{longfbox}#c,\\newfboxstyle#c,\\fboxset#c"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "fboxset",
      "arg": {
        "format": "{}",
        "snippet": "fboxset{${1:options%keyvals}}",
        "keys": [
          "\\lfbox#c,\\begin{longfbox}#c,\\newfboxstyle#c,\\fboxset#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "optionlengthlimit",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "optionlengthlimit{${1:length}}{${2:min}}{${3:max}}"
      }
    },
    {
      "name": "optionradiuslimit",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "optionradiuslimit{${1:r1}}{${2:r2}}{${3:min}}{${4:max}}"
      }
    }
  ],
  "envs": [
    {
      "name": "longfbox"
    },
    {
      "name": "longfbox",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\lfbox#c,\\begin{longfbox}#c,\\newfboxstyle#c,\\fboxset#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\lfbox#c,\\begin{longfbox}#c,\\newfboxstyle#c,\\fboxset#c": [
      "border-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
      "border-top-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
      "border-right-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
      "border-bottom-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
      "border-left-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
      "border-break-style=",
      "border-break-top-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
      "border-break-bottom-style=${1|none,hidden,solid,dotted,dashed,double,inset,outset,groove,ridge|}",
