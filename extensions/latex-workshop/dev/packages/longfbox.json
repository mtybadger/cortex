      "raise=",
      "height-align=${1|top,middle,bottom|}",
      "text-align=${1|default,left,center,right,justify|}",
      "background-color=",
      "background-clip=border-box,padding-box,content-box",
      "background-padding-color=",
      "background-border-color=",
