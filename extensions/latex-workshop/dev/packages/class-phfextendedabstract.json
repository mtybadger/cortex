{
  "deps": [
    {
      "name": "class-revtex4-2"
    },
    {
      "name": "geometry"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "phfnote"
    },
    {
      "name": "phfthm"
    },
    {
      "name": "float"
    },
    {
      "name": "verbdef"
    },
    {
      "name": "csquotes"
    },
    {
      "name": "dsfont"
    },
    {
      "name": "bbm"
    },
    {
      "name": "mathtools"
    }
  ],
  "macros": [
    {
      "name": "section<decoration>",
      "arg": {
        "format": "{}",
        "snippet": "section<decoration>{${1:title}}"
      }
    },
    {
      "name": "section<decoration>",
      "arg": {
        "format": "[]{}",
        "snippet": "section<decoration>[${2:short title}]{${1:title}}"
      }
    },
    {
      "name": "paragraph<decoration>",
      "arg": {
        "format": "{}",
        "snippet": "paragraph<decoration>{${1:title}}"
      }
    },
    {
      "name": "paragraph<decoration>",
      "arg": {
        "format": "[]{}",
        "snippet": "paragraph<decoration>[${2:short title}]{${1:title}}"
      }
    },
    {
      "name": "phfeaSectionDecoration",
      "arg": {
        "format": "{}",
        "snippet": "phfeaSectionDecoration{${1:symbol}}"
      }
    },
    {
      "name": "phfeaParagraphDecoration",
      "arg": {
        "format": "{}",
        "snippet": "phfeaParagraphDecoration{${1:symbol}}"
      }
    },
    {
      "name": "phfeaSectionDecorationSymbol",
      "unusual": true
    },
    {
      "name": "phfeaParagraphDecorationSymbol",
      "unusual": true
    },
    {
      "name": "phfeaSectionBeforeSkip",
      "unusual": true
    },
    {
      "name": "phfeaSectionAfterHSkip",
      "unusual": true
    },
    {
      "name": "phfeaParagraphBeforeSkip",
      "unusual": true
    },
    {
      "name": "phfeaParagraphAfterHSkip",
      "unusual": true
    },
    {
      "name": "phfeaSectionStyle",
      "unusual": true
    },
    {
      "name": "phfeaParagraphStyle",
      "unusual": true
    },
    {
      "name": "phfeaSectionFormatHeading",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "phfeaSectionFormatHeading{${1:text}}"
      }
    },
    {
      "name": "phfeaParagraphFormatHeading",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "phfeaParagraphFormatHeading{${1:text}}"
      }
    },
    {
      "name": "phfeaVerticalSpacingCompressionFactor",
      "unusual": true
    },
    {
      "name": "phfeaDefineTheoremStyle",
      "unusual": true
    },
    {
      "name": "phfeaDisplayVerticalSpacingFactorWeight",
      "unusual": true
    },
    {
      "name": "phfeaParskipVerticalSpacingFactorWeight",
      "unusual": true
    },
    {
      "name": "phfeaListsVerticalSkip",
      "unusual": true
    },
    {
      "name": "phfeaListsItemSep",
      "unusual": true
    },
    {
      "name": "phfeaListsParSep",
      "unusual": true
    },
    {
      "name": "phfeaHeadingStyle",
      "unusual": true
    },
    {
      "name": "phfeaTitleStyle",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "enumerate*"
    },
    {
      "name": "enumerate*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]"
      }
    }
  ],
  "keys": {
    "\\documentclass/phfextendedabstract#c": [
      "papertype=${1:type}",
      "ptsize=${1|10pt,11pt,12pt|}",
      "sectiondecorations",
      "paragraphdecorations",
      "noheadingdecorations",
      "compressverticalspacing=${1:factor}",
      "loadtheorems",
      "sansstyle",
      "usehyperref",
      "pkgset=${1|none,minimal,rich,extended|}"
    ]
  },
  "args": [
    "\\documentclass/phfextendedabstract#c"
  ]
}