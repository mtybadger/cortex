{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikz-3dplot"
    },
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "brick",
      "arg": {
        "format": "{}{}",
        "snippet": "brick{${1:length%plain}}{${2:width%plain}}"
      }
    },
    {
      "name": "brick",
      "arg": {
        "format": "[]{}{}",
        "snippet": "brick[${3:options%keyvals}]{${1:length%plain}}{${2:width%plain}}",
        "keys": [
          "\\brick#c,\\usepackage/tikzbricks#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "wallbrick",
      "arg": {
        "format": "{}{}",
        "snippet": "wallbrick{${1:length%plain}}{${2:width%plain}}"
      }
    },
    {
      "name": "wallbrick",
      "arg": {
        "format": "[]{}{}",
        "snippet": "wallbrick[${3:options%keyvals}]{${1:length%plain}}{${2:width%plain}}",
        "keys": [
          "\\wallbrick#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "newrow"
    },
    {
      "name": "thebrickx",
      "unusual": true
    },
    {
      "name": "thebricky",
      "unusual": true
    },
    {
      "name": "thebrickz",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "wall"
    }
  ],
  "keys": {
    "\\brick#c,\\usepackage/tikzbricks#c": [
      "color=",
      "frontcolor=",
      "topcolor=",
      "sidecolor=",
      "studcolor=",
      "brickheight=${1:number}",
      "bricklength=${1:number}",
      "brickwidth=${1:number}",
      "studradius=${1:number}",
      "studheight=${1:number}",
      "studtext={${1:text}}"
    ],
    "\\wallbrick#c": [
      "color="
    ]
  },
  "args": [
    "\\brick#c,\\usepackage/tikzbricks#c"
  ]
}