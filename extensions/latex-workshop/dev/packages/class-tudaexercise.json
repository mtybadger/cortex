{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "environ"
    },
    {
      "name": "class-scrartcl"
    },
    {
      "name": "tudarules"
    },
    {
      "name": "scrlayer-scrpage"
    },
    {
      "name": "tudafonts"
    },
    {
      "name": "geometry"
    },
    {
      "name": "tudacolors"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "pgf",
      "if": "department=mecheng"
    }
  ],
  "macros": [
    {
      "name": "MechEngArrow",
      "if": "department=mecheng",
      "arg": {
        "format": "{}",
        "snippet": "MechEngArrow{${1:Länge}}"
      }
    },
    {
      "name": "MechEngArrow*",
      "if": "department=mecheng",
      "arg": {
        "format": "{}",
        "snippet": "MechEngArrow*{${1:Länge}}"
      }
    },
    {
      "name": "title",
      "arg": {
        "format": "[]{}",
        "snippet": "title[${2:Kurztitel für die Kopfzeile%text}]{${1:text}}"
      }
    },
    {
      "name": "term",
      "arg": {
        "format": "{}",
        "snippet": "term{${1:Semester}}"
      }
    },
    {
      "name": "sheetnumber",
      "arg": {
        "format": "{}",
        "snippet": "sheetnumber{${1:Nummer des Übungsblatts}}"
      }
    },
    {
      "name": "thetask"
    },
    {
      "name": "thesubtask"
    },
    {
      "name": "ConfigureHeadline",
      "arg": {
        "format": "{}",
        "snippet": "ConfigureHeadline{${1:keyvals}}",
        "keys": [
          "\\ConfigureHeadline#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ShortTitle"
    },
    {
      "name": "StudentID"
    },
    {
      "name": "StudentName"
    },
    {
      "name": "IfSolutionT",
      "arg": {
        "format": "{}",
        "snippet": "IfSolutionT{${1:true code}}"
      }
    },
    {
      "name": "IfSolutionF",
      "arg": {
        "format": "{}",
        "snippet": "IfSolutionF{${1:false code}}"
      }
    },
    {
      "name": "IfSolutionTF",
      "arg": {
        "format": "{}{}",
        "snippet": "IfSolutionTF{${1:true code}}{${2:false code}}"
      }
    },
    {
      "name": "creditformat",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "creditformat{${1:text}}"
      }
    },
    {
      "name": "creditformatsum",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "creditformatsum{${1:text}}"
      }
    },
    {
      "name": "PointName",
      "unusual": true
    },
    {
      "name": "PointsName",
      "unusual": true
    },
    {
      "name": "pointformat",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "pointformat{${1:text}}"
      }
    },
    {
      "name": "getPoints",
      "arg": {
        "format": "{}",
        "snippet": "getPoints{${1:Nummer}}"
      }
    },
    {
      "name": "getPointsTotal"
    },
    {
      "name": "refPoints",
      "arg": {
        "format": "{}",
        "snippet": "refPoints{${1:label}}"
      }
    },
    {
      "name": "mapPoints",
      "arg": {
        "format": "{}",
        "snippet": "mapPoints{${1:code%text}}"
      }
    },
    {
      "name": "mapPoints*",
      "arg": {
        "format": "{}",
        "snippet": "mapPoints*{${1:code%text}}"
      }
    },
    {
      "name": "authorandname",
      "unusual": true
    },
    {
      "name": "institution",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "institution{${1:Einrichtung}}"
      }
    },
    {
      "name": "sheetsep",
      "unusual": true
    },
    {
      "name": "solutionsep",
      "unusual": true
    },
    {
      "name": "StudentIDname",
      "unusual": true
    },
    {
      "name": "StudentIDsep",
      "unusual": true
    },
    {
      "name": "StudentNamesep",
      "unusual": true
    },
    {
      "name": "subtaskformat",
      "unusual": true
    },
    {
      "name": "taskformat",
      "unusual": true
    },
    {
      "name": "tasksep",
      "unusual": true
    },
    {
      "name": "titleimage",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "titleimage{${1:graphics code}}"
      }
    }
  ],
  "envs": [
    {
      "name": "task",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Überschrift%text}}"
      }
    },
    {
      "name": "task",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:Optionen%keyvals}]{${1:Überschrift%text}}",
        "keys": [
          "\\begin{task}#c,\\begin{task*}#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "task*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Aufgabentitel%text}}"
      }
    },
    {
      "name": "task*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:Optionen%keyvals}]{${1:Aufgabentitel%text}}",
        "keys": [
          "\\begin{task}#c,\\begin{task*}#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "subtask"
    },
    {
      "name": "subtask",
      "arg": {
        "format": "[]",
        "snippet": "[${1:Optionen%keyvals}]",
        "keys": [
          "\\begin{subtask}#c,\\begin{subtask*}#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "subtask*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:Titel der Teilaufgabe%text}}"
      }
    },
    {
      "name": "subtask*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:Optionen%keyvals}]{${1:Titel der Teilaufgabe%text}}",
        "keys": [
          "\\begin{subtask}#c,\\begin{subtask*}#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "solution"
    },
    {
      "name": "solution*"
    }
  ],
  "keys": {
    "\\documentclass/tudaexercise#c": [
      "color=",
      "accentcolor=",
      "textaccentcolor=",
      "identbarcolor=",
      "custommargins=${1|true,false,geometry|}",
      "fontsize=",
      "BCOR=${1:Länge}",
      "paper=${1|a0,a1,a2,a3,a4,a5,a6|}",
      "logofile=${1:Dateipfad}",
      "credit",
      "headontitle",
      "colorback=${1|true,false|}",
      "title=${1|default,small,large|}",
      "department=mecheng",
      "departmentlogofile=${1:Dateipfad}",
      "match-task-subtask-syntax",
      "solution",
      "points=${1|true,false,auto|}",
      "subtask=${1|ruled,runin,plain|}"
    ],
    "\\begin{task}#c,\\begin{task*}#c": [
      "points=${1:auto|<Nummer>}",
      "credit=${1:Nummer}",
      "solution"
    ],
    "\\begin{subtask}#c,\\begin{subtask*}#c": [
      "points=${1:Nummer}",
      "credit=${1:Nummer}",
      "title=${1:text}"
    ],
    "\\ConfigureHeadline#c": [
      "headline={${1:text}}",
      "even={${1:text}}",
      "odd={${1:text}}",
      "oneside={${1:text}}"
    ]
  },
  "args": [
    "\\documentclass/tudaexercise#c"
  ]
}