    {
      "name": "htfframenrboxwidth",
      "unusual": true
    },
    {
      "name": "htfprogress",
      "unusual": true
    },
    {
      "name": "htfprintmessage",
      "unusual": true
    },
    {
      "name": "htfupdateprogress",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/hackthefootline#c": [
      "onecol",
      "twocols",
      "threecols",
      "countappendixframes",
      "nofun"
    ],
    "\\htfconfig": [
      "cols=${1|one,two,three,none|}",
      "title=${1|short,long,none|}",
      "authinst=${1|onlyauthor,onlyinst,instpths,authpths,both,none|}",
      "date=${1|short,long,none|}",
      "framenrs=${1|counter,fraction,percent,none|}",
      "atsep=${1|colon,comma,space|}"
    ]
  },
  "args": [
    "\\usepackage/hackthefootline#c"
  ]
}