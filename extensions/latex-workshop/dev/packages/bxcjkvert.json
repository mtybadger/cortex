{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "keyval"
    },
    {
      "name": "CJKvert"
    }
  ],
  "macros": [
    {
      "name": "bxcjkvertsetup",
      "arg": {
        "format": "{}",
        "snippet": "bxcjkvertsetup{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/bxcjkvert#c,\\bxcjkvertsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/bxcjkvert#c,\\bxcjkvertsetup": [
      "resetfont",
      "usebaselinestretch=${1|true,false,retain|}",
      "main=${1|true,false,retain|}"
    ]
  },
  "args": [
    "\\usepackage/bxcjkvert#c,\\bxcjkvertsetup"
  ]
}