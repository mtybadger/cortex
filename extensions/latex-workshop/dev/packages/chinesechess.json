{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "l3draw"
    }
  ],
  "macros": [
    {
      "name": "cchessboard"
    },
    {
      "name": "cchessboard",
      "arg": {
        "format": "[]",
        "snippet": "cchessboard[${1:外观选项%keyvals}]",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cchessboard*"
    },
    {
      "name": "cchessboard*",
      "arg": {
        "format": "[]",
        "snippet": "cchessboard*[${1:外观选项%keyvals}]",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cchessman",
      "arg": {
        "format": "{}",
        "snippet": "cchessman{${1:棋子列表}}"
      }
    },
    {
      "name": "cchessman",
      "arg": {
        "format": "[]{}",
        "snippet": "cchessman[${2:外观选项%keyvals}]{${1:棋子列表}}",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "init",
      "arg": {
        "format": "{}",
        "snippet": "init{${1:棋子列表}}"
      }
    },
    {
      "name": "set",
      "arg": {
        "format": "{}{}",
        "snippet": "set{${1:棋子位置}}{${2:棋子位置列表}}"
      }
    },
    {
      "name": "del",
      "arg": {
        "format": "{}",
        "snippet": "del{${1:棋盘坐标}}"
      }
    },
    {
      "name": "mov",
      "arg": {
        "format": "{}{}{}",
        "snippet": "mov{${1:棋子编码}}{${2:原棋盘坐标}}{${3:新棋盘坐标}}"
      }
    },
    {
      "name": "printman",
      "arg": {
        "format": "{}",
        "snippet": "printman{${1:棋子编码%ref}}"
      }
    },
    {
      "name": "printman",
      "arg": {
        "format": "[]{}",
        "snippet": "printman[${2:外观选项%keyvals}]{${1:棋子编码%ref}}",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "getpiece",
      "arg": {
        "format": "{}",
        "snippet": "getpiece{${1:棋子编码}}"
      }
    },
    {
      "name": "getpiece",
      "arg": {
        "format": "[]{}",
        "snippet": "getpiece[${2:棋子字号}]{${1:棋子编码}}"
      }
    },
    {
      "name": "piecechar",
      "arg": {
        "format": "{}{}",
        "snippet": "piecechar{${1:棋子编码}}{${2:棋子字符}}"
      }
    },
    {
      "name": "resetpiece"
    },
    {
      "name": "cchessset",
      "arg": {
        "format": "{}",
        "snippet": "cchessset{${1:外观选项%keyvals}}",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "setcchessman"
    },
    {
      "name": "setcchessman",
      "arg": {
        "format": "[]",
        "snippet": "[${1:外观选项%keyvals}]",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "setcchessman*"
    },
    {
      "name": "setcchessman*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:外观选项%keyvals}]",
        "keys": [
          "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/chinesechess#c": [
      "draft"
    ],
    "\\cchessboard,\\cchessboard*,\\cchessman,\\begin{setcchessman},\\begin{setcchessman*},\\printman,\\cchessset": [
      "draft",
      "gridsize=",
      "boardtype=${1|x,x+,x+t,x+tn,x+Xtn|}",
      "boardlinewd=",
      "boardlinecolor=",
      "boardbg=${1:imagefile}",
      "piecechar={${1:piece}}{${2:symbol}}",
      "piecefont=${1:font commands}",
      "piecetype=${1|o,oo,ooo|}",
      "boxlinewd=",
      "boxcolor=",
      "redpiece=",
      "blkpiece=",
      "lower=",
      "donut=",
      "redupper=",
      "blkupper=",
      "shadow=",
      "charstroke=${1|none,solid,white,bold,invisible|}",
      "man={${1:man options}}",
      "man/label=",
      "man/nums=${1:integer}",
      "resize={${1:resize options}}",
      "resize/type=${1|none,real|}",
      "resize/xscale=${1:factor}",
      "resize/yscale=${1:factor}",
      "resize/scale=${1:factor}",
      "resize/width=",
      "resize/height="
    ]
  },
  "args": [
    "\\usepackage/chinesechess#c"
  ]
}