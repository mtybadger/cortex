{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "verbatim"
    }
  ],
  "macros": [
    {
      "name": "egpwrite",
      "arg": {
        "format": "{}",
        "snippet": "egpwrite{${1:gnuplot code%definition}}"
      }
    },
    {
      "name": "egpprelude",
      "arg": {
        "format": "{}",
        "snippet": "egpprelude{${1:gnuplot prelude%definition}}"
      }
    },
    {
      "name": "egpaddtoprelude",
      "arg": {
        "format": "{}",
        "snippet": "egpaddtoprelude{${1:gnuplot code%definition}}"
      }
    },
    {
      "name": "egpuse",
      "arg": {
        "format": "{}",
        "snippet": "egpuse{${1:name}}"
      }
    },
    {
      "name": "egpuse",
      "arg": {
        "format": "[]{}",
        "snippet": "egpuse[${2:keyvals}]{${1:name}}",
        "keys": [
          "\\begin{egpx},\\egpuse"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "egpfigprelude",
      "arg": {
        "format": "{}",
        "snippet": "egpfigprelude{${1:gnuplot prelude%definition}}"
      }
    },
    {
      "name": "egpaddtofigprelude",
      "arg": {
        "format": "{}",
        "snippet": "egpaddtofigprelude{${1:gnuplot code%definition}}"
      }
    },
    {
      "name": "egpfigepilog",
      "arg": {
        "format": "{}",
        "snippet": "egpfigepilog{${1:gnuplot epilog%definition}}"
      }
    },
    {
      "name": "egpaddtofigepilog",
      "arg": {
        "format": "{}",
        "snippet": "egpaddtofigepilog{${1:gnuplot code%definition}}"
      }
    },
    {
      "name": "egpcalc",
      "arg": {
        "format": "{}",
        "snippet": "egpcalc{${1:gnuplot expr%definition}}"
      }
    },
    {
      "name": "egpcalc",
      "arg": {
        "format": "[]{}",
        "snippet": "egpcalc[${2:name%definition}]{${1:gnuplot expr%definition}}"
      }
    },
    {
      "name": "egpuseval",
      "arg": {
        "format": "{}",
        "snippet": "egpuseval{${1:name%definition}}"
      }
    },
    {
      "name": "egpshowval",
      "arg": {
        "format": "{}",
        "snippet": "egpshowval{${1:gnuplot expr%definition}}"
      }
    },
    {
      "name": "egpshowval",
      "arg": {
        "format": "[]{}",
        "snippet": "egpshowval[${2:name%definition}]{${1:gnuplot expr%definition}}"
      }
    },
    {
      "name": "egpassign",
      "arg": {
        "format": "{}{}",
        "snippet": "egpassign{${1:variable%definition}}{${2:gnuplot expr%definition}}"
      }
    },
    {
      "name": "theegpcalc",
      "unusual": true
    },
    {
      "name": "theegpfig",
      "unusual": true
    },
    {
      "name": "theegpfile",
      "unusual": true
    },
    {
      "name": "theegpfilename",
      "unusual": true
    },
    {
      "name": "theegpfilenum",
      "unusual": true
    },
    {
      "name": "egpcomment",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "egpfile"
    },
    {
      "name": "egpfile",
      "arg": {
        "format": "[]",
        "snippet": "[${1:gnuplot file%file}]"
      }
    },
    {
      "name": "egpcmds"
    },
    {
      "name": "egp"
    },
    {
      "name": "egp",
      "arg": {
        "format": "[]",
        "snippet": "[${1:name}]"
      }
    },
    {
      "name": "egpx",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}",
        "keys": [
          "\\begin{egpx},\\egpuse"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "egpx",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:name}]{${1:keyvals}}",
        "keys": [
          "\\begin{egpx},\\egpuse"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "egpdef",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/egplot#c": [
      "german",
      "gnuplot35",
      "gnuplot36beta",
      "unknownkeysallowed",
      "draft",
      "final",
      "hiresbb",
      "demo",
      "setpagesize",
      "nosetpagesize",
      "dvips",
      "xdvi",
      "dvipdf",
      "dvipdfm",
      "dvipdfmx",
      "xetex",
      "pdftex",
      "luatex",
      "dvisvgm",
      "dvipsone",
      "dviwindo",
      "emtex",
      "dviwin",
      "oztex",
      "textures",
      "pctexps",
      "pctexwin",
      "pctexhp",
      "pctex32",
      "truetex",
      "tcidvi",
      "vtex",
      "debugshow",
      "hiderotate",
      "hidescale"
    ],
    "\\begin{egpx},\\egpuse": [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox|}",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ]
  },
  "args": [
    "\\usepackage/egplot#c"
  ]
}