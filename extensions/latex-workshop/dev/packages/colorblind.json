{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarymath"
    },
    {
      "name": "pgfplots"
    }
  ],
  "macros": [
    {
      "name": "drawScheme",
      "arg": {
        "format": "{}",
        "snippet": "drawScheme{${1:scheme}}"
      }
    },
    {
      "name": "drawSchemeC",
      "arg": {
        "format": "{}",
        "snippet": "drawSchemeC{${1:scheme}}"
      }
    },
    {
      "name": "drawSchemeC",
      "arg": {
        "format": "[]{}",
        "snippet": "drawSchemeC[${2:width}]{${1:scheme}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/colorblind#c": [
      "Tol",
      "OkabeIto"
    ]
  },
  "args": [
    "\\usepackage/colorblind#c"
  ]
}