  ],
  "macros": [
    {
      "name": "interval",
      "arg": {
        "format": "{}{}",
        "snippet": "interval{${1:start}}{${2:end}}"
      }
    },
    {
      "name": "interval",
      "arg": {
        "format": "[]{}{}",
