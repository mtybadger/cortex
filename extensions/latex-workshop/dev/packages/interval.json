        "keys": [
          "\\interval",
          "\\interval,\\ointerval,\\linterval,\\rinterval"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ointerval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "ointerval[${3:options}]{${1:start}}{${2:end}}",
        "keys": [
          "\\interval,\\ointerval,\\linterval,\\rinterval"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "linterval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "linterval[${3:options}]{${1:start}}{${2:end}}",
        "keys": [
          "\\interval,\\ointerval,\\linterval,\\rinterval"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "rinterval",
      "arg": {
        "format": "[]{}{}",
        "snippet": "rinterval[${3:options}]{${1:start}}{${2:end}}",
        "keys": [
          "\\interval,\\ointerval,\\linterval,\\rinterval"
        ],
        "keyPos": 0
      }
    },
    {
