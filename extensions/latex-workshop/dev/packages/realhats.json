{
  "deps": [
    {
      "name": "amsmath"
    },
    {
      "name": "calc"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "lcg"
    },
    {
      "name": "stackengine"
    }
  ],
  "macros": [
    {
      "name": "hat",
      "arg": {
        "format": "[]{}",
        "snippet": "hat[${2:type%keyvals}]{${1:arg}}",
        "keys": [
          "\\hat,\\usepackage/realhats#c,\\hatn,\\hatnoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "hatwidth",
      "unusual": true
    },
    {
      "name": "hshif",
      "unusual": true
    },
    {
      "name": "vshif",
      "unusual": true
    },
    {
      "name": "hatused",
      "unusual": true
    },
    {
      "name": "gethat",
      "unusual": true
    },
    {
      "name": "hatn",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "hatn{${1:arg}}{${2:type%keyvals}}"
      }
    },
    {
      "name": "hatnoptions",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "hatnoptions{${1:arg}}{${2:type%keyvals}}{${3:hshift%l}}{${4:vshift%l}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\hat,\\usepackage/realhats#c,\\hatn,\\hatnoptions": [
      "beret",
      "santa",
      "sombrero",
      "witch",
      "tophat",
      "ash",
      "fez",
      "cowboy",
      "crown",
      "dunce",
      "policeman",
      "scottish",
      "birthday",
      "mortarboard",
      "tile-white",
      "tile-gray",
      "tile-light-blue",
      "tile-blue"
    ]
  },
  "args": [
    "\\hat,\\usepackage/realhats#c,\\hatn,\\hatnoptions"
  ]
}