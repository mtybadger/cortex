{
  "deps": [
    {
      "name": "siunitx"
    },
    {
      "name": "pgfmath"
    },
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "SmartUnit",
      "arg": {
        "format": "{}",
        "snippet": "SmartUnit{${1:keyvals}}",
        "keys": [
          "\\SmartUnit,\\SmartUnitSettings",
          "\\SmartUnit"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "SmartUnitSettings",
      "arg": {
        "format": "{}",
        "snippet": "SmartUnitSettings{${1:settings%keyvals}}",
        "keys": [
          "\\SmartUnit,\\SmartUnitSettings"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\SmartUnit,\\SmartUnitSettings": [
      "metric",
      "imperial",
      "metric imperial",
      "imperial metric",
      "figures=${1:integer}",
      "places=${1:integer}",
      "uk",
      "usa"
    ],
    "\\SmartUnit": [
      "km=${1:number}",
      "miles=${1:number}",
      "mi=${1:number}",
      "cm=${1:number}",
      "inches=${1:number}",
      "feet=${1:number}",
      "celsius=${1:number}",
      "C=${1:number}",
      "fahrenheit=${1:number}",
      "F=${1:number}",
      "hours=${1:number}",
      "minutes=${1:number}",
      "seconds=${1:number}",
      "am",
      "pm",
      "L=${1:number}",
      "l=${1:number}",
      "gal=${1:number}",
      "gallons=${1:number}",
      "kg=${1:number}",
      "pounds=${1:number}",
      "pound=${1:number}",
      "lbs=${1:number}"
    ]
  },
  "args": []
}