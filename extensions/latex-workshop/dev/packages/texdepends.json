  "envs": [],
  "keys": {
    "\\RequirePackage/texdepends#c,\\usepackage/texdepends#c": [
      "debug",
      "extension=${1:file ext}"
    ]
  },
  "args": [
    "\\RequirePackage/texdepends#c,\\usepackage/texdepends#c"
  ]
}