{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "xstring"
    }
  ],
  "macros": [
    {
      "name": "RequireFile",
      "arg": {
        "format": "{}",
        "snippet": "RequireFile{${1:file}}"
