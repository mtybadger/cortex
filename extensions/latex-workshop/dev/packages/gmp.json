{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "environ"
    }
  ],
  "macros": [
    {
      "name": "gmpoptions",
      "arg": {
        "format": "{}",
        "snippet": "gmpoptions{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/gmp#c,\\gmpoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "usempxclass",
      "arg": {
        "format": "{}",
        "snippet": "usempxclass{${1:class}}"
      }
    },
    {
      "name": "usempxclass",
      "arg": {
        "format": "[]{}",
        "snippet": "usempxclass[${2:options}]{${1:class}}"
      }
    },
    {
      "name": "usempxpackage",
      "arg": {
        "format": "{}",
        "snippet": "usempxpackage{${1:package}}"
      }
    },
    {
      "name": "usempxpackage",
      "arg": {
        "format": "[]{}",
        "snippet": "usempxpackage[${2:options}]{${1:package}}"
      }
    },
    {
      "name": "resetmpxpackages"
    },
    {
      "name": "mpxcommands",
      "arg": {
        "format": "{}",
        "snippet": "mpxcommands{${1:commands}}"
      }
    },
    {
      "name": "resetmpxcommands"
    },
    {
      "name": "mpdim",
      "arg": {
        "format": "{}",
        "snippet": "mpdim{${1:dimen param}}"
      }
    },
    {
      "name": "usempost",
      "arg": {
        "format": "{}",
        "snippet": "usempost{${1:string}}"
      }
    },
    {
      "name": "usempost",
      "arg": {
        "format": "[]{}",
        "snippet": "usempost[${2:options}]{${1:string}}",
        "keys": [
          "\\usempost"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "btex"
    },
    {
      "name": "verbatimtex"
    }
  ],
  "envs": [
    {
      "name": "mpost"
    },
    {
      "name": "mpost",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{mpost},\\begin{mpost*}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "mpost*"
    },
    {
      "name": "mpost*",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{mpost},\\begin{mpost*}"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/gmp#c,\\gmpoptions": [
      "tex",
      "latex",
      "noshellescape",
      "shellescape",
      "nowrite",
      "write",
      "envname=${1:envname}",
      "extension=${1:extension}",
      "everymp={${1:metapost code}}",
      "clean=${1|none,aux,mp|}",
      "rmcommand=${1:cmd}",
      "postrmcommand=${1:cmd}"
    ],
    "\\usempost": [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox|}",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ],
    "\\begin{mpost},\\begin{mpost*}": [
      "name=${1:string}",
      "use",
      "mpmem=${1:metapost format}",
      "mpxprogram=${1|tex,latex|}",
      "mpxcommands=${1:commands}",
      "mpsettings={${1:metapost code}}",
      "runs=${1:number}"
    ]
  },
  "args": [
    "\\usepackage/gmp#c,\\gmpoptions"
  ]
}