{
  "deps": [],
  "macros": [
    {
      "name": "usefastocksize",
      "arg": {
        "format": "{}",
        "snippet": "usefastocksize{${1:width,height}}"
      }
    },
    {
      "name": "usefapapersize",
      "arg": {
        "format": "{}",
        "snippet": "usefapapersize{${1:width,height,lmargin,rmargin,tmargin,bmargin}}"
      }
    },
    {
      "name": "definefageometry",
      "arg": {
        "format": "{}{}",
        "snippet": "definefageometry{${1:name%specialDef}}{${2:layout}}"
      }
    },
    {
      "name": "definefageometry",
      "arg": {
        "format": "{}{}[]",
        "snippet": "definefageometry{${1:name}}{${2:layout}}[${3:after}]"
      }
    },
    {
      "name": "definefageometry",
      "arg": {
        "format": "{}[]{}",
        "snippet": "definefageometry{${1:name}}[${3:before}]{${2:layout}}"
      }
    },
    {
      "name": "definefageometry",
      "arg": {
        "format": "{}[]{}[]",
        "snippet": "definefageometry{${1:name}}[${3:before}]{${2:layout}}[${4:after}]"
      }
    },
    {
      "name": "selectfageometry",
      "arg": {
        "format": "{}",
        "snippet": "selectfageometry{${1:name%keyvals}}",
        "keys": [
          "\\selectfageometry#c,\\selectfageometry*#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "selectfageometry*",
      "arg": {
        "format": "{}",
        "snippet": "selectfageometry*{${1:name%keyvals}}",
        "keys": [
          "\\selectfageometry#c,\\selectfageometry*#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "evenmarginsameasodd"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/fapapersize#c": [
      "dbl4x6",
      "mum",
      "newmum",
      "1in",
      "a4",
      "stock",
      "showtrims"
    ],
    "\\selectfageometry#c,\\selectfageometry*#c": [
      "%fageometry"
    ]
  },
  "args": [
    "\\usepackage/fapapersize#c"
  ]
}