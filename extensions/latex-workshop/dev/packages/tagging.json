{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "verbatim"
    }
  ],
  "macros": [
    {
      "name": "tagged",
      "arg": {
        "format": "{}{}",
        "snippet": "tagged{${1:tag list}}{${2:source text}}"
      }
    },
    {
      "name": "untagged",
      "arg": {
        "format": "{}{}",
        "snippet": "untagged{${1:tag list}}{${2:alt text}}"
      }
    },
    {
      "name": "iftagged",
      "arg": {
        "format": "{}{}{}",
        "snippet": "iftagged{${1:tag list}}{${2:source text}}{${3:alt text}}"
      }
    },
    {
      "name": "usetag",
      "arg": {
        "format": "{}",
        "snippet": "usetag{${1:tag list}}"
      }
    },
    {
      "name": "droptag",
      "arg": {
        "format": "{}",
        "snippet": "droptag{${1:tag list}}"
      }
    },
    {
      "name": "taggedy",
      "unusual": true
    },
    {
      "name": "taggedn",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "taggedblock",
      "arg": {
        "format": "{}",
        "snippet": "{${1:tag list}}"
      }
    },
    {
      "name": "untaggedblock",
      "arg": {
        "format": "{}",
        "snippet": "{${1:tag list}}"
      }
    }
  ],
  "keys": {},
  "args": []
}