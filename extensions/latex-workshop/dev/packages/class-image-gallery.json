{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "keyval"
    },
    {
      "name": "url"
    },
    {
      "name": "geometry"
    },
    {
      "name": "color",
      "if": "dummy"
    }
  ],
  "macros": [
    {
      "name": "makeGallery",
      "arg": {
        "format": "{}",
        "snippet": "makeGallery{${1:file}}"
      }
    },
    {
      "name": "gallerySetup",
      "arg": {
        "format": "{}",
        "snippet": "gallerySetup{${1:options%keyvals}}",
        "keys": [
          "\\gallerySetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/image-gallery#c": [
      "dummy",
      "nocaption",
      "a4paper",
      "a5paper",
      "b5paper",
      "letterpaper",
      "legalpaper",
      "executivepaper",
      "landscape",
      "10pt",
      "11pt",
      "12pt",
      "oneside",
      "twoside",
      "draft",
      "final",
      "titlepage",
      "notitlepage",
      "onecolumn",
      "twocolumn",
      "leqno",
      "fleqn",
      "openbib"
    ],
    "\\gallerySetup": [
      "width=",
      "height=",
      "top=",
      "bottom=",
      "left=",
      "right=",
      "rows=${1:integer}",
      "columns=${1:integer}",
      "autorotate"
    ]
  },
  "args": [
    "\\documentclass/image-gallery#c"
  ]
}