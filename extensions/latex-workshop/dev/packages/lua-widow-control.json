{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "microtype"
    }
  ],
  "macros": [
    {
      "name": "lwcsetup",
      "arg": {
        "format": "{}",
        "snippet": "lwcsetup{${1:options%keyvals}}",
        "keys": [
          "\\lwcsetup,\\usepackage/lua-widow-control#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\lwcsetup,\\usepackage/lua-widow-control#c": [
      "disable",
      "enable",
      "disablecmds={${1:\\macro1,\\macro2,...}}",
      "emergencystretch=",
      "widowpenalty=${1:integer}",
      "orphanpenalty=${1:integer}",
      "nobreak=${1|keep,split,warn|}",
      "max-cost=${1:integer}",
      "draft",
      "showcosts",
      "showcolours",
      "draftoffset=",
      "debug",
      "default",
      "strict",
      "balanced"
    ],
    "\\usepackage/lua-widow-control#c": [
      "microtype"
    ]
  },
  "args": [
    "\\lwcsetup,\\usepackage/lua-widow-control#c",
    "\\usepackage/lua-widow-control#c"
  ]
}