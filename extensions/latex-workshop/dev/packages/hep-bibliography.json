{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "biblatex"
    },
    {
      "name": "relsize"
    }
  ],
  "macros": [
    {
      "name": "online",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "online{${1:text}}{${2:URL}}"
      }
    },
    {
      "name": "email",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "email{${1:email%URL}}"
      }
    },
    {
      "name": "commalistbody",
      "unusual": true
    },
    {
      "name": "relateddelimerratum",
      "unusual": true
    },
    {
      "name": "ccite",
      "arg": {
        "format": "[][]{}",
        "snippet": "ccite[${2:prenote}][${3:postnote}]{${1:bibid}}"
      }
    },
    {
      "name": "ccite",
      "arg": {
        "format": "[]{}",
        "snippet": "ccite[${2:postnote}]{${1:bibid}}"
      }
    },
    {
      "name": "ccite",
      "arg": {
        "format": "{}",
        "snippet": "ccite{${1:bibid}}"
      }
    },
    {
      "name": "Ccite",
      "arg": {
        "format": "[][]{}",
        "snippet": "Ccite[${2:prenote}][${3:postnote}]{${1:bibid}}"
      }
    },
    {
      "name": "Ccite",
      "arg": {
        "format": "[]{}",
        "snippet": "Ccite[${2:postnote}]{${1:bibid}}"
      }
    },
    {
      "name": "Ccite",
      "arg": {
        "format": "{}",
        "snippet": "Ccite{${1:bibid}}"
      }
    }
  ],
  "envs": [
    {
      "name": "commalist",
      "unusual": true
    },
    {
      "name": "commalist",
      "unusual": true,
      "arg": {
        "format": "[]",
        "snippet": "[${1:opt}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/hep-bibliography#c": [
      "style=${1|authoryear,authortitle,alphabetic,numeric,alphabetic-verb,authortitle-comp,authortitle-ibid,authortitle-icomp,authortitle-tcomp,authortitle-terse,authortitle-ticomp,authoryear-comp,authoryear-ibid,authoryear-icomp,debug,draft,numeric-comp,numeric-verb,reading,verbose,verbose-ibid,verbose-inote,verbose-note,verbose-trad1,verbose-trad2,verbose-trad3|}"
    ]
  },
  "args": [
    "\\usepackage/hep-bibliography#c"
  ]
}