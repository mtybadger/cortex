{
  "deps": [
    {
      "name": "saveenv"
    },
    {
      "name": "currfile"
    },
    {
      "name": "currfile-abspath"
    },
    {
      "name": "precattl"
    }
  ],
  "macros": [
    {
      "name": "py",
      "arg": {
        "format": "{}",
        "snippet": "py{${1:python expr%definition}}"
      }
    },
    {
      "name": "pyc",
      "arg": {
        "format": "{}",
        "snippet": "pyc{${1:python code%definition}}"
      }
    },
    {
      "name": "pycq",
      "arg": {
        "format": "{}",
        "snippet": "pycq{${1:python code%definition}}"
      }
    },
    {
      "name": "pyfile",
      "arg": {
        "format": "{}",
        "snippet": "pyfile{${1:file}}"
      }
    },
    {
      "name": "pythonimmediatecontinue",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "pythonimmediatecontinue{${1:code}}"
      }
    }
  ],
  "envs": [
    {
      "name": "pycode"
    },
    {
      "name": "pycodeq"
    }
  ],
  "keys": {
    "\\usepackage/pythonimmediate#c": [
      "args=${1:Python args}",
      "python-executable=${1:path}",
      "python-flags=${1:flags}",
      "child-process"
    ]
  },
  "args": [
    "\\usepackage/pythonimmediate#c"
  ]
}