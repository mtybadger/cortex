{
  "deps": [
    {
      "name": "simplekv"
    }
  ],
  "macros": [
    {
      "name": "hitem"
    },
    {
      "name": "hitem>"
    },
    {
      "name": "hitem*"
    },
    {
      "name": "hitem*>"
    },
    {
      "name": "hitem",
      "arg": {
        "format": "()",
        "snippet": "hitem(${1:num})"
      }
    },
    {
      "name": "sethlist",
      "arg": {
        "format": "{}",
        "snippet": "sethlist{${1:keyvals}}",
        "keys": [
          "\\sethlist,\\setdefaulthlist"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "setdefaulthlist",
      "arg": {
        "format": "{}",
        "snippet": "setdefaulthlist{${1:keyvals}}",
        "keys": [
          "\\sethlist,\\setdefaulthlist"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "hlist",
      "arg": {
        "format": "{}",
        "snippet": "{${1:num of colums}}"
      }
    }
  ],
  "keys": {
    "\\sethlist,\\setdefaulthlist": [
      "pre skip=",
      "post skip=",
      "left margin=",
      "col sep=",
      "item offset=",
      "label sep=",
      "label width=",
      "label align=${1|left,right,center|}",
      "label=${1:code}",
      "pre label=${1:code}",
      "post label=${1:code}",
      "item sep=",
      "resume",
      "autoindent",
      "pre item=${1:code}",
      "post item=${1:code}",
      "show label",
      "list parindent=",
      "show frame"
    ]
  },
  "args": []
}