{
  "deps": [
    {
      "name": "ifxetex"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "class-ctexbook"
    },
    {
      "name": "xeCJK"
    },
    {
      "name": "geometry"
    },
    {
      "name": "xeCJKfntef"
    },
    {
      "name": "array"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "calc"
    },
    {
      "name": "tikz"
    },
    {
      "name": "xifthen"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "textpos"
    }
  ],
  "macros": [
    {
      "name": "coversetup",
      "arg": {
        "format": "{}",
        "snippet": "coversetup{${1:options%keyvals}}",
        "keys": [
          "\\coversetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "makecover"
    },
    {
      "name": "makecover",
      "arg": {
        "format": "[]",
        "snippet": "makecover[${1:pdf file%file}]"
      }
    },
    {
      "name": "xingkai",
      "unusual": true
    },
    {
      "name": "zhkai",
      "unusual": true
    },
    {
      "name": "xbsong",
      "unusual": true
    },
    {
      "name": "dbsong",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/buctcover#c": [
      "type=${1|bachelor,master,doctor|}"
    ],
    "\\coversetup": [
      "class={${1:class}}",
      "studentid={${1:id number}}",
      "titleA={${1:text}}",
      "titleB={${1:text}}",
      "major={${1:专业}}",
      "author={${1:中文名}}",
      "supervisor={${1:导师姓名, 教授}}",
      "date={${1:YYYY-MM-DD}}"
    ]
  },
  "args": [
    "\\documentclass/buctcover#c"
  ]
}