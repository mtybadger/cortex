{
  "deps": [
    {
      "name": "l3draw"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "zhlipsum",
      "if": "enable-zhlipsum"
    },
    {
      "name": "zhlipsum",
      "if": "enable-zhlipsum=true"
    }
  ],
  "macros": [
    {
      "name": "zitienewfont*",
      "arg": {
        "format": "{}{}{}",
        "snippet": "zitienewfont*{${1:font family}}{${2:font name}}{${3:font features}}"
      }
    },
    {
      "name": "zitieCJKfamily",
      "arg": {
        "format": "{}",
        "snippet": "zitieCJKfamily{${1:font family}}"
      }
    },
    {
      "name": "framesingle",
      "arg": {
        "format": "{}",
        "snippet": "framesingle{${1:character}}"
      }
    },
    {
      "name": "framesingle",
      "arg": {
        "format": "[]{}",
        "snippet": "framesingle[${2:options%keyvals}]{${1:character}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezi",
      "arg": {
        "format": "{}",
        "snippet": "framezi{${1:characters}}"
      }
    },
    {
      "name": "framezi",
      "arg": {
        "format": "[]{}",
        "snippet": "framezi[${2:options%keyvals}]{${1:characters}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezi*",
      "arg": {
        "format": "{}",
        "snippet": "framezi*{${1:characters}}"
      }
    },
    {
      "name": "framezi*",
      "arg": {
        "format": "[]{}",
        "snippet": "framezi*[${2:options%keyvals}]{${1:characters}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framerange",
      "arg": {
        "format": "{}",
        "snippet": "framerange{${1:hex range}}"
      }
    },
    {
      "name": "framerange",
      "arg": {
        "format": "[]{}",
        "snippet": "framerange[${2:options%keyvals}]{${1:hex range}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezifile",
      "arg": {
        "format": "{}",
        "snippet": "framezifile{${1:file}}"
      }
    },
    {
      "name": "framezifile",
      "arg": {
        "format": "[]{}",
        "snippet": "framezifile[${2:options%keyvals}]{${1:file}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezifile*",
      "arg": {
        "format": "{}",
        "snippet": "framezifile*{${1:file}}"
      }
    },
    {
      "name": "framezifile*",
      "arg": {
        "format": "[]{}",
        "snippet": "framezifile*[${2:options%keyvals}]{${1:file}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "frametallrange",
      "arg": {
        "format": "{}",
        "snippet": "frametallrange{${1:hex range}}"
      }
    },
    {
      "name": "frametallrange",
      "arg": {
        "format": "[]{}",
        "snippet": "frametallrange[${2:options%keyvals}]{${1:hex range}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezitallfile",
      "arg": {
        "format": "{}",
        "snippet": "framezitallfile{${1:file}}"
      }
    },
    {
      "name": "framezitallfile",
      "arg": {
        "format": "[]{}",
        "snippet": "framezitallfile[${2:options%keyvals}]{${1:file}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezitallfile*",
      "arg": {
        "format": "{}",
        "snippet": "framezitallfile*{${1:file}}"
      }
    },
    {
      "name": "framezitallfile*",
      "arg": {
        "format": "[]{}",
        "snippet": "framezitallfile*[${2:options%keyvals}]{${1:file}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "zitiesetup",
      "arg": {
        "format": "{}",
        "snippet": "zitiesetup{${1:options%keyvals}}",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "zitiestrokechars",
      "arg": {
        "format": "{}{}",
        "snippet": "zitiestrokechars{${1:pdf literal}}{${2:typeset material}}"
      }
    },
    {
      "name": "zitiecolorlet",
      "arg": {
        "format": "{}{}",
        "snippet": "zitiecolorlet{${1:name%specialDef}}{${2:value}}"
      }
    },
    {
      "name": "zitiecolorlet",
      "arg": {
        "format": "{}[]{}",
        "snippet": "zitiecolorlet{${1:name%specialDef}}[${3:model}]{${2:value}}"
      }
    },
    {
      "name": "zitienewprocessorrule",
      "arg": {
        "format": "{}{}{}",
        "snippet": "zitienewprocessorrule{${1:processor}}{${2:rule}}{${3:replace}}"
      }
    },
    {
      "name": "zitienewprocessorrule",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "zitienewprocessorrule[${4:arg nums}]{${1:processor}}{${2:rule}}{${3:replace}}"
      }
    },
    {
      "name": "zitienewrule",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "zitienewrule{${1:processor}}{${2:rule}}{${3:replace}}"
      }
    },
    {
      "name": "zitienewrule",
      "unusual": true,
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "zitienewrule[${4:arg nums}]{${1:processor}}{${2:rule}}{${3:replace}}"
      }
    },
    {
      "name": "zitieuseprocessorrule",
      "arg": {
        "format": "{}{}",
        "snippet": "zitieuseprocessorrule{${1:processor}}{${2:rule}}"
      }
    },
    {
      "name": "zitieuseprocessorrule",
      "arg": {
        "format": "[]{}{}",
        "snippet": "zitieuseprocessorrule[${3:arg nums}]{${1:processor}}{${2:rule}}"
      }
    },
    {
      "name": "zitiebasechar"
    },
    {
      "name": "zitiebasecharwidth"
    },
    {
      "name": "zitiebasecharheight"
    },
    {
      "name": "zitiewidth"
    },
    {
      "name": "zitieheight"
    },
    {
      "name": "zitieboxwd"
    },
    {
      "name": "zitieboxht"
    },
    {
      "name": "zitieboxdp"
    },
    {
      "name": "zitiefontname"
    },
    {
      "name": "zitiexscaleratio"
    },
    {
      "name": "zitieyscaleratio"
    },
    {
      "name": "zitiebackground"
    },
    {
      "name": "zitiebackground",
      "arg": {
        "format": "[]",
        "snippet": "zitiebackground[${1:options%keyvals}]",
        "keys": [
          "\\zitiebackground"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "framezhlipsum",
      "arg": {
        "format": "{}",
        "snippet": "framezhlipsum{${1:paragraph list}}"
      }
    },
    {
      "name": "framezhlipsum",
      "arg": {
        "format": "{}[]",
        "snippet": "framezhlipsum{${1:paragraph list}}[${2:name}]"
      }
    },
    {
      "name": "framezhlipsum",
      "arg": {
        "format": "[]{}",
        "snippet": "framezhlipsum[${2:options%keyvals}]{${1:paragraph list}}"
      }
    },
    {
      "name": "framezhlipsum",
      "arg": {
        "format": "[]{}[]",
        "snippet": "framezhlipsum[${2:options%keyvals}]{${1:paragraph list}}[${3:name}]"
      }
    },
    {
      "name": "zitierawCJKfamily",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "zitierawCJKfamily{${1:font family}}"
      }
    }
  ],
  "envs": [
    {
      "name": "zitieframe"
    },
    {
      "name": "zitieframe",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/zitie#c": [
      "enable-zhlipsum",
      "enable-background"
    ],
    "\\framesingle,\\framezi,\\framezi*,\\framerange,\\framezifile,\\framezifile*,\\begin{zitieframe},\\frametallrange,\\framezitallfile,\\framezitallfile*,\\zitiesetup,\\framezhlipsum": [
      "basechar=${1:character}",
      "fontsize=${1:fontsize command}",
      "zihao=${1:字号}",
      "position=",
      "position*={${1:replace}}",
      "anchor=${1|east,southeast,south,southwest,west,northwest,north,northeast,center|}",
      "punctuation=${1|ignore,leave,onlast,scale|}",
      "punctuation*={${1:replace}}",
      "frametype=${1|none,口,十,田,×,米,咪|}",
      "resize=${1|none,real,base,square|}",
      "xscale=${1:scale ratio}",
      "yscale=${1:scale ratio}",
      "scale=${1:scale ratio}",
      "width=",
      "height=",
      "holdbasecharheight",
      "holdbasecharwidth",
      "holdbasechar",
      "linewidth=",
      "dashpattern={${1:dim1,dim2,...}}",
      "framearc=",
      "framearc*={${1:dim1}}{${2:dim2}}",
      "framecolor=",
      "framecolor*={${1:models}}{${2:values}}",
      "charcolor=",
      "charcolor*={${1:models}}{${2:values}}",
      "fillcolor=",
      "fillcolor*={${1:models}}{${2:values}}",
      "charstroke=${1|none,solid,dashed,whitesolid,whitedashed,thicksolid,thickdashed,thickwhitesolid,thickwhitedashed,invisible|}",
      "charstrokespecial=${1:pdf literal}",
      "font=${1:font family}",
      "savefontname",
      "fallback",
      "fallbackfont={${1:fallback font list}}",
      "fallbackfont+={${1:fallback font list}}",
      "filepath={${1:filepath1,filepath2,...}}",
      "filepath+={${1:filepath1,filepath2,...}}",
      "repeat=${1:integer}",
      "break=${1|default,allowbreak|}",
      "tolerance=",
      "tallheight=${1:integer}",
      "validateglyph"
    ],
    "\\zitiebackground": [
      "true",
      "on",
      "false",
      "off",
      "next",
      "colboxes=${1:integer}",
      "rowboxes=${1:integer}",
      "framewidth=",
      "frameheight=",
      "boxwidth=",
      "boxheight=",
      "onpaper",
      "ontext",
      "frametype=${1|none,口,十,田,×,米,咪,二|}",
      "linewidth=",
      "framecolor=",
      "framecolor*={${1:models}}{${2:values}}",
      "fillcolor=",
      "fillcolor*={${1:models}}{${2:values}}",
      "dashpattern={${1:dim1,dim2,...}}",
      "xrange={${1:left,right}}",
      "yrange={${1:top,bottom}}"
    ]
  },
  "args": [
    "\\usepackage/zitie#c"
  ]
}