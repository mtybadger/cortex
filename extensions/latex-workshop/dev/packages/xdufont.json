{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "xeCJK"
    },
    {
      "name": "unicode-math"
    }
  ],
  "macros": [
    {
      "name": "xdusetup",
      "arg": {
        "format": "{}",
        "snippet": "xdusetup{${1:options%keyvals}}",
        "keys": [
          "\\xdusetup#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\xdusetup#c": [
      "style={${1:keyvals}}",
      "style/cjk-font=${1|adobe,fandol,founder,hanyi,sinotype,win,none|}",
      "style/cjk-fake-bold=${1:伪粗体粗细程度}",
      "style/cjk-fake-slant=${1:伪斜体倾斜程度}",
      "style/latin-font=${1|gyre,tac,tacn,tcc,thcs,tll,none|}",
      "style/latin-sans-scale=${1|upper,lower,off|}",
      "style/latin-mono-scale=${1|upper,lower,off|}",
      "style/math-font=${1|asana,cambria,cm,concrete,erewhon,euler,fira,garamond,gfsneohellenic,kp,libertinus,lm,newcm,stix2,stix,xcharter,xits,bonum,dejavu,pagella,schola,termes,none|}",
      "style/unicode-math={${1:unicode-math宏包选项}}",
      "style/font-type=${1|font,file|}",
      "style/font-path={${1:路径}}"
    ]
  },
  "args": []
}