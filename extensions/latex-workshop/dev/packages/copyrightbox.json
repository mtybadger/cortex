{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "tikzlibrarypositioning"
    }
  ],
  "macros": [
    {
      "name": "copyrightbox",
      "arg": {
        "format": "{}{}",
        "snippet": "copyrightbox{${1:content}}{${2:text}}"
      }
    },
    {
      "name": "copyrightbox",
      "arg": {
        "format": "[]{}{}",
        "snippet": "copyrightbox[${3:placement%keyvals}]{${1:content}}{${2:text}}",
        "keys": [
          "\\copyrightbox"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\copyrightbox": [
      "l",
      "r",
      "b"
    ]
  },
  "args": []
}