{
  "deps": [
    {
      "name": "pdfbase"
    }
  ],
  "macros": [
    {
      "name": "xsbox",
      "arg": {
        "format": "{}{}",
        "snippet": "xsbox{${1:name}}{${2:content%text}}"
      }
    },
    {
      "name": "xsavebox",
      "arg": {
        "format": "{}{}",
        "snippet": "xsavebox{${1:name}}{${2:content%text}}"
      }
    },
    {
      "name": "xsavebox",
      "arg": {
        "format": "{}[]{}",
        "snippet": "xsavebox{${1:name}}[${3:width}]{${2:content%text}}"
      }
    },
    {
      "name": "xsavebox",
      "arg": {
        "format": "{}[][]{}",
        "snippet": "xsavebox{${1:name}}[${3:width}][${4:position}]{${2:content%text}}"
      }
    },
    {
      "name": "xsavebox*",
      "arg": {
        "format": "{}{}",
        "snippet": "xsavebox*{${1:name}}{${2:content%text}}"
      }
    },
    {
      "name": "xsavebox*",
      "arg": {
        "format": "{}[]{}",
        "snippet": "xsavebox*{${1:name}}[${3:width}]{${2:content%text}}"
      }
    },
    {
      "name": "xsavebox*",
      "arg": {
        "format": "{}[][]{}",
        "snippet": "xsavebox*{${1:name}}[${3:width}][${4:position}]{${2:content%text}}"
      }
    },
    {
      "name": "xusebox",
      "arg": {
        "format": "{}",
        "snippet": "xusebox{${1:name}}"
      }
    }
  ],
  "envs": [
    {
      "name": "xlrbox",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    },
    {
      "name": "xlrbox*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/xsavebox#c": [
      "margin="
    ]
  },
  "args": [
    "\\usepackage/xsavebox#c"
  ]
}