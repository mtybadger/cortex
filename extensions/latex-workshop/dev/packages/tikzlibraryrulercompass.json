{
  "deps": [
    {
      "name": "intersections"
    },
    {
      "name": "calc"
    }
  ],
  "macros": [
    {
      "name": "thepointlabels",
      "unusual": true
    },
    {
      "name": "point",
      "arg": {
        "format": "{}{}{}",
        "snippet": "point{${1:path1}}{${2:path2}}{${3:intersection}}"
      }
    },
    {
      "name": "point",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "point[${4:styles}]{${1:path1}}{${2:path2}}{${3:intersection}}"
      }
    },
    {
      "name": "ruler",
      "arg": {
        "format": "{}{}",
        "snippet": "ruler{${1:point1}}{${2:point2}}"
      }
    },
    {
      "name": "ruler",
      "arg": {
        "format": "[]{}{}",
        "snippet": "ruler[${3:styles}]{${1:point1}}{${2:point2}}"
      }
    },
    {
      "name": "compass",
      "arg": {
        "format": "{}{}",
        "snippet": "compass{${1:point1}}{${2:point2}}"
      }
    },
    {
      "name": "compass",
      "arg": {
        "format": "[]{}{}",
        "snippet": "compass[${3:styles}]{${1:point1}}{${2:point2}}"
      }
    },
    {
      "name": "constrain"
    }
  ],
  "envs": [],
  "keys": {
    "\\begin{tikzpicture}#c,\\tikz#c,\\begin{scope}#c,\\tikzset#c,\\scoped#c": [
      "stop jumping",
      "max size={${1:width}}{${2:height}}",
      "enclosing box/offset=",
      "constrain"
    ]
  },
  "args": []
}