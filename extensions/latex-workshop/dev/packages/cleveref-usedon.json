{
  "deps": [
    {
      "name": "cleveref"
    }
  ],
  "macros": [
    {
      "name": "cref",
      "arg": {
        "format": "[]{}",
        "snippet": "cref[${2:option%keyvals}]{${1:labellist}}",
        "keys": [
          "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "cref*",
      "arg": {
        "format": "[]{}",
        "snippet": "cref*[${2:option%keyvals}]{${1:labellist}}",
        "keys": [
          "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "Cref",
      "arg": {
        "format": "[]{}",
        "snippet": "Cref[${2:option%keyvals}]{${1:labellist}}",
        "keys": [
          "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "Cref*",
      "arg": {
        "format": "[]{}",
        "snippet": "Cref*[${2:option%keyvals}]{${1:labellist}}",
        "keys": [
          "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/cleveref-usedon#c": [
      "poorman",
      "sort",
      "compress",
      "sort&compress",
      "nosort",
      "capitalise",
      "capitalize",
      "nameinlink",
      "noabbrev",
      "english",
      "german",
      "ngerman",
      "dutch",
      "french",
      "spanish",
      "italian",
      "russian",
      "ukrainian",
      "norsk",
      "danish",
      "esperanto",
      "swedish",
      "brazilian",
      "catalan"
    ],
    "\\cref#c,\\cref*#c,\\Cref#c,\\Cref*#c": [
      "UsedOn",
      "UsedBy",
      "UsedByAndOn"
    ]
  },
  "args": [
    "\\usepackage/cleveref-usedon#c"
  ]
}