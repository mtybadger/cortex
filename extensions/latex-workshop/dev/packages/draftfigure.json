{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "calc"
    }
  ],
  "macros": [
    {
      "name": "setdf",
      "arg": {
        "format": "{}",
        "snippet": "setdf{${1:options%keyvals}}",
        "keys": [
          "\\setdf,\\usepackage/draftfigure#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\setdf,\\usepackage/draftfigure#c": [
      "content={%|}",
      "filename",
      "style=${1|sf,tt,rm,normal,bf,it,sc|}",
      "size=${1|tiny,scriptsize,small,footnotesize,normal,large,Large,LARGE,huge|}",
      "position=${1|left,center,right|}",
      "noframe"
    ]
  },
  "args": [
    "\\setdf,\\usepackage/draftfigure#c"
  ]
}