      "name": "replaced",
      "arg": {
        "format": "[]{}{}",
        "snippet": "replaced[${3:keyvals}]{${1:new%text}}{${2:old%text}}",
        "keys": [
          "\\added,\\deleted,\\replaced,\\highlight"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "replaced",
      "arg": {
        "format": "{}{}",
        "snippet": "replaced{${1:new%text}}{${2:old%text}}"
      }
    },
    {
      "name": "highlight",
      "arg": {
        "format": "[]{}",
        "snippet": "highlight[${2:keyvals}]{${1:text}}",
