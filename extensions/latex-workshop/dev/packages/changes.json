      "name": "definechangesauthor",
      "arg": {
        "format": "[]{}",
        "snippet": "definechangesauthor[${2:keyvals}]{${1:authorid%specialDef}}",
        "keys": [
          "\\definechangesauthor"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "definechangesauthor",
      "arg": {
        "format": "{}",
        "snippet": "definechangesauthor{${1:authorid%specialDef}}"
      }
    },
    {
      "name": "added",
      "arg": {
        "format": "[]{}",
        "snippet": "added[${2:keyvals}]{${1:text}}",
        "keys": [
          "\\added,\\deleted,\\replaced,\\highlight"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "added",
      "arg": {
        "format": "{}",
        "snippet": "added{${1:text}}"
      }
    },
    {
      "name": "deleted",
