  "envs": [],
  "keys": {
    "\\usepackage/cooking-units#c": [
      "use-fmtcount-numerals"
    ],
    "\\cunum#c,\\cutext#c,\\Cutext#c,\\cuam#c,\\cusetup#c": [
      "${1:old unit}=${2:new unit}",
      "${1:group}=${2:unit}",
      "add-unit-to-group={${1:group={unit list},...}}",
      "set-option-for-${1:unit}={${2:keyvals}}",
      "add-option-for-${1:unit}={${2:keyvals}}",
      "set-option-for={${1:unit={keyvals},...}}",
      "add-option-for={${1:unit={keyvals},...}}",
      "erase-all-options",
      "erase-all-options-for={${1:unit list}}",
      "cutext-to-cunum",
      "cutext-change-unit",
      "cuam-version=${1|old,new|}",
      "cutext-version=${1|old,new|}",
      "commands-add-hook={${1:code}}",
      "cunum-add-hook={${1:code}}",
      "cutext-add-hook={${1:code}}",
      "Cutext-add-hook={${1:code}}",
      "cuam-add-hook={${1:code}}",
      "expand-both=${1|n,o,f,x|}",
      "expand-amount=${1|n,o,f,x|}",
      "expand-unit=${1|n,o,f,x|}",
      "set-special-sign={${1:characters}}",
      "add-special-sign={${1:characters}}",
      "set-unknown-message=${1|error,warning,none|}",
      "set-cutext-translation-message=${1|error,warning,none|}",
      "print-numerals",
      "use-numerals-below=${1:integer}",
      "numeral-function=${1:function}",
      "Numeral-function=${1:function}",
      "parse-number",
      "range-sign=${1:string}",
      "cunum-range-sign=${1:string}",
      "cutext-range-sign=${1:string}",
      "use-phrases",
      "round-precision=${1:integer}",
      "round-to-int",
      "round-half=${1|default,commercial|}",
      "eval-fraction",
      "convert-fraction",
      "fraction-inline={${1:input}}",
      "mixed-fraction-space=",
      "cutext-space=${1:string}",
      "phrase-space=${1:string}",
      "amount-unit-space=${1:string}",
      "times-space=${1:string}",
      "recalculate-amount",
      "set-number-of-persons=${1:integer}",
      "label=${1:string}*${2:integer}",
      "get-label=${1:label}",
      "ref=${1:label}",
      "curef-add-forbidden-unit={${1:unit list}}",
      "curef-remove-forbidden-unit={${1:unit list}}",
      "curef-clear-forbidden-units",
      "check-temperature",
      "add-temperature-to-check={${1:unit=min value,...}}",
      "convert-to-eV",
      "add-natural-unit=${1:unit}",
      "42",
      "nothing-special",
      "going-bonkers",
      "fully-bonkers",
      "xD-lol"
    ],
    "\\declarecookingunit#c,\\newcookingunit#c,\\providecookingunit#c": [
      "symbol=${1:symbol}",
      "gender=${1|m,f,n|}",
      "set-options={${1:keyvals}}",
      "add-to-group=${1:group}",
      "natural-unit"
    ]
  },
  "args": [
    "\\usepackage/cooking-units#c"
  ]
}