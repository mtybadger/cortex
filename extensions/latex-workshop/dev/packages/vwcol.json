{
  "deps": [
    {
      "name": "calc"
    },
    {
      "name": "color"
    },
    {
      "name": "environ"
    },
    {
      "name": "keyval"
    },
    {
      "name": "ragged2e"
    }
  ],
  "macros": [
    {
      "name": "vwcolsetup",
      "arg": {
        "format": "{}",
        "snippet": "vwcolsetup{${1:options%keyvals}}",
        "keys": [
          "\\begin{vwcol},\\vwcolsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "vwcol"
    },
    {
      "name": "vwcol",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{vwcol},\\vwcolsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/vwcol#c": [
      "quiet"
    ],
    "\\begin{vwcol},\\vwcolsetup": [
      "widths={${1:ratio1,ratio2,...}}",
      "sep=${1:factor}",
      "sidesep",
      "presep",
      "postsep",
      "rule=",
      "siderule",
      "prerule",
      "postrule",
      "rulecolor=",
      "justify=${1|ragged,flush,raggedleft,center|}",
      "indent=",
      "quiet",
      "lines=${1:integer}",
      "maxrecursion=${1:integer}"
    ]
  },
  "args": [
    "\\usepackage/vwcol#c"
  ]
}