{
  "deps": [],
  "macros": [
    {
      "name": "sepfootnotecontent",
      "arg": {
        "format": "{}{}",
        "snippet": "sepfootnotecontent{${1:key%specialDef}}{${2:content%text}}"
      }
    },
    {
      "name": "sepfootnote",
      "arg": {
        "format": "{}",
        "snippet": "sepfootnote{${1:key%keyvals}}",
        "keys": [
          "\\sepfootnote#c,\\sepfootnotemark#c,\\sepfootnotetext#c,\\printsepfootnote#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "sepfootnotemark",
      "arg": {
        "format": "{}",
        "snippet": "sepfootnotemark{${1:key%keyvals}}",
        "keys": [
          "\\sepfootnote#c,\\sepfootnotemark#c,\\sepfootnotetext#c,\\printsepfootnote#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "sepfootnotetext",
      "arg": {
        "format": "{}",
        "snippet": "sepfootnotetext{${1:key%keyvals}}",
        "keys": [
          "\\sepfootnote#c,\\sepfootnotemark#c,\\sepfootnotetext#c,\\printsepfootnote#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "printsepfootnote",
      "arg": {
        "format": "{}",
        "snippet": "printsepfootnote{${1:key%keyvals}}",
        "keys": [
          "\\sepfootnote#c,\\sepfootnotemark#c,\\sepfootnotetext#c,\\printsepfootnote#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "sepfootquicknote",
      "arg": {
        "format": "{}",
        "snippet": "sepfootquicknote{${1:content%text}}"
      }
    },
    {
      "name": "newfootnotes",
      "arg": {
        "format": "{}",
        "snippet": "newfootnotes{${1:prefix}}"
      }
    },
    {
      "name": "newfootnotes*",
      "arg": {
        "format": "{}",
        "snippet": "newfootnotes*{${1:prefix}}"
      }
    },
    {
      "name": "newsymbolfootnotes",
      "arg": {
        "format": "{}",
        "snippet": "newsymbolfootnotes{${1:prefix}}"
      }
    },
    {
      "name": "newsymbolfootnotes",
      "arg": {
        "format": "[]{}",
        "snippet": "newsymbolfootnotes[${2:counter}]{${1:prefix}}"
      }
    },
    {
      "name": "newendnotes",
      "arg": {
        "format": "{}",
        "snippet": "newendnotes{${1:prefix}}"
      }
    },
    {
      "name": "newcommentnotes",
      "arg": {
        "format": "{}",
        "snippet": "newcommentnotes{${1:prefix}}"
      }
    },
    {
      "name": "newsymbolcommentnotes",
      "arg": {
        "format": "{}",
        "snippet": "newsymbolcommentnotes{${1:prefix}}"
      }
    },
    {
      "name": "newsymbolcommentnotes",
      "arg": {
        "format": "[]{}",
        "snippet": "newsymbolcommentnotes[${2:counter}]{${1:prefix}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/sepfootnotes#c": [
      "quiet",
      "warn",
      "error",
      "global",
      "after"
    ],
    "\\sepfootnote#c,\\sepfootnotemark#c,\\sepfootnotetext#c,\\printsepfootnote#c": [
      "%sepfootnotekey"
    ]
  },
  "args": [
    "\\usepackage/sepfootnotes#c"
  ]
}