{
  "deps": [
    {
      "name": "keyval"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "fancyvrb"
    },
    {
      "name": "color"
    },
    {
      "name": "listings"
    }
  ],
  "macros": [
    {
      "name": "checklistings",
      "arg": {
        "format": "{}",
        "snippet": "checklistings{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/checklistings#c,\\checklistings"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "chklistingcmd"
    },
    {
      "name": "chklistingmsg",
      "arg": {
        "format": "{}",
        "snippet": "chklistingmsg{${1:label}}"
      }
    },
    {
      "name": "chklistingerr",
      "arg": {
        "format": "{}",
        "snippet": "chklistingerr{${1:label}}"
      }
    },
    {
      "name": "ChkListingErr",
      "unusual": true
    },
    {
      "name": "chklistingfalse",
      "unusual": true
    },
    {
      "name": "chklistingfile",
      "unusual": true
    },
    {
      "name": "ChkListingMsg",
      "unusual": true
    },
    {
      "name": "chklistingtrue",
      "unusual": true
    },
    {
      "name": "ifchklisting",
      "unusual": true
    },
    {
      "name": "setchklistingcmd",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "setchklistingcmd{${1:command}}"
      }
    },
    {
      "name": "thechklisting",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "chklisting"
    },
    {
      "name": "chklisting",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{chklisting}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ChkListingErr",
      "unusual": true
    },
    {
      "name": "ChkListingMsg",
      "unusual": true
    }
  ],
  "keys": {
    "\\usepackage/checklistings#c,\\checklistings": [
      "withresult",
      "withoutresult",
      "showproblems",
      "hideproblems",
      "skipone",
      "skiptwo",
      "codestyle={${1:fancyvrb options}}",
      "msgstyle={${1:fancyvrb options}}",
      "errstyle={${1:fancyvrb options}}",
      "codelst={${1:listings options}}",
      "msglst={${1:listings options}}",
      "errlst={${1:listings options}}",
      "emptyoption=${1:string}",
      "subdir=${1:directory}",
      "prefix=${1:prefix}",
      "ext=${1:extension}",
      "prompt=${1:prompt}",
      "compiler=${1:path}",
      "compilerflags=${1:flags}",
      "lastflags=${1:flags}",
      "includecmd=${1:command}",
      "html=${1:path}"
    ],
    "\\begin{chklisting}": [
      "continue",
      "include=${1:label}",
      "fail",
      "label=${1:label}",
      "skipnone",
      "skipone",
      "skiptwo",
      "hide",
      "withresult",
      "withoutresult",
      "showproblems",
      "hideproblems"
    ]
  },
  "args": [
    "\\usepackage/checklistings#c,\\checklistings"
  ]
}