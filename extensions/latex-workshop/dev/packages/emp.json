{
  "deps": [
    {
      "name": "graphics"
    },
    {
      "name": "verbatim"
    }
  ],
  "macros": [
    {
      "name": "empuse",
      "arg": {
        "format": "{}",
        "snippet": "empuse{${1:name}}"
      }
    },
    {
      "name": "empTeX",
      "arg": {
        "format": "{}",
        "snippet": "empTeX{${1:LaTeX prelude%definition}}"
      }
    },
    {
      "name": "empaddtoTeX",
      "arg": {
        "format": "{}",
        "snippet": "empaddtoTeX{${1:LaTeX code%definition}}"
      }
    },
    {
      "name": "empprelude",
      "arg": {
        "format": "{}",
        "snippet": "empprelude{${1:METAPOST prelude%definition}}"
      }
    },
    {
      "name": "empaddtoprelude",
      "arg": {
        "format": "{}",
        "snippet": "empaddtoprelude{${1:METAPOST code%definition}}"
      }
    },
    {
      "name": "empwrite",
      "arg": {
        "format": "{}",
        "snippet": "empwrite{${1:METAPOST code%definition}}"
      }
    },
    {
      "name": "theempfig",
      "unusual": true
    },
    {
      "name": "theempfile",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "empfile"
    },
    {
      "name": "empfile",
      "arg": {
        "format": "[]",
        "snippet": "[${1:METAPOST file%file}]"
      }
    },
    {
      "name": "emp",
      "arg": {
        "format": "()",
        "snippet": "(${1:width},${2:height})"
      }
    },
    {
      "name": "emp",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:name}](${2:width},${3:height})"
      }
    },
    {
      "name": "empdef",
      "arg": {
        "format": "()",
        "snippet": "(${1:width},${2:height})"
      }
    },
    {
      "name": "empdef",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:name}](${2:width},${3:height})"
      }
    },
    {
      "name": "empcmds"
    },
    {
      "name": "empgraph",
      "arg": {
        "format": "()",
        "snippet": "(${1:width},${2:height})"
      }
    },
    {
      "name": "empgraph",
      "arg": {
        "format": "[]()",
        "snippet": "[${1:name}](${2:width},${3:height})"
      }
    }
  ],
  "keys": {
    "\\usepackage/emp#c": [
      "unknownkeysallowed",
      "draft",
      "final",
      "hiresbb",
      "demo",
      "setpagesize",
      "nosetpagesize",
      "dvips",
      "xdvi",
      "dvipdf",
      "dvipdfm",
      "dvipdfmx",
      "xetex",
      "pdftex",
      "luatex",
      "dvisvgm",
      "dvipsone",
      "dviwindo",
      "emtex",
      "dviwin",
      "oztex",
      "textures",
      "pctexps",
      "pctexwin",
      "pctexhp",
      "pctex32",
      "truetex",
      "tcidvi",
      "vtex",
      "debugshow",
      "hiderotate",
      "hidescale"
    ]
  },
  "args": [
    "\\usepackage/emp#c"
  ]
}