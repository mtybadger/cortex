{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-grad"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "PstLens",
      "arg": {
        "format": "{}",
        "snippet": "PstLens{${1:stuff}}"
      }
    },
    {
      "name": "PstLens",
      "arg": {
        "format": "(){}",
        "snippet": "PstLens(${2:x},${3:y}){${1:stuff}}"
      }
    },
    {
      "name": "PstLens",
      "arg": {
        "format": "[]{}",
        "snippet": "PstLens[${2:options%keyvals}]{${1:stuff}}",
        "keys": [
          "\\PstLens"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "PstLens",
      "arg": {
        "format": "[](){}",
        "snippet": "PstLens[${2:options%keyvals}](${3:x},${4:y}){${1:stuff}}",
        "keys": [
          "\\PstLens"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "PstLensShape",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\PstLens": [
      "LensHandle",
      "LensStyleHandle=${1:style}",
      "LensHandleWidth=",
      "LensHandleHeight=",
      "LensShadow",
      "LensStyleGlass=${1:style}",
      "LensSize=",
      "LensMagnification=${1:factor}",
      "LensRotation=${1:degrees}"
    ]
  },
  "args": []
}