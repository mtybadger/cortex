{
  "deps": [
    {
      "name": "graphicx"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "subcaption"
    },
    {
      "name": "enumitem"
    },
    {
      "name": "verbatim"
    },
    {
      "name": "ulem"
    },
    {
      "name": "mdframed"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "set",
      "arg": {
        "format": "{}{}",
        "snippet": "set{${1:attribute%specialDef}}{${2:value}}"
      }
    },
    {
      "name": "get",
      "arg": {
        "format": "{}",
        "snippet": "get{${1:attribute%keyvals}}",
        "keys": [
          "\\get#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "mn",
      "arg": {
        "format": "{}",
        "snippet": "mn{${1:keyvals}}",
        "keys": [
          "\\mn#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "alt",
      "arg": {
        "format": "{}",
        "snippet": "alt{${1:term%text}}"
      }
    },
    {
      "name": "deprecated",
      "arg": {
        "format": "{}",
        "snippet": "deprecated{${1:term%text}}"
      }
    },
    {
      "name": "domain",
      "arg": {
        "format": "{}",
        "snippet": "domain{${1:domain%text}}"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "{}",
        "snippet": "mncite{${1:key}}"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "[]{}",
        "snippet": "mncite[${2:add. text%text}]{${1:key}}"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "{}[]",
        "snippet": "mncite{${1:key}}[${2:localities}]"
      }
    },
    {
      "name": "mncite",
      "arg": {
        "format": "[]{}[]",
        "snippet": "mncite[${2:add. text%text}]{${1:key}}[${3:localities}]"
      }
    }
  ],
  "envs": [
    {
      "name": "example"
    },
    {
      "name": "note"
    },
    {
      "name": "source"
    },
    {
      "name": "key"
    },
    {
      "name": "requirement"
    },
    {
      "name": "recommendation"
    },
    {
      "name": "permission"
    },
    {
      "name": "specification"
    },
    {
      "name": "verification"
    },
    {
      "name": "import"
    },
    {
      "name": "tip"
    },
    {
      "name": "important"
    },
    {
      "name": "caution"
    },
    {
      "name": "warning"
    }
  ],
  "keys": {
    "\\get#c": [
      "%metanormaattribute"
    ],
    "\\mn#c": [
      "heading=${1:heading}",
      "level=${1:level}",
      "obligation=${1|informative,normative|}",
      "language=${1:lang code}",
      "script=${1:script name}",
      "reviewer=\"${1:name}\"",
      "from=${1:anchor}",
      "to=${1:anchor}",
      "date=${1:date}"
    ]
  },
  "args": []
}