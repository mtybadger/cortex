{
  "deps": [
    {
      "name": "vrbexin"
    },
    {
      "name": "verbatim"
    },
    {
      "name": "here"
    }
  ],
  "macros": [
    {
      "name": "VautoSubF",
      "arg": {
        "format": "{}{}{}{}{}",
        "snippet": "VautoSubF{${1:startline}}{${2:endline}}{${3:file}}{${4:caption}}{${5:label}}"
      }
    },
    {
      "name": "VautoSubF",
      "arg": {
        "format": "[]{}{}{}{}{}",
        "snippet": "VautoSubF[${6:lines-numbered}]{${1:startline}}{${2:endline}}{${3:file}}{${4:caption}}{${5:label}}"
      }
    },
    {
      "name": "VautoSfFont",
      "arg": {
        "format": "{}",
        "snippet": "VautoSfFont{${1:font commands}}"
      }
    },
    {
      "name": "VautoSfFont",
      "arg": {
        "format": "[]{}",
        "snippet": "VautoSfFont[${2:linenum-font-commands}]{${1:font-commands}}"
      }
    },
    {
      "name": "VautoPl",
      "arg": {
        "format": "{}",
        "snippet": "VautoPl{${1:placement}}"
      }
    },
    {
      "name": "VautoLines",
      "arg": {
        "format": "{}",
        "snippet": "VautoLines{${1:lines-per-page}}"
      }
    },
    {
      "name": "VautoLines",
      "arg": {
        "format": "[]{}",
        "snippet": "VautoLines[${2:first-page-lines}]{${1:lines-per-page}}"
      }
    },
    {
      "name": "VautoSubFF",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "VautoSubFF{${1:font commands}}"
      }
    },
    {
      "name": "VautoSubFF",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "VautoSubFF[${2:linenum-font-commands}]{${1:font-commands}}"
      }
    },
    {
      "name": "ifsubstr",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "ifsubstr{${1:string}}{${2:substring}}{${3:true}}{${4:false}}"
      }
    }
  ],
  "envs": [],
  "keys": {},
  "args": []
}