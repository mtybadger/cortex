      "name": "yesnumber"
    },
    {
      "name": "eqnnum",
      "arg": {
        "format": "{}",
        "snippet": "eqnnum{${1:number}}"
      }
    }
  ],
  "envs": [
    {
      "name": "equationarray",
      "arg": {
