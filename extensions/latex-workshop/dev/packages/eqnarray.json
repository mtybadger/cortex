      }
    },
    {
      "name": "equationarray*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:alignment%keyvals}]{${1:preamble}}",
        "keys": [
          "\\begin{equationarray},\\begin{equationarray*}"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\begin{equationarray},\\begin{equationarray*}": [
      "l",
      "c",
      "r"
    ]
  },
  "args": []
}