{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "cprotect"
    },
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "ifinclude",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "ifinclude{${1:content%text}}"
      }
    },
    {
      "name": "ifshow",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "ifshow{${1:content%text}}"
      }
    },
    {
      "name": "framefor",
      "arg": {
        "format": "{}{}",
        "snippet": "framefor{${1:audience}}{${2:content%text}}"
      }
    },
    {
      "name": "justfor",
      "arg": {
        "format": "{}{}",
        "snippet": "justfor{${1:audience}}{${2:content%text}}"
      }
    },
    {
      "name": "showcontentfor",
      "arg": {
        "format": "{}{}",
        "snippet": "showcontentfor{${1:audience}}{${2:content%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/beameraudience#c": [
      "audience=${1:keyword}"
    ]
  },
  "args": [
    "\\usepackage/beameraudience#c"
  ]
}