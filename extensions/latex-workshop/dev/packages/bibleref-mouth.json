    {
      "name": "thebookname",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "thebookname{${1:style name}}{${2:passage}}"
      }
    },
    {
      "name": "providebiblestyle",
      "arg": {
        "format": "{}{}",
        "snippet": "providebiblestyle{${1:style name}}{${2:formatting}}"
      }
    },
    {
      "name": "providebiblebookalias",
      "arg": {
        "format": "{}{}",
        "snippet": "providebiblebookalias{${1:book}}{${2:alias}}"
      }
    },
    {
      "name": "providebiblebook",
      "arg": {
        "format": "{}{}{}",
        "snippet": "providebiblebook{${1:style name}}{${2:book}}{${3:bookname}}"
