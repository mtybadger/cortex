        "format": "{}{}{}{}",
        "snippet": "ifsamechapter{${1:left}}{${2:right}}{${3:truehandler}}{${4:falsehandler}}"
      }
    },
    {
      "name": "ifsameverse",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "ifsameverse{${1:left}}{${2:right}}{${3:truehandler}}{${4:falsehandler}}"
      }
    },
    {
      "name": "ifhasbook",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "ifhasbook{${1:passage}}{${2:truehandler}}{${3:falsehandler}}"
      }
    },
    {
      "name": "ifhaschapter",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "ifhaschapter{${1:passage}}{${2:truehandler}}{${3:falsehandler}}"
      }
