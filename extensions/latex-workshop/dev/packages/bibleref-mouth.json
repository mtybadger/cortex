      }
    },
    {
      "name": "providebiblegatewayurl",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "providebiblegatewayurl{${1:url style name}}{${2:version}}"
      }
    },
    {
      "name": "providebiblegatewaystyle",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "providebiblegatewaystyle{${1:style name}}{${2:url style name}}{${3:text style name}}"
      }
    }
