    {
      "name": "fmtcount"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "bibleref",
      "arg": {
        "format": "{}",
        "snippet": "bibleref{${1:reference}}"
      }
    },
    {
      "name": "setbiblestyle",
      "arg": {
        "format": "{}",
        "snippet": "setbiblestyle{${1:style name%keyvals}}",
        "keys": [
          "\\setbiblestyle#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "thebook",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "thebook{${1:passage}}"
      }
    },
    {
      "name": "thechapter",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "thechapter{${1:passage}}"
      }
    },
    {
      "name": "theverse",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "theverse{${1:passage}}"
      }
    },
    {
      "name": "bookchapterseparator",
