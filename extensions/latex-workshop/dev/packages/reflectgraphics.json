{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "keyval"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "calc"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibraryfadings"
    }
  ],
  "macros": [
    {
      "name": "reflectgraphics",
      "arg": {
        "format": "{}",
        "snippet": "reflectgraphics{${1:imagefile}}"
      }
    },
    {
      "name": "reflectgraphics",
      "arg": {
        "format": "[]{}",
        "snippet": "reflectgraphics[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\reflectgraphics"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\reflectgraphics": [
      "width=",
      "height=",
      "scale=${1:factor}",
      "angle=${1:degrees}",
      "trim=${1:<left> <bot> <up> <right>}",
      "distance=",
      "length=${1:percent}",
      "opacity=${1:percent}"
    ]
  },
  "args": []
}