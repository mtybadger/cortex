{
  "deps": [
    {
      "name": "amstext"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "xtemplate"
    }
  ],
  "macros": [
    {
      "name": "sfrac",
      "arg": {
        "format": "{}{}",
        "snippet": "sfrac{${1:num}}{${2:denom}}"
      }
    },
    {
      "name": "sfrac",
      "arg": {
        "format": "{}[]{}",
        "snippet": "sfrac{${1:num}}[${3:sep}]{${2:denom}}"
      }
    },
    {
      "name": "sfrac",
      "arg": {
        "format": "[]{}{}",
        "snippet": "sfrac[${3:instance}]{${1:num}}{${2:denom}}"
      }
    },
    {
      "name": "sfrac",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "sfrac[${3:instance}]{${1:num}}[${4:sep}]{${2:denom}}"
      }
    }
  ],
  "envs": [],
  "keys": {},
  "args": []
}