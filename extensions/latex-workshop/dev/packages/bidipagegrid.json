{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "atbegshi"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "bidipagegridsetup",
      "arg": {
        "format": "{}",
        "snippet": "bidipagegridsetup{${1:options%keyvals}}",
        "keys": [
          "\\bidipagegridsetup,\\usepackage/bidipagegrid#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "setLTRpagegrid"
    },
    {
      "name": "setRTLpagegrid"
    },
    {
      "name": "bidipagegridShipoutDoubleBegin",
      "unusual": true
    },
    {
      "name": "bidipagegridShipoutDoubleEnd",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\bidipagegridsetup,\\usepackage/bidipagegrid#c": [
      "enable",
      "disable",
      "left-top",
      "lt",
      "top-left",
      "tl",
      "tr",
      "top-right",
      "rt",
      "right-top",
      "left-bottom",
      "lb",
      "bottom-left",
      "bl",
      "br",
      "bottom-right",
      "rb",
      "right-bottom",
      "step=",
      "firstcolor=",
      "secondcolor=",
      "arrows",
      "arrowlength=",
      "double=${1|false,first,last,true|}",
      "foreground",
      "direction=${1|LTR,RTL|}"
    ]
  },
  "args": [
    "\\bidipagegridsetup,\\usepackage/bidipagegrid#c"
  ]
}