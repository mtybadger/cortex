{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "luacode"
    }
  ],
  "macros": [
    {
      "name": "luaModularMult",
      "arg": {
        "format": "{}",
        "snippet": "luaModularMult{${1:integer}}"
      }
    },
    {
      "name": "luaModularMult",
      "arg": {
        "format": "[]{}",
        "snippet": "luaModularMult[${2:options%keyvals}]{${1:integer}}",
        "keys": [
          "\\luaModularMult"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "luaModularAdd",
      "arg": {
        "format": "{}",
        "snippet": "luaModularAdd{${1:integer}}"
      }
    },
    {
      "name": "luaModularAdd",
      "arg": {
        "format": "[]{}",
        "snippet": "luaModularAdd[${2:options%keyvals}]{${1:integer}}",
        "keys": [
          "\\luaModularAdd"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\luaModularMult": [
      "multlabel=${1:text}",
      "headline=${1:command}",
      "midline=${1:command}"
    ],
    "\\luaModularAdd": [
      "addlabel=${1:text}",
      "headline=${1:command}",
      "midline=${1:command}"
    ]
  },
  "args": []
}