      }
    },
    {
      "name": "bargraphenv",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options%keyvals}]{${1:bgenv name}}",
        "keys": [
          "\\begin{bargraphenv}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "bargraph",
      "arg": {
        "format": "{}",
        "snippet": "{${1:bg name}}"
      }
