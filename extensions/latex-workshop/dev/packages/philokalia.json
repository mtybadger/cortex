      }
    },
    {
      "name": "phkl"
    },
    {
      "name": "textphlk",
      "arg": {
        "format": "{}",
        "snippet": "textphlk{${1:text}}"
      }
    },
    {
      "name": "dsubop"
    },
    {
      "name": "dUnit"
    },
    {
      "name": "dunit"
    },
    {
      "name": "dunknown"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/philokalia#c": [
      "global",
      "local"
    ]
  },
