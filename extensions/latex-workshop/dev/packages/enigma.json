{
  "deps": [
    {
      "name": "luatexbase"
    }
  ],
  "macros": [
    {
      "name": "defineenigma",
      "arg": {
        "format": "{}",
        "snippet": "defineenigma{${1:name}}"
      }
    },
    {
      "name": "setupenigma",
      "arg": {
        "format": "{}{}",
        "snippet": "setupenigma{${1:name}}{${2:keyvals}}",
        "keys": [
          "\\setupenigma"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "ifenigmaisrunningplain",
      "unusual": true
    },
    {
      "name": "enigmaisrunningplaintrue",
      "unusual": true
    },
    {
      "name": "enigmaisrunningplainfalse",
      "unusual": true
    },
    {
      "name": "luastringsep",
      "unusual": true
    },
    {
      "name": "enigmasetupcatcodes",
      "unusual": true
    },
    {
      "name": "escapecatcode",
      "unusual": true
    },
    {
      "name": "begingroupcatcode",
      "unusual": true
    },
    {
      "name": "endgroupcatcode",
      "unusual": true
    },
    {
      "name": "spacecatcode",
      "unusual": true
    },
    {
      "name": "lettercatcode",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\setupenigma": [
      "other_chars=${1:boolean}",
      "day_key=${1:string}",
      "rotor_setting=${1:string}",
      "spacing=${1:boolean}",
      "verbose=${1:integer}"
    ]
  },
  "args": []
}