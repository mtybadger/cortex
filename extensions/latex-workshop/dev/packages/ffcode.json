{
  "deps": [
    {
      "name": "pgfopts"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "listings"
    },
    {
      "name": "tcolorbox"
    }
  ],
  "macros": [
    {
      "name": "ff",
      "arg": {
        "format": "{}",
        "snippet": "ff{${1:code%definition}}"
      }
    }
  ],
  "envs": [
    {
      "name": "ffcode"
    },
    {
      "name": "ffcode",
      "arg": {
        "format": "[]",
        "snippet": "[${1:listings options}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/ffcode#c": [
      "noframes",
      "nobars",
      "nonumbers",
      "nocn",
      "bold",
      "sf"
    ]
  },
  "args": [
    "\\usepackage/ffcode#c"
  ]
}