      "fill blocks",
      "fill gray=${1:factor}",
      "contrast=${1:integer}",
      "text color=",
      "flag gray=${1:factor}",
      "line gray=${1:factor}",
      "num blocks",
      "num sep=",
      "num start=${1:integer}",
      "baseline=${1:l, c, or <integer>}"
