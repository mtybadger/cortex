{
  "deps": [
    {
      "name": "pgfopts"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibrary3d"
    },
    {
      "name": "sourcesanspro"
    }
  ],
  "macros": [
    {
      "name": "trigonset",
      "arg": {
        "format": "{}",
        "snippet": "trigonset{${1:options%keyvals}}",
        "keys": [
          "\\usetheme/trigon,\\trigonset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "headcol",
      "unusual": true
    },
    {
      "name": "txtcol",
      "unusual": true
    },
    {
      "name": "logbig",
      "unusual": true
    },
    {
      "name": "biglogo",
      "arg": {
        "format": "{}",
        "snippet": "biglogo{${1:imagefile}}"
      }
    },
    {
      "name": "biglogo",
      "arg": {
        "format": "[]{}",
        "snippet": "biglogo[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\biglogo,\\smalllogo"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "slidestyle",
      "unusual": true
    },
    {
      "name": "titlestyle",
      "unusual": true
    },
    {
      "name": "sectionstyle",
      "unusual": true
    },
    {
      "name": "leftTriangle",
      "arg": {
        "format": "{}{}",
        "snippet": "leftTriangle{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "rightTriangle",
      "arg": {
        "format": "{}{}",
        "snippet": "rightTriangle{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "topTriangle",
      "arg": {
        "format": "{}{}",
        "snippet": "topTriangle{${1:x}}{${2:y}}"
      }
    },
    {
      "name": "leftColorTriangle",
      "arg": {
        "format": "{}{}{}",
        "snippet": "leftColorTriangle{${1:color}}{${2:x}}{${3:y}}"
      }
    },
    {
      "name": "rightColorTriangle",
      "arg": {
        "format": "{}{}{}",
        "snippet": "rightColorTriangle{${1:color}}{${2:x}}{${3:y}}"
      }
    },
    {
      "name": "topColorTriangle",
      "arg": {
        "format": "{}{}{}",
        "snippet": "topColorTriangle{${1:color}}{${2:x}}{${3:y}}"
      }
    },
    {
      "name": "titleframe"
    },
    {
      "name": "sectionframe"
    },
    {
      "name": "logsmall",
      "unusual": true
    },
    {
      "name": "smalllogo",
      "arg": {
        "format": "{}",
        "snippet": "smalllogo{${1:imagefile}}"
      }
    },
    {
      "name": "smalllogo",
      "arg": {
        "format": "[]{}",
        "snippet": "smalllogo[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\biglogo,\\smalllogo"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usetheme/trigon,\\trigonset": [
      "background=${1|light,dark|}",
      "titleformat=${1|regular,smallcaps,allsmallcaps,allcaps|}",
      "usesourcefonts",
      "sectionpage=${1|none,simple|}",
      "titlestyle=${1|plain,style1,style2|}",
      "sectionstyle=${1|plain,style1,style2,style3|}",
      "slidestyle=${1|blank,style1,cyber|}",
      "numbering=${1|none,counter,fraction|}",
      "block=${1|transparent,fill|}",
      "headingcolor=${1|default,theme|}",
      "textgcolor=${1|default,theme|}",
      "titleformat title=${1|regular,smallcaps,allsmallcaps,allcaps|}",
      "titleformat subtitle=${1|regular,smallcaps,allsmallcaps,allcaps|}",
      "titleformat section=${1|regular,smallcaps,allsmallcaps,allcaps|}",
      "titleformat frame=${1|regular,smallcaps,allsmallcaps,allcaps|}"
    ],
    "\\biglogo,\\smalllogo": [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox|}",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ]
  },
  "args": []
}