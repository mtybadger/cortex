    },
    {
      "name": "scripturestyle",
      "arg": {
        "format": "{}{}",
        "snippet": "scripturestyle{${1:name}}{${2:options%keyvals}}",
        "keys": [
          "\\usepackage/scripture#c,\\scripturesetup,\\scripturestyle,\\scripturestyle*,\\textscripture,\\begin{scripture}"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "scripturestyle*",
      "arg": {
        "format": "{}{}",
        "snippet": "scripturestyle*{${1:name}}{${2:options%keyvals}}",
        "keys": [
          "\\usepackage/scripture#c,\\scripturesetup,\\scripturestyle,\\scripturestyle*,\\textscripture,\\begin{scripture}"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "selah"
    },
    {
      "name": "scripturesetup",
      "arg": {
        "format": "{}",
        "snippet": "scripturesetup{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/scripture#c,\\scripturesetup,\\scripturestyle,\\scripturestyle*,\\textscripture,\\begin{scripture}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "textright",
      "arg": {
        "format": "{}",
        "snippet": "textright{${1:text}}"
