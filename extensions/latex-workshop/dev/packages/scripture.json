        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{center},\\begin{flushright},\\begin{hanging},\\begin{narrow},\\begin{poetry}",
          "\\begin{poetry}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "scripture"
    },
    {
      "name": "scripture",
      "arg": {
        "format": "[]",
        "snippet": "[${1:reference}]"
