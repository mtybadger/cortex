{
  "deps": [],
  "macros": [
    {
      "name": "ch",
      "arg": {
        "format": "{}",
        "snippet": "ch{${1:chapter number}}"
      }
    },
    {
      "name": "ch*",
      "arg": {
        "format": "{}",
        "snippet": "ch*{${1:chapter number}}"
      }
    },
    {
      "name": "extraskip"
    },
    {
