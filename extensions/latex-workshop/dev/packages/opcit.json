{
  "deps": [
    {
      "name": "xspace"
    },
    {
      "name": "hyperref",
      "if": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "bibcase",
      "arg": {
        "format": "{}",
        "snippet": "bibcase{${1:word}}"
      }
    },
    {
      "name": "bibhereafter",
      "arg": {
        "format": "{}",
        "snippet": "bibhereafter{${1:style}}"
      }
    },
    {
      "name": "biblastnames",
      "arg": {
        "format": "{}",
        "snippet": "biblastnames{${1:last names}}"
      }
    },
    {
      "name": "bibpunctuation",
      "unusual": true
    },
    {
      "name": "bibref",
      "arg": {
        "format": "{}",
        "snippet": "bibref{${1:bibid}}"
      }
    },
    {
      "name": "cite*",
      "arg": {
        "format": "{}",
        "snippet": "cite*{${1:bibid}}"
      }
    },
    {
      "name": "cited",
      "arg": {
        "format": "{}",
        "snippet": "cited{${1:keylist}}"
      }
    },
    {
      "name": "hereafter"
    },
    {
      "name": "idemtext",
      "unusual": true
    },
    {
      "name": "newBibCommand",
      "arg": {
        "format": "{}{}{}",
        "snippet": "newBibCommand{${1:command}}{${2:footnote style}}{${3:list style}}"
      }
    },
    {
      "name": "nobibliography",
      "arg": {
        "format": "{}",
        "snippet": "nobibliography{${1:bib file}}"
      }
    },
    {
      "name": "opcitends"
    },
    {
      "name": "opcitstart"
    },
    {
      "name": "opcittext",
      "unusual": true
    },
    {
      "name": "opcitwarning",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "opcitwarning{${1:warning%text}}"
      }
    },
    {
      "name": "QuoteOrNot",
      "unusual": true
    },
    {
      "name": "resetcites"
    },
    {
      "name": "sameauthors"
    },
    {
      "name": "sameauthorsrule",
      "unusual": true
    },
    {
      "name": "toomit",
      "arg": {
        "format": "{}",
        "snippet": "toomit{${1:text}}"
      }
    },
    {
      "name": "with",
      "arg": {
        "format": "{}",
        "snippet": "with{${1:field}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/opcit#c": [
      "custombst",
      "nocustombst",
      "hyperref",
      "nohyperref",
      "omitting",
      "noomitting",
      "idem",
      "noidem",
      "checkidem",
      "nocheckidem"
    ]
  },
  "args": [
    "\\usepackage/opcit#c"
  ]
}