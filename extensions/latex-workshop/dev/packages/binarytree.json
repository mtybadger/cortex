{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "BinaryTree",
      "arg": {
        "format": "[]{}{}",
        "snippet": "BinaryTree[${3:options%keyvals}]{${1:path spec}}{${2:depth}}",
        "keys": [
          "\\BinaryTree#c,\\btreeset#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "btreeset",
      "arg": {
        "format": "{}",
        "snippet": "btreeset{${1:options%keyvals}}",
        "keys": [
          "\\BinaryTree#c,\\btreeset#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "btreesetexternal",
      "arg": {
        "format": "{}",
        "snippet": "btreesetexternal{${1:options%keyvals}}",
        "keys": [
          "\\btreesetexternal#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\BinaryTree#c,\\btreeset#c": [
      "default",
      "grow=${1|up,down,left,right,none|}",
      "root label anchor=",
      "left label anchor=",
      "right label anchor=",
      "final label anchor=",
      "root edge",
      "draw missing",
      "label on every edge",
      "math labels",
      "continue at path end",
      "continue after turn",
      "default color=",
      "default color after turn",
      "xscale=",
      "yscale=",
      "scale=",
      "label distance=",
      "sibling distance=",
      "level distance=",
      "sibling distance scales",
      "level distance scales",
      "top padding=",
      "bottom padding=",
      "left padding=",
      "right padding=",
      "framed",
      "separate",
      "external",
      "external/use automatic file name",
      "external/file name=${1:file}"
    ],
    "\\btreesetexternal#c": [
      "use automatic file name"
    ]
  },
  "args": []
}