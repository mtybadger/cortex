{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "DraftwatermarkOptions",
      "arg": {
        "format": "{}",
        "snippet": "DraftwatermarkOptions{${1:options%keyvals}}",
        "keys": [
          "\\DraftwatermarkOptions,\\usepackage/draftwatermark#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "DraftwatermarkStdMark"
    }
  ],
  "envs": [],
  "keys": {
    "\\DraftwatermarkOptions,\\usepackage/draftwatermark#c": [
      "firstpageonly",
      "firstpage",
      "stamp",
      "nostamp",
      "final",
      "angle=${1:degrees}",
      "scale=${1:factor}",
      "fontsize=",
      "text=${1:text}",
      "hpos=",
      "vpos=",
      "pos={${1:hpos,vpos}}",
      "hanchor=${1|l,c,r|}",
      "vanchor=${1|t,m,b|}",
      "anchor=${1:string}",
      "alignment=${1|l,c,r|}",
      "colormodel=${1:color model}",
      "colorspec=${1:color spec}",
      "color=${1:color}",
      "color={[${1:model}]{${2:spec}}}",
      "markcmd=${1:macro}"
    ]
  },
  "args": [
    "\\DraftwatermarkOptions,\\usepackage/draftwatermark#c"
  ]
}