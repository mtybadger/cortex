{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psMarble",
      "arg": {
        "format": "()",
        "snippet": "psMarble(${1:width},${2:height})"
      }
    },
    {
      "name": "psMarble",
      "arg": {
        "format": "()()",
        "snippet": "psMarble(${1:x0},${2:y0})(${3:x1},${4:y1})"
      }
    },
    {
      "name": "psMarble",
      "arg": {
        "format": "[]()",
        "snippet": "psMarble[${1:options%keyvals}](${2:width},${3:height})",
        "keys": [
          "\\psMarble#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "psMarble",
      "arg": {
        "format": "[]()()",
        "snippet": "psMarble[${1:options%keyvals}](${2:x0},${3:y0})(${4:x1},${5:y1})",
        "keys": [
          "\\psMarble#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psMarble#c": [
      "actions={${1:marbling actions}}",
      "spractions={${1:spray actions}}",
      "shadings={${1:shading actions}}",
      "background={[${1:r g b}]}",
      "paper={[${1:r g b}]}",
      "drawcontours",
      "seed=${1:seed}",
      "oversample=${1:number}",
      "overscan=${1:number}",
      "bckg",
      "colors={[${1:r1 g1 b1}] ${2:[r2 b2 g2] ...}}",
      "colors={(${1:RrGgBb}) ${2:(RrGgBb) ...}}",
      "viscosity=${1:number}",
      "linewidth=",
      "linecolor=",
      "blendmode=${1:mode number}",
      "shapealpha=${1:number}",
      "opacity=${1:factor}"
    ]
  },
  "args": []
}