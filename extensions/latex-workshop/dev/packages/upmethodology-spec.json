{
  "deps": [
    {
      "name": "upmethodology-p-common"
    },
    {
      "name": "ulem"
    },
    {
      "name": "upmethodology-fmt"
    },
    {
      "name": "upmethodology-code"
    }
  ],
  "macros": [
    {
      "name": "speccons",
      "arg": {
        "format": "{}{}",
        "snippet": "speccons{${1:name}}{${2:prototype}}"
      }
    },
    {
      "name": "speccons",
      "arg": {
        "format": "[]{}{}",
        "snippet": "speccons[${3:modifiers}]{${1:name}}{${2:prototype}}"
      }
    },
    {
      "name": "specget",
      "arg": {
        "format": "{}{}",
        "snippet": "specget{${1:return type}}{${2:name}}"
      }
    },
    {
      "name": "specget",
      "arg": {
        "format": "[]{}{}",
        "snippet": "specget[${3:modifiers}]{${1:return type}}{${2:name}}"
      }
    },
    {
      "name": "specset",
      "arg": {
        "format": "{}{}",
        "snippet": "specset{${1:name}}{${2:prototype}}"
      }
    },
    {
      "name": "specset",
      "arg": {
        "format": "[]{}{}",
        "snippet": "specset[${3:modifiers}]{${1:name}}{${2:prototype}}"
      }
    },
    {
      "name": "specfunc",
      "arg": {
        "format": "{}{}{}",
        "snippet": "specfunc{${1:return type}}{${2:name}}{${3:prototype}}"
      }
    },
    {
      "name": "specfunc",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "specfunc[${4:modifiers}]{${1:return type}}{${2:name}}{${3:prototype}}"
      }
    },
    {
      "name": "specreturn",
      "arg": {
        "format": "{}",
        "snippet": "specreturn{${1:description%text}}"
      }
    },
    {
      "name": "specglobalreturn",
      "arg": {
        "format": "{}",
        "snippet": "specglobalreturn{${1:description%text}}"
      }
    },
    {
      "name": "specparam",
      "arg": {
        "format": "{}{}",
        "snippet": "specparam{${1:name}}{${2:description%text}}"
      }
    },
    {
      "name": "specparam",
      "arg": {
        "format": "[]{}{}",
        "snippet": "specparam[${3:in|${4:inout}|out}]{${1:name}}{${2:description%text}}"
      }
    },
    {
      "name": "specendhline"
    },
    {
      "name": "specstarthline"
    }
  ],
  "envs": [
    {
      "name": "detailspec",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "detailspec",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:width}]{${1:title%text}}"
      }
    },
    {
      "name": "detailspec*",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "detailspec*",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:width}]{${1:title%text}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/upmethodology-spec#c": [
      "french",
      "francais",
      "english"
    ]
  },
  "args": [
    "\\usepackage/upmethodology-spec#c"
  ]
}