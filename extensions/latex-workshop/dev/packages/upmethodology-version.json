{
  "deps": [
    {
      "name": "upmethodology-p-common"
    },
    {
      "name": "upmethodology-fmt"
    }
  ],
  "macros": [
    {
      "name": "upmrestricted"
    },
    {
      "name": "upmvalidable"
    },
    {
      "name": "upmvalidated"
    },
    {
      "name": "upmpublic"
    },
    {
      "name": "upmdate",
      "arg": {
        "format": "{}",
        "snippet": "upmdate{${1:date}}"
      }
    },
    {
      "name": "upmdescription",
      "arg": {
        "format": "{}",
        "snippet": "upmdescription{${1:text}}"
      }
    },
    {
      "name": "upmstatus",
      "arg": {
        "format": "{}",
        "snippet": "upmstatus{${1:text}}"
      }
    },
    {
      "name": "theupmdate"
    },
    {
      "name": "theupmlastmodif"
    },
    {
      "name": "theupmstatus"
    },
    {
      "name": "updateversion",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "updateversion{${1:version}}{${2:date}}{${3:description%text}}{${4:status%text}}"
      }
    },
    {
      "name": "initialversion",
      "arg": {
        "format": "{}{}{}",
        "snippet": "initialversion{${1:date}}{${2:description%text}}{${3:status%text}}"
      }
    },
    {
      "name": "initialversion",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "initialversion[${4:version}]{${1:date}}{${2:description%text}}{${3:status%text}}"
      }
    },
    {
      "name": "incversion",
      "arg": {
        "format": "{}{}{}",
        "snippet": "incversion{${1:date}}{${2:description%text}}{${3:status%text}}"
      }
    },
    {
      "name": "incsubversion",
      "arg": {
        "format": "{}{}{}",
        "snippet": "incsubversion{${1:date}}{${2:description%text}}{${3:status%text}}"
      }
    },
    {
      "name": "theupmversion"
    },
    {
      "name": "upmhistory"
    },
    {
      "name": "upmhistory",
      "arg": {
        "format": "[]",
        "snippet": "upmhistory[${1:width}]"
      }
    },
    {
      "name": "upmcopyrightdate"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/upmethodology-version#c": [
      "french",
      "francais",
      "english"
    ]
  },
