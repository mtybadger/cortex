{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-node"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "psFidgetSpinner"
    },
    {
      "name": "psFidgetSpinner",
      "arg": {
        "format": "()",
        "snippet": "psFidgetSpinner(${1:x},${2:y})"
      }
    },
    {
      "name": "psFidgetSpinner",
      "arg": {
        "format": "[]",
        "snippet": "psFidgetSpinner[${1:options%keyvals}]",
        "keys": [
          "\\psFidgetSpinner#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "psFidgetSpinner",
      "arg": {
        "format": "[]()",
        "snippet": "psFidgetSpinner[${1:options%keyvals}](${2:x},${3:y})",
        "keys": [
          "\\psFidgetSpinner#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\psFidgetSpinner#c": [
      "R=${1:radius}",
      "color0=",
      "color1=",
      "color2=",
      "color3=",
      "colorMask=",
      "rotation=${1:degrees}",
      "mask",
      "customize",
      "customizeMask",
      "image=${1:imagefile}",
      "fillcolor=",
      "linecolor=",
      "linewidth="
    ]
  },
  "args": []
}