    {
      "name": "code*"
    },
    {
      "name": "invisible"
    },
    {
      "name": "example"
    },
    {
      "name": "example*"
    }
  ],
  "keys": {
    "\\documentclass/codedoc#c": [
      "autoclose",
      "index",
      "noheader",
      "obeystop",
      "tracing0",
      "tracing1",
      "tracing2",
      "${1:class name}"
    ],
    "\\NewExample,\\RenewExample": [
      "numbered",
      "continuous",
      "visibleEOL"
    ]
  },
  "args": [
    "\\documentclass/codedoc#c"
  ]
