{
  "deps": [
    {
      "name": "filehook"
    },
    {
      "name": "pgfkeys"
    }
  ],
  "macros": [
    {
      "name": "pgffilehook",
      "arg": {
        "format": "{}",
        "snippet": "pgffilehook{${1:keyvals}}",
        "keys": [
          "\\pgffilehook"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\pgffilehook": [
      "EveryFile",
      "Files",
      "File",
      "Inputs",
      "InputFile",
      "Includes",
      "IncludeFile",
      "ClassFile",
      "PackageFile",
      "AtBegin={${1:code}}",
      "AtEnd={${1:code}}"
    ]
  },
  "args": []
}