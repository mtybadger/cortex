  "macros": [
    {
      "name": "CVbg",
      "arg": {
        "format": "[]",
        "snippet": "CVbg[${1:options%keyvals}]",
        "keys": [
          "\\CVbg"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "infoLeft",
      "arg": {
        "format": "{}",
        "snippet": "infoLeft{${1:info%text}}"
      }
    },
    {
      "name": "infoRight",
      "arg": {
        "format": "{}",
        "snippet": "infoRight{${1:info%text}}"
      }
    },
    {
      "name": "CVmargins"
    },
    {
      "name": "CVtitle",
      "arg": {
        "format": "{}",
        "snippet": "CVtitle{${1:le titre%text}}"
      }
    },
    {
      "name": "CVtitle",
      "arg": {
        "format": "[]{}",
        "snippet": "CVtitle[${2:options%keyvals}]{${1:le titre%text}}",
        "keys": [
          "\\CVtitle"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "CVclearpage"
    },
    {
      "name": "CVh",
      "arg": {
        "format": "{}",
        "snippet": "CVh{${1:hauteur en cm}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\CVbg": [
      "modele=${1|andromede,architecte,centaure,dynamique,gaia,jupiter,mars,neptune,orion,pegase,pluton,saturne,univers,uranus,venus|}",
      "bgcolor=",
      "bordercolor=",
      "postesize=${1:size macro}",
      "postecolor=",
      "poste={${1:texte}}",
      "pic=${1:image file}",
      "borderpic=",
      "scalepic=${1:factor}"
    ],
    "\\CVtitle": [
      "color=",
      "bordercolor="
    ]
  },
  "args": []
}