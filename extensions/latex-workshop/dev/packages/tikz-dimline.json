{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "pgfplots"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "tikzlibrarycalc"
    },
    {
      "name": "tikzlibrarydecorations.markings"
    }
  ],
  "macros": [
    {
      "name": "dimline",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "dimline[${4:options%keyvals}]{${1:start}}{${2:end}}{${3:label}}",
        "keys": [
          "\\dimline"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\dimline": [
      "color=",
      "line style=",
      "label style=",
      "extension start length=",
      "extension end length=",
      "extension start angle=${1:degrees}",
      "extension end angle=${1:degrees}",
      "extension start style=",
      "extension end style=",
      "extension start path=",
      "extension end path=",
      "arrows=${1|dimline-dimline,dimline reverse-dimline reverse|}"
    ]
  },
  "args": []
}