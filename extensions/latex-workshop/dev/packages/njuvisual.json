{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "njuemblem",
      "arg": {
        "format": "{}{}",
        "snippet": "njuemblem{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuemblem",
      "arg": {
        "format": "[]{}{}",
        "snippet": "njuemblem[${3:选项%keyvals}]{${1:宽度}}{${2:高度}}",
        "keys": [
          "\\njuemblem,\\njuname,\\njuname*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "njuname",
      "arg": {
        "format": "{}{}",
        "snippet": "njuname{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuname",
      "arg": {
        "format": "[]{}{}",
        "snippet": "njuname[${3:选项%keyvals}]{${1:宽度}}{${2:高度}}",
        "keys": [
          "\\njuemblem,\\njuname,\\njuname*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "njuname*",
      "arg": {
        "format": "{}{}",
        "snippet": "njuname*{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuname*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "njuname*[${3:选项%keyvals}]{${1:宽度}}{${2:高度}}",
        "keys": [
          "\\njuemblem,\\njuname,\\njuname*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "njumotto",
      "arg": {
        "format": "{}{}",
        "snippet": "njumotto{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njumotto",
      "arg": {
        "format": "[]{}{}",
        "snippet": "njumotto[${3:color}]{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuspirit",
      "arg": {
        "format": "{}{}",
        "snippet": "njuspirit{${1:宽度}}{${2:高度}}"
      }
    },
    {
      "name": "njuspirit",
      "arg": {
        "format": "[]{}{}",
        "snippet": "njuspirit[${3:color}]{${1:宽度}}{${2:高度}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/njuvisual#c": [
      "force"
    ],
    "\\njuemblem,\\njuname,\\njuname*": [
      "color=",
      "color*=",
      "%color",
      "department=${1:院系名称}"
    ]
  },
  "args": [
    "\\usepackage/njuvisual#c"
  ]
}