{
  "deps": [
    {
      "name": "color",
      "if": "color"
    },
    {
      "name": "xcolor",
      "if": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "cbcolor",
      "if": "color",
      "arg": {
        "format": "{}",
        "snippet": "cbcolor{${1:color}}"
      }
    },
    {
      "name": "cbcolor",
      "if": "xcolor",
      "arg": {
        "format": "{}",
        "snippet": "cbcolor{${1:color}}"
      }
    },
    {
      "name": "cbstart"
    },
    {
      "name": "cbend"
    },
    {
      "name": "cbdelete"
    },
    {
      "name": "cbdelete",
      "arg": {
        "format": "[]",
        "snippet": "cbdelete[${1:width}]"
      }
    },
    {
      "name": "nochangebars"
    },
    {
      "name": "changebarwidth",
      "unusual": true
    },
    {
      "name": "deletebarwidth",
      "unusual": true
    },
    {
      "name": "changebarsep",
      "unusual": true
    },
    {
      "name": "thechangebargrey",
      "unusual": true
    },
    {
      "name": "driver",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "driver{${1:driver name}}"
      }
    }
  ],
  "envs": [
    {
      "name": "changebar"
    }
  ],
  "keys": {
    "\\usepackage/changebar#c": [
      "dvitoln03",
      "dvitops",
      "dvips",
      "emtex",
      "textures",
      "vtex",
      "pdftex",
      "xetex",
      "luatex",
      "outerbars",
      "innerbars",
      "leftbars",
      "rightbars",
      "traceon",
      "traceoff",
      "tracestacks",
      "grey",
      "color",
      "xcolor"
    ]
  },
  "args": [
    "\\usepackage/changebar#c"
  ]
}