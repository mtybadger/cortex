  "macros": [
    {
      "name": "alltikzpeople",
      "if": "demo",
      "arg": {
        "format": "{}{}",
        "snippet": "alltikzpeople{${1:width}}{${2:node options}}"
      }
    },
    {
      "name": "alltikzpeople*",
      "if": "demo",
      "arg": {
        "format": "{}{}",
        "snippet": "alltikzpeople*{${1:width}}{${2:node options}}"
      }
    },
    {
      "name": "tikzpeoplecolors",
