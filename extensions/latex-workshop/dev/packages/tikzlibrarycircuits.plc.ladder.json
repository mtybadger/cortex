{
  "deps": [
    {
      "name": "tikzlibrarycircuits"
    }
  ],
  "macros": [
    {
      "name": "ladderskip"
    },
    {
      "name": "ladderrungend",
      "arg": {
        "format": "{}",
        "snippet": "ladderrungend{${1:number}}"
      }
    },
    {
      "name": "ladderpowerrails"
    }
  ],
  "envs": [],
  "keys": {
    "\\begin{tikzpicture}#c,\\tikz#c,\\begin{scope}#c,\\tikzset#c,\\scoped#c": [
      "circuit plc ladder",
      "ladderrungsep=${1:number}"
    ],
    "\\node#c": [
      "symbol=${1:symbol}",
      "symbol color=",
      "clksize=",
      "input sep=",
      "output sep=",
      "inputs={${1:input1,input2,...}}",
      "outputs={${1:output1,output2,...}}"
    ]
  },
  "args": []
}