{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "jlreq-helpers"
    }
  ],
  "macros": [
    {
      "name": "jlreqtrimmarkssetup",
      "arg": {
        "format": "{}",
        "snippet": "jlreqtrimmarkssetup{${1:options%keyvals}}",
        "keys": [
          "\\jlreqtrimmarkssetup#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/jlreq-trimmarks#c": [
      "platex",
      "uplatex",
      "lualatex",
      "dvipdfmx",
      "dvips",
      "dviout",
      "trimmarks_paper=${1:paper type}",
      "trimmarks_paper={${1:width,height}}",
      "trimmarks_paper=${1:paper type}+{${2:width,height}}",
      "landscape",
      "show",
      "show={${1:trimmarks, banner, digital, and/or no}}",
      "bleed_margin={${1:top=<length>,bottom=<length>,gutter=<length>,fore-edge=<length>}}"
    ],
    "\\jlreqtrimmarkssetup#c": [
      "banner={${1:specs}}",
      "trimmarks_width=",
      "color=",
      "trimmarks_pattern={${1:options}}"
    ]
  },
  "args": [
    "\\usepackage/jlreq-trimmarks#c"
  ]
}