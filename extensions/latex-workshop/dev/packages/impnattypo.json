    {
      "name": "ifluatex"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "luatexbase"
    },
    {
      "name": "luacode"
    }
  ],
  "macros": [
    {
      "name": "usecolor",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "usecolor{${1:color}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/impnattypo#c": [
      "draft",
      "frenchchapters",
      "hyphenation",
      "nosingleletter",
      "parindent",
      "lastparline",
      "homeoarchy",
      "rivers",
      "homeoarchywordcolor=",
      "homeoarchycharcolor=",
      "nosinglelettercolor=",
      "lastparlinecolor=",
      "riverscolor=",
      "homeoarchymaxwords=${1:integer}",
      "homeoarchymaxchars=${1:integer}"
    ]
  },
  "args": [
    "\\usepackage/impnattypo#c"
  ]
}