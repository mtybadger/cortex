        "snippet": "{${1:layer name}}{${2:ocg id}}{${3:initial visibility}}"
      }
    },
    {
      "name": "ocg",
      "arg": {
        "format": "[]{}{}{}",
        "snippet": "[${4:options%keyvals}]{${1:layer name}}{${2:ocg id}}{${3:initial visibility}}",
        "keys": [
          "\\begin{ocg}"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "ocmd",
      "arg": {
        "format": "{}",
        "snippet": "{${1:visibility policy,visibility expression}}"
      }
    },
    {
      "name": "ocmd",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:id}]{${1:visibility policy,visibility expression}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/ocgx2#c": [
      "dvipdfmx",
      "viewocg=${1|always,never,ifvisible|}",
      "printocg=${1|always,never,ifvisible|}",
      "exportocg=${1|always,never,ifvisible|}",
      "showingui=${1|true,false,always,never,iffirstuse|}",
      "tikz",
      "ocgcolorlinks"
    ],
    "\\begin{ocg}": [
      "viewocg=${1|always,never,ifvisible|}",
      "printocg=${1|always,never,ifvisible|}",
      "exportocg=${1|always,never,ifvisible|}",
      "showingui=${1|true,false,always,never,iffirstuse|}",
      "listintoolbar=${1|true,false,always,never,iffirstuse|}",
      "radiobtngrps={${1:group1,group2,...}}"
    ],
    "\\switchocg,\\switchocg*,\\showocg,\\showocg*,\\hideocg,\\hideocg*,\\actionsocg,\\actionsocg*": [
      "onmousenter",
      "onmouseexit",
      "onmousedown",
      "onmouseup",
      "onmouseall"
    ],
    "\\tikzset#c,\\begin{tikzpicture}#c,\\begin{scope}#c,\\node#c": [
      "ocg={${1:options}}",
      "ocmd={${1:options}}",
      "trigger ocg=${1|onmousenter,onmouseexit,onmousedown,onmouseup,onmouseall|}",
      "switch ocg={${1:ocg list}}",
      "show ocg={${1:ocg list}}",
      "hide ocg={${1:ocg list}}",
      "actions ocg={${1:ocg list1}}{${2:ocg list2}}{${3:ocg list3}}",
      "switch ocg with mark on={${1:ocg ref}}{${2:ocg list}}",
      "switch ocg with mark off={${1:ocg ref}}{${2:ocg list}}",
      "ocg={${1:options}}",
      "ocmd={${1:options}}",
      "trigger ocg=${1|onmousenter,onmouseexit,onmousedown,onmouseup,onmouseall|}",
      "switch ocg={${1:ocg list}}",
      "show ocg={${1:ocg list}}",
      "hide ocg={${1:ocg list}}",
      "actions ocg={${1:ocg list1}}{${2:ocg list2}}{${3:ocg list3}}",
      "switch ocg with mark on={${1:ocg ref}}{${2:ocg list}}",
      "switch ocg with mark off={${1:ocg ref}}{${2:ocg list}}"
