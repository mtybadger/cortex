{
  "deps": [
    {
      "name": "array"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "ifdraft"
    },
    {
      "name": "iftex"
    },
    {
      "name": "ltxcmds"
    },
    {
      "name": "scrlfile"
    }
  ],
  "macros": [
    {
      "name": "IfDefined",
      "arg": {
        "format": "{}{}",
        "snippet": "IfDefined{${1:command}}{${2:code-defined}}"
      }
    },
    {
      "name": "IfUndefined",
      "arg": {
        "format": "{}{}",
        "snippet": "IfUndefined{${1:command}}{${2:code-undefined}}"
      }
    },
    {
      "name": "IfElseDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfElseDefined{${1:command}}{${2:code-defined}}{${3:code-undefined}}"
      }
    },
    {
      "name": "IfElseUndefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfElseUndefined{${1:command}}{${2:code-undefined}}{${3:code-defined}}"
      }
    },
    {
      "name": "IfMultDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfMultDefined{${1:list of commands%cmd}}{${2:code-defined}}{${3:code-undefined}}"
      }
    },
    {
      "name": "IfDraft",
      "arg": {
        "format": "{}",
        "snippet": "IfDraft{${1:draft-mode-active code}}"
      }
    },
    {
      "name": "IfNotDraft",
      "arg": {
        "format": "{}",
        "snippet": "IfNotDraft{${1:draft-mode-disabled code}}"
      }
    },
    {
      "name": "IfNotDraftElse",
      "arg": {
        "format": "{}{}",
        "snippet": "IfNotDraftElse{${1:draft-mode-active code}}{${2:draft-mode-disabled code}}"
      }
    },
    {
      "name": "IfPackageLoaded",
      "arg": {
        "format": "{}{}",
        "snippet": "IfPackageLoaded{${1:package}}{${2:is-loaded code}}"
      }
    },
    {
      "name": "IfPackageNotLoaded",
      "arg": {
        "format": "{}{}",
        "snippet": "IfPackageNotLoaded{${1:package}}{${2:is-not-loaded code}}"
      }
    },
    {
      "name": "IfPackagesLoaded",
      "arg": {
        "format": "{}{}",
        "snippet": "IfPackagesLoaded{${1:list of packages}}{${2:all-are-loaded code}}"
      }
    },
    {
      "name": "IfPackagesNotLoaded",
      "arg": {
        "format": "{}{}",
        "snippet": "IfPackagesNotLoaded{${1:list of packages}}{${2:none-are-loaded code}}"
      }
    },
    {
      "name": "IfElsePackageLoaded",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfElsePackageLoaded{${1:package}}{${2:is-loaded code}}{${3:is-not-loaded code}}"
      }
    },
    {
      "name": "ExecuteAfterPackage",
      "arg": {
        "format": "{}{}",
        "snippet": "ExecuteAfterPackage{${1:package}}{${2:code}}"
      }
    },
    {
      "name": "ExecuteBeforePackage",
      "arg": {
        "format": "{}{}",
        "snippet": "ExecuteBeforePackage{${1:package}}{${2:code}}"
      }
    },
    {
      "name": "IfTikzLibraryLoaded",
      "arg": {
        "format": "{}{}",
        "snippet": "IfTikzLibraryLoaded{${1:library}}{${2:if-loaded code}}"
      }
    },
    {
      "name": "IfColumntypeDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfColumntypeDefined{${1:column type}}{${2:code-defined}}{${3:code-undefined}}"
      }
    },
    {
      "name": "IfColumntypesDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfColumntypesDefined{${1:column type}}{${2:code-undefined}}{${3:code-defined}}"
      }
    },
    {
      "name": "IfColorDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfColorDefined{${1:color}}{${2:code-defined}}{${3:code-undefined}}"
      }
    },
    {
      "name": "IfColorsDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfColorsDefined{${1:color}}{${2:code-undefined}}{${3:code-defined}}"
      }
    },
    {
      "name": "IfMathVersionDefined",
      "arg": {
        "format": "{}{}{}",
        "snippet": "IfMathVersionDefined{${1:font version}}{${2:code-defined}}{${3:code-undefined}}"
      }
    },
    {
      "name": "IfGlossariesStyleDefined",
      "arg": {
        "format": "{}{}",
        "snippet": "IfGlossariesStyleDefined{${1:style name}}{${2:code-defined}}"
      }
    },
    {
      "name": "IfBibEnvironmentDefined",
      "arg": {
        "format": "{}{}",
        "snippet": "IfBibEnvironmentDefined{${1:envname}}{${2:code-defined}}"
      }
    },
    {
      "name": "SetTemplateDefinition",
      "arg": {
        "format": "{}{}{}",
        "snippet": "SetTemplateDefinition{${1:group}}{${2:property}}{${3:code}}"
      }
    },
    {
      "name": "UseDefinition",
      "arg": {
        "format": "{}{}",
        "snippet": "UseDefinition{${1:group}}{${2:property}}"
      }
    },
    {
      "name": "CheckIfColumntypeDefined",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "CheckIfColumntypeDefined{${1:column type}}"
      }
    },
    {
      "name": "isColumntypeDefined",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "isColumntypeDefined{${1:column type}}"
      }
    }
  ],
  "envs": [],
  "keys": {},
  "args": []
}