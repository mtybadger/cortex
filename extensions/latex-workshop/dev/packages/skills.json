    {
      "name": "skillcounter",
      "unusual": true
    },
    {
      "name": "skillsclosing",
      "unusual": true
    },
    {
      "name": "skillsopening",
      "unusual": true
    },
    {
      "name": "skill",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "skill{${1:label}}"
      }
    }
  ],
  "envs": [
    {
      "name": "skillquestions"
    },
    {
      "name": "skillquestions",
      "arg": {
        "format": "[]",
        "snippet": "[${1:skill1,skill2,...}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/skills#c": [
      "makenoidxglossaries",
      "donotmakenoidxglossaries",
      "counter=${1:counter}"
    ],
    "\\skillstable": [
      "word",
      "letter",
      "standard",
      "use",
      "def",
      "nocase",
      "case"
    ]
  },
  "args": [
    "\\usepackage/skills#c"
  ]
}