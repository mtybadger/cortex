    },
    {
      "name": "marginnote"
    }
  ],
  "macros": [
    {
      "name": "skilldef",
      "arg": {
        "format": "{}{}{}",
        "snippet": "skilldef{${1:label}}{${2:reference}}{${3:description%text}}"
      }
    },
    {
      "name": "skillquestion"
    },
    {
      "name": "skillquestion",
      "arg": {
        "format": "[]",
        "snippet": "skillquestion[${1:skill1,skill2,...}]"
      }
    },
    {
      "name": "skillquestion",
      "arg": {
        "format": "[][]",
        "snippet": "skillquestion[${1:skill1,skill2,...}][${2:points}]"
      }
    },
    {
      "name": "skills",
      "arg": {
        "format": "{}",
        "snippet": "skills{${1:skill1,skill2,...}}"
      }
    },
    {
      "name": "globalskills",
      "arg": {
        "format": "{}",
        "snippet": "globalskills{${1:skill1,skill2,...}}"
      }
    },
    {
      "name": "skillstable"
    },
    {
      "name": "skillstable",
      "arg": {
        "format": "[]",
        "snippet": "skillstable[${1:sort type%keyvals}]",
        "keys": [
          "\\skillstable"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "putglobalskills",
