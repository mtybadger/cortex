      "unusual": true
    },
    {
      "name": "defaultskillsclosing",
      "unusual": true
    },
    {
      "name": "defaultskillsenclosement",
      "unusual": true
    },
    {
      "name": "defaultskillsopening",
      "unusual": true
    },
    {
      "name": "globalskill",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "globalskill{${1:arg}}"
      }
    },
    {
      "name": "globalskill",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "globalskill[${2:opt}]{${1:arg}}"
      }
    },
    {
      "name": "glsgobblenumber",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "glsgobblenumber{${1:arg}}"
      }
    },
    {
      "name": "multiskills",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "multiskills{${1:arg1}}"
