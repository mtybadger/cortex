        "format": "{}",
        "snippet": "skillsinmarginvadjust{${1:length}}"
      }
    },
    {
      "name": "bracketedskills"
    },
    {
      "name": "nobracketedskills"
    },
    {
      "name": "boxedskills"
    },
    {
      "name": "noboxedskills"
    },
    {
      "name": "onlyskills"
    },
    {
      "name": "notonlyskills"
