{
  "deps": [
    {
      "name": "setspace"
    }
  ],
  "macros": [
    {
      "name": "spacesetup",
      "arg": {
        "format": "{}",
        "snippet": "spacesetup{${1:options%keyvals}}",
        "keys": [
          "\\usepackage/setspaceenhanced#c,\\spacesetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/setspaceenhanced#c,\\spacesetup": [
      "byselectfont",
      "doublespacing",
      "keepfontsize",
      "onehalfspacing",
      "singlespacing"
    ]
  },
  "args": [
    "\\usepackage/setspaceenhanced#c,\\spacesetup"
  ]
}