{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-ctexbeamer"
    },
    {
      "name": "xeCJKfntef"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "CJKhl",
      "arg": {
        "format": "{}{}",
        "snippet": "CJKhl{${1:color}}{${2:text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/bitbeamer#c": [
      "titlegraphic=${1:imagefile}",
      "framelogo=${1:imagefile}",
      "GBK",
      "UTF8",
      "zhmap=${1|true,false,zhmCJK|}",
      "zihao=${1|-4,5,false|}",
      "heading",
      "scheme=${1|chinese,plain|}",
      "linespread=${1:number}",
      "fontset=${1|adobe,fandol,founder,mac,macnew,macold,ubuntu,windows,none|}",
      "punct=${1|quanjiao,banjiao,kaiming,CCT,plain|}",
      "space=${1|true,false,auto|}",
      "autoindent=",
      "usepdftitle",
      "envcountsect",
      "notheorems",
      "noamsthm",
      "compress",
      "t",
      "c",
      "leqno",
      "fleqn",
      "handout",
      "trans",
      "ignorenonframetext",
      "noamssymb",
      "bigger",
      "smaller",
      "8pt",
      "9pt",
      "10pt",
      "11pt",
      "12pt",
      "14pt",
      "17pt",
      "20pt",
      "draft",
      "onlytextwidth",
      "pgf={${1:pgf options}}",
      "hyperref={${1:hyperref options}}",
      "color={${1:color options}}",
      "xcolor={${1:xcolor options}}",
      "aspectratio=${2|2013,1610,169,149,141,54,43,32,${1:xxxx}|}"
    ]
  },
  "args": [
    "\\documentclass/bitbeamer#c"
  ]
}