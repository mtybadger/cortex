{
  "deps": [
    {
      "name": "xifthen"
    },
    {
      "name": "ragged2e"
    },
    {
      "name": "geometry"
    },
    {
      "name": "changepage"
    },
    {
      "name": "optparams"
    },
    {
      "name": "placeins"
    },
    {
      "name": "fancyhdr"
    }
  ],
  "macros": [
    {
      "name": "floatalignment",
      "unusual": true
    },
    {
      "name": "forcerectofloat"
    },
    {
      "name": "forceversofloat"
    },
    {
      "name": "gsetlength",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "gsetlength{${1:length cmd}}{${2:length}}"
      }
    },
    {
      "name": "morefloats"
    },
    {
      "name": "newlinetospace",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "newlinetospace{${1:arg}}"
      }
    },
    {
      "name": "setcaptionfont",
      "arg": {
        "format": "{}",
        "snippet": "setcaptionfont{${1:font commands}}"
      }
    },
    {
      "name": "setfloatalignment",
      "arg": {
        "format": "{}",
        "snippet": "setfloatalignment{${1:pos%keyvals}}",
        "keys": [
          "\\setfloatalignment#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [
    {
      "name": "fullwidth"
    },
    {
      "name": "marginfigure"
    },
    {
      "name": "marginfigure",
      "arg": {
        "format": "[]",
        "snippet": "[${1:offset%l}]"
      }
    },
    {
      "name": "margintable"
    },
    {
      "name": "margintable",
      "arg": {
        "format": "[]",
        "snippet": "[${1:offset%l}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/bidituftefloat#c": [
      "reversegeometry"
    ],
    "\\setfloatalignment#c": [
      "t",
      "b"
    ]
  },
  "args": [
    "\\usepackage/bidituftefloat#c"
  ]
}