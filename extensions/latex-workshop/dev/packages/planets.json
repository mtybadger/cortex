{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "pgfkeys"
    },
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibrarydecorations.pathmorphing"
    },
    {
      "name": "xstring"
    }
  ],
  "macros": [
    {
      "name": "planet",
      "arg": {
        "format": "[]",
        "snippet": "planet[${1:keyvals}]",
        "keys": [
          "\\planet"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\planet": [
      "surface=${1|mercury,venus,earth,mars,jupiter,saturn,uranus,neptune,pluto,sun,moon|}",
      "rings",
      "ring=${1:radius}",
      "tilt=${1:degrees}",
      "rotation",
      "retrograde",
      "phase=${1|new,first crescent,first half,waxing gibbous,full,waning gibbous,last half,last crescent|}",
      "scale=",
      "centerx=${1:x-coord}",
      "centery=${1:y-coord}",
      "center={${1:(x,y)}}"
    ]
  },
  "args": []
}