{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "tikzlibraryfadings"
    },
    {
      "name": "fontenc"
    },
    {
      "name": "xetex",
      "if": "numerals=dvn"
    },
    {
      "name": "zhnumber",
      "if": "numerals=zh"
    }
  ],
  "macros": [
    {
      "name": "pgfPTdvnfont",
      "arg": {
        "format": "{}",
        "snippet": "pgfPTdvnfont{${1:font name}}"
      }
    },
    {
      "name": "pgfPTdvnfont",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfPTdvnfont[${2:font options}]{${1:font name}}"
      }
    },
    {
      "name": "pgfPTzhfont",
      "arg": {
        "format": "{}",
        "snippet": "pgfPTzhfont{${1:font name}}"
      }
    },
    {
      "name": "pgfPTzhfont",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfPTzhfont[${2:font options}]{${1:font name}}"
      }
    },
    {
      "name": "pgfPTzhnumber",
      "arg": {
        "format": "{}",
        "snippet": "pgfPTzhnumber{${1:list}}"
      }
    },
    {
      "name": "pgfPTzhnumber",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfPTzhnumber[${2:true|false}]{${1:list}}"
      }
    },
    {
      "name": "pgfPT"
    },
    {
      "name": "pgfPT",
      "arg": {
        "format": "[]",
        "snippet": "pgfPT[${1:options%keyvals}]",
        "keys": [
          "\\pgfPT#c,\\pgfPTstyle#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfPTstyle",
      "arg": {
        "format": "[]",
        "snippet": "pgfPTstyle[${1:options%keyvals}]",
        "keys": [
          "\\pgfPT#c,\\pgfPTstyle#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfPTresetstyle"
    },
    {
      "name": "pgfPTbuildcell",
      "arg": {
        "format": "()[]",
        "snippet": "pgfPTbuildcell(${2:rows},${3:columns})[${1:entries}]"
      }
    },
    {
      "name": "pgfPTresetcell"
    },
    {
      "name": "pgfPTbuildcellstyle",
      "arg": {
        "format": "{}()[]",
        "snippet": "pgfPTbuildcellstyle{${1:name%specialDef}}(${3:rows},${4:columns})[${2:entries}]"
      }
    },
    {
      "name": "pgfPTpreviewcell"
    },
    {
      "name": "pgfPTpreviewcell",
      "arg": {
        "format": "[]",
        "snippet": "pgfPTpreviewcell[${1:scale}]"
      }
    },
    {
      "name": "pgfPTpreviewcellstyle",
      "arg": {
        "format": "{}",
        "snippet": "pgfPTpreviewcellstyle{${1:name}}"
      }
    },
    {
      "name": "pgfPTpreviewcellstyle",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfPTpreviewcellstyle[${2:scale}]{${1:name}}"
      }
    },
    {
      "name": "pgfPTnewColorScheme",
      "arg": {
        "format": "{}{}",
        "snippet": "pgfPTnewColorScheme{${1:name%specialDef}}{${2:r1/g1/b1,r2/g2/b2,...}}"
      }
    },
    {
      "name": "pgfPTnewColorScheme",
      "arg": {
        "format": "[]{}{}",
        "snippet": "pgfPTnewColorScheme[${3:r/g/b}]{${1:name}}{${2:r1/g1/b1,r2/g2/b2,...}}"
      }
    },
    {
      "name": "pgfPTnewZlist",
      "arg": {
        "format": "{}{}",
        "snippet": "pgfPTnewZlist{${1:name%specialDef}}{${2:list}}"
      }
    },
    {
      "name": "usepgfPTlibrary",
      "arg": {
        "format": "{}",
        "snippet": "usepgfPTlibrary{${1:library%keyvals}}",
        "keys": [
          "\\usepgfPTlibrary#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfPTGroupColors",
      "arg": {
        "format": "{}{}",
        "snippet": "pgfPTGroupColors{${1:new colorscheme name}}{${2:options%keyvals}}",
        "keys": [
          "\\pgfPTGroupColors#c"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "pgfPTGroupColors",
      "arg": {
        "format": "[]{}{}",
        "snippet": "pgfPTGroupColors[${3:default color}]{${1:new colorscheme name}}{${2:options%keyvals}}",
        "keys": [
          "\\pgfPTGroupColors#c"
        ],
        "keyPos": 2
      }
    },
    {
      "name": "pgfPTPeriodColors",
      "arg": {
        "format": "{}{}",
        "snippet": "pgfPTPeriodColors{${1:new colorscheme name}}{${2:options%keyvals}}",
        "keys": [
          "\\pgfPTPeriodColors#c"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "pgfPTPeriodColors",
      "arg": {
        "format": "[]{}{}",
        "snippet": "pgfPTPeriodColors[${3:default color}]{${1:new colorscheme name}}{${2:options%keyvals}}",
        "keys": [
          "\\pgfPTPeriodColors#c"
        ],
        "keyPos": 2
      }
    },
    {
      "name": "pgfPTCScombine",
      "arg": {
        "format": "{}",
        "snippet": "pgfPTCScombine{${1:colorscheme1,colorscheme2,new colorscheme name}}"
      }
    },
    {
      "name": "pgfPTCScombine",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfPTCScombine[${2:proportion,mode}]{${1:colorscheme1,colorscheme2,new colorscheme name}}"
      }
    },
    {
      "name": "pgfPTCSwrite",
      "arg": {
        "format": "{}",
        "snippet": "pgfPTCSwrite{${1:list of colorschemes}}"
      }
    },
    {
      "name": "pgfPTCSwrite",
      "arg": {
        "format": "[]{}",
        "snippet": "pgfPTCSwrite[${2:filename%file}]{${1:list of colorschemes}}"
      }
