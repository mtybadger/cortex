{
  "deps": [
    {
      "name": "kvoptions"
    },
    {
      "name": "url"
    }
  ],
  "macros": [
    {
      "name": "urisetup",
      "arg": {
        "format": "{}",
        "snippet": "urisetup{${1:options%keyvals}}",
        "keys": [
          "\\urisetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "uref",
      "arg": {
        "format": "{}{}",
        "snippet": "uref{${1:URL}}{${2:link text}}"
      }
    },
    {
      "name": "arxiv",
      "arg": {
        "format": "{}",
        "snippet": "arxiv{${1:arXiv path}}"
      }
    },
    {
      "name": "asin",
      "arg": {
        "format": "{}",
        "snippet": "asin{${1:Amazon number}}"
      }
    },
    {
      "name": "doi",
      "arg": {
        "format": "{}",
        "snippet": "doi{${1:DOI number}}"
      }
    },
    {
      "name": "hdl",
      "arg": {
        "format": "{}",
        "snippet": "hdl{${1:handle.net path}}"
      }
    },
    {
      "name": "nbn",
      "arg": {
        "format": "{}",
        "snippet": "nbn{${1:NBN spec}}"
      }
    },
    {
      "name": "oclc",
      "arg": {
        "format": "{}",
        "snippet": "oclc{${1:WorldCat number}}"
      }
    },
    {
      "name": "oid",
      "arg": {
        "format": "{}",
        "snippet": "oid{${1:OID number}}"
      }
    },
    {
      "name": "pubmed",
      "arg": {
        "format": "{}",
        "snippet": "pubmed{${1:PubMed number}}"
      }
    },
    {
      "name": "tinyuri",
      "arg": {
        "format": "{}",
        "snippet": "tinyuri{${1:TinyURL path}}"
      }
    },
    {
      "name": "tinypuri",
      "arg": {
        "format": "{}",
        "snippet": "tinypuri{${1:TinyURL path}}"
      }
    },
    {
      "name": "wc",
      "arg": {
        "format": "{}",
        "snippet": "wc{${1:WebCite path}}"
      }
    },
    {
      "name": "citeurl",
      "arg": {
        "format": "{}",
        "snippet": "citeurl{${1:URL}}"
      }
    },
    {
      "name": "mailto",
      "arg": {
        "format": "{}",
        "snippet": "mailto{${1:address}}"
      }
    },
    {
      "name": "mailto",
      "arg": {
        "format": "[]{}",
        "snippet": "mailto[${2:subject}]{${1:address}}"
      }
    },
    {
      "name": "ukoeln",
      "arg": {
        "format": "{}",
        "snippet": "ukoeln{${1:UKoeln address}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\urisetup": [
      "arxivpre={%|}",
      "arxivpost={%|}",
      "asinpre={%|}",
      "asinpost={%|}",
      "doipre={%|}",
      "doipost={%|}",
      "hdlpre={%|}",
      "hdlpost={%|}",
      "nbnpre={%|}",
      "nbnpost={%|}",
      "oclcpre={%|}",
      "oclcpost={%|}",
      "oidpre={%|}",
      "oidpost={%|}",
      "pubmedpre={%|}",
      "pubmedpost={%|}",
      "tinyuripre={%|}",
      "tinyuripost={%|}",
      "tinypuripre={%|}",
      "tinypuripost={%|}",
      "wcpre={%|}",
      "wcpost={%|}",
      "xmpppre={%|}",
      "xmpppost={%|}",
      "citeurlpre={%|}",
      "citeurlpost={%|}",
      "mailtopre={%|}",
      "mailtopost={%|}",
      "ukoelnpre={%|}",
      "ukoelnpost={%|}"
