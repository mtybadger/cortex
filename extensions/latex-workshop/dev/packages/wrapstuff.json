{
  "deps": [
    {
      "name": "l3keys2e"
    }
  ],
  "macros": [
    {
      "name": "wrapstuffset",
      "arg": {
        "format": "{}",
        "snippet": "wrapstuffset{${1:options%keyvals}}",
        "keys": [
          "\\begin{wrapstuff},\\wrapstuffset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "wrapstuffclear"
    }
  ],
  "envs": [
    {
      "name": "wrapstuff"
    },
    {
      "name": "wrapstuff",
      "arg": {
        "format": "[]",
        "snippet": "[${1:options%keyvals}]",
        "keys": [
          "\\begin{wrapstuff},\\wrapstuffset"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\begin{wrapstuff},\\wrapstuffset": [
      "top=${1:integer}",
      "width=",
      "height=",
      "leftsep=",
      "rightsep=",
      "hsep=",
      "abovesep=",
      "belowsep=",
      "vsep=",
      "l",
      "r",
      "c",
      "i",
      "o",
      "ratio=${1:real number}",
      "column=${1|true,false,par|}",
      "hoffset=",
      "voffset=",
      "type=${1:float type}",
      "lines=${1:integer}",
      "linewidth="
    ]
  },
  "args": []
}