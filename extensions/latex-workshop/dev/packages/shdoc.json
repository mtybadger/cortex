{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "float"
    },
    {
      "name": "caption"
    },
    {
      "name": "mdframed"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "relsize"
    },
    {
      "name": "stringstrings"
    },
    {
      "name": "ifthen"
    }
  ],
  "macros": [
    {
      "name": "shchange",
      "arg": {
        "format": "{}{}",
        "snippet": "shchange{${1:key%keyvals}}{${2:value}}",
        "keys": [
          "\\shchange,\\shchangecolor",
          "\\shchange,\\shchangesymbol",
          "\\shchange"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "shchangecolor",
      "arg": {
        "format": "{}{}",
        "snippet": "shchangecolor{${1:color key%keyvals}}{${2:color}}",
        "keys": [
          "\\shchange,\\shchangecolor"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "shchangesymbol",
      "arg": {
        "format": "{}{}",
        "snippet": "shchangesymbol{${1:symbol key%keyvals}}{${2:symbol}}",
        "keys": [
          "\\shchange,\\shchangesymbol"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "shpreset",
      "arg": {
        "format": "{}",
        "snippet": "shpreset{${1:option%keyvals}}",
        "keys": [
          "\\shpreset#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "shpresetdef",
      "arg": {
        "format": "{}{}",
        "snippet": "shpresetdef{${1:name}}{${2:definition}}"
      }
    },
    {
      "name": "shuser",
      "arg": {
        "format": "{}",
        "snippet": "shuser{${1:user name}}"
      }
    },
    {
      "name": "shmachine",
      "arg": {
        "format": "{}",
        "snippet": "shmachine{${1:machine name}}"
      }
    },
    {
      "name": "shline",
      "arg": {
        "format": "{}{}",
        "snippet": "shline{${1:path}}{${2:command}}"
      }
    },
    {
      "name": "shpath"
    },
    {
      "name": "shoutput",
      "arg": {
        "format": "{}{}",
        "snippet": "shoutput{${1:shell cmd options}}{${2:text%plain}}"
      }
    },
    {
      "name": "shlistname",
      "arg": {
        "format": "{}",
        "snippet": "shlistname{${1:sessions list name}}"
      }
    },
    {
      "name": "shfloatname",
      "arg": {
        "format": "{}",
        "snippet": "shfloatname{${1:sessions float name}}"
      }
    },
    {
      "name": "shread",
      "arg": {
        "format": "{}{}",
        "snippet": "shread{${1:init cmd}}{${2:file}}"
      }
    },
    {
      "name": "shrun",
      "arg": {
        "format": "{}",
        "snippet": "shrun{${1:shell command}}"
      }
    },
    {
      "name": "shautoread",
      "arg": {
        "format": "{}{}",
        "snippet": "shautoread{${1:init cmd}}{${2:file}}"
      }
    },
    {
      "name": "shautorun",
      "arg": {
        "format": "{}",
        "snippet": "shautorun{${1:shell command}}"
      }
    },
    {
      "name": "shautopath",
      "arg": {
        "format": "{}",
        "snippet": "shautopath{${1:true or false}}"
      }
    },
    {
      "name": "shautoformat",
      "arg": {
        "format": "{}",
        "snippet": "shautoformat{${1:true or false}}"
      }
    },
    {
      "name": "shclearfiles"
    },
    {
      "name": "listofsh"
    },
    {
      "name": "theshlinenumber"
    }
  ],
  "envs": [
    {
      "name": "sh"
    },
    {
      "name": "shbox"
    }
  ],
  "keys": {
    "\\shchange,\\shchangecolor": [
      "backgroundcolor",
      "usernamecolor",
      "machinenamecolor",
      "indicatorcolor",
      "separatorcolor",
      "pathcolor",
      "optioncolor",
      "textcolor"
    ],
    "\\shchange,\\shchangesymbol": [
      "indicatorsymbol",
      "separatorsymbol"
    ],
    "\\shchange": [
      "defaultpath=${1:path}"
    ],
    "\\shpreset#c": [
      "default",
      "mint",
      "arrows",
      "darkblue",
      "airy",
      "blackwhite"
    ]
  },
  "args": []
}