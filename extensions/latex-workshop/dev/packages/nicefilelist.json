      "if": "wrap"
    },
    {
      "name": "xstring",
      "if": "autolength"
    }
  ],
  "macros": [
    {
      "name": "NFLspaceI"
    },
    {
      "name": "NFLspaceII"
    },
    {
      "name": "NFLspaceIII"
    },
    {
      "name": "NFLnodate"
    },
    {
      "name": "NFLnoversion"
    },
    {
      "name": "NFLnotfound"
    },
    {
      "name": "MaxBaseEmptyList",
