    {
      "name": "NoProcess"
    },
    {
      "name": "NoProcess",
      "arg": {
        "format": "[]",
        "snippet": "NoProcess[${1:ranges}]"
      }
    },
    {
      "name": "configure",
      "arg": {
        "format": "[][]",
        "snippet": "configure[${1:mpgraphic}][${2:keyvals}]",
