    "\\usepackage/mpgraphics#c": [
      "metapost={${1:MetaPost options}}",
      "epstopdf={${1:epstopdf options}}",
      "compilation=${1|on,off|}",
      "debug=${1|on,off|}",
      "runs=${1:integer}"
    ],
    "\\configure": [
      "width=",
      "height=",
      "scale=${1:factor}",
      "color=",
      "linecolor=",
      "background=",
      "linewidth=",
