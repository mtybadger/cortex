{
  "deps": [
    {
      "name": "pstricks"
    },
    {
      "name": "pst-plot"
    },
    {
      "name": "multido"
    },
    {
      "name": "pst-xkey"
    }
  ],
  "macros": [
    {
      "name": "Oscillo"
    },
    {
      "name": "Oscillo",
      "arg": {
        "format": "[]",
        "snippet": "Oscillo[${1:options%keyvals}]",
        "keys": [
          "\\Oscillo#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "TriangleA"
    },
    {
      "name": "TriangleB"
    },
    {
      "name": "RectangleA"
    },
    {
      "name": "RectangleB"
    },
    {
      "name": "RDogToothA"
    },
    {
      "name": "RDogToothB"
    },
    {
      "name": "LDogToothA"
    },
    {
      "name": "LDogToothB"
    },
    {
      "name": "SinusA"
    },
    {
      "name": "SinusB"
    }
  ],
  "envs": [],
  "keys": {
    "\\Oscillo#c": [
      "period1=${1:number}",
      "period2=${1:number}",
      "periodmodulation1=${1:number}",
      "freqmod1=${1:number}",
      "periodmodulation2=${1:number}",
      "freqmod2=${1:number}",
      "amplitude1=${1:number}",
      "amplitude2=${1:number}",
      "CC1=${1:number}",
      "CC2=${1:number}",
      "timediv=${1:number}",
      "phase1=${1:number}",
      "phase2=${1:number}",
      "sensivity1=${1:number}",
      "sensivity2=${1:number}",
      "damping1=${1:number}",
      "damping2=${1:number}",
      "Fourier=${1:number}",
      "offset1=${1:number}",
      "offset2=${1:number}",
      "offset3=${1:number}",
      "plotstyle1=${1:style}",
      "plotstyle2=${1:style}",
      "plotstyle3=${1:style}",
      "plotstyle4=${1:style}",
      "operation=${1|add,sub,mul|}",
      "Wave1=${1|\\TriangleA,\\RectangleA,\\RDogToothA,\\LDogToothA,\\SinusA|}",
      "Wave2=${1|\\TriangleB,\\RectangleB,\\RDogToothB,\\LDogToothB,\\SinusB|}",
      "Lissajous",
      "AllColor",
      "combine",
      "plotpoints=${1:number}"
    ]
  },
  "args": []
}