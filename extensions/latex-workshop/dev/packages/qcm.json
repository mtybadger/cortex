{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "calc"
    },
    {
      "name": "verbatim"
    },
    {
      "name": "tabularx"
    }
  ],
  "macros": [
    {
      "name": "headerfont",
      "arg": {
        "format": "{}",
        "snippet": "headerfont{${1:font commands}}"
      }
    },
    {
      "name": "X",
      "arg": {
        "format": "{}",
        "snippet": "X{${1:symbol}}"
      }
    },
    {
      "name": "pbs",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "pbs{${1:arg}}"
      }
    },
    {
      "name": "questionspace"
    },
    {
      "name": "answerstitle",
      "arg": {
        "format": "{}",
        "snippet": "answerstitle{${1:text}}"
      }
    },
    {
      "name": "answerstitlefont",
      "arg": {
        "format": "{}",
        "snippet": "answerstitlefont{${1:font commands}}"
      }
    },
    {
      "name": "answernumberfont",
      "arg": {
        "format": "{}",
        "snippet": "answernumberfont{${1:font commands}}"
      }
    },
    {
      "name": "thequestion"
    },
    {
      "name": "truesymbol",
      "arg": {
        "format": "{}",
        "snippet": "truesymbol{${1:symbol}}"
      }
    },
    {
      "name": "falsesymbol",
      "arg": {
        "format": "{}",
        "snippet": "falsesymbol{${1:symbol}}"
      }
    },
    {
      "name": "true"
    },
    {
      "name": "false"
    },
    {
      "name": "correctionstyle",
      "arg": {
        "format": "{}",
        "snippet": "correctionstyle{${1:font commands}}"
      }
    },
    {
      "name": "makeform"
    },
    {
      "name": "makemask"
    }
  ],
  "envs": [
    {
      "name": "question",
      "arg": {
        "format": "{}",
        "snippet": "{${1:question text%text}}"
      }
    },
    {
      "name": "correction"
    }
  ],
  "keys": {
    "\\usepackage/qcm#c": [
      "english",
      "french",
      "francais",
      "nocorrection",
      "correction"
    ]
  },
  "args": [
    "\\usepackage/qcm#c"
  ]
}