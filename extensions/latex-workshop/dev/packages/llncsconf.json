{
  "deps": [
    {
      "name": "hyperref"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "rcsinfo",
      "if": "rcsinfo"
    },
    {
      "name": "eso-pic",
      "if": "svninfo"
    },
    {
      "name": "svninfo",
      "if": "svninfo"
    }
  ],
  "macros": [
    {
      "name": "conference",
      "arg": {
        "format": "{}",
        "snippet": "conference{${1:conference name%text}}"
      }
    },
    {
      "name": "llncs",
      "arg": {
        "format": "{}{}",
        "snippet": "llncs{${1:text}}{${2:page number}}"
      }
    },
    {
      "name": "llncsdoi",
      "arg": {
        "format": "{}",
        "snippet": "llncsdoi{${1:DOI}}"
      }
    },
    {
      "name": "copyrightnote"
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/llncsconf#c": [
      "crop",
      "nocrop",
      "rcsinfo",
      "svninfo",
      "accepted",
      "submitted",
      "intended",
      "llncs",
      "proceedings"
    ]
  },
  "args": [
    "\\usepackage/llncsconf#c"
  ]
}