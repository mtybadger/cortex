{
  "deps": [
    {
      "name": "etoolbox"
    },
    {
      "name": "pgfopts"
    },
    {
      "name": "geometry"
    },
    {
      "name": "microtype"
    },
    {
      "name": "newtxtext"
    },
    {
      "name": "newtxmath"
    },
    {
      "name": "titlesec"
    },
    {
      "name": "float"
    },
    {
      "name": "caption"
    },
    {
      "name": "booktabs"
    },
    {
      "name": "biblatex"
    },
    {
      "name": "nowidow"
    },
    {
      "name": "url"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "hyperref"
    },
    {
      "name": "titleps"
    }
  ],
  "macros": [
    {
      "name": "iscramset",
      "arg": {
        "format": "{}",
        "snippet": "iscramset{${1:options%keyvals}}",
        "keys": [
          "\\documentclass/iscram#c,\\iscramset"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "abstract",
      "arg": {
        "format": "{}",
        "snippet": "abstract{${1:text}}"
      }
    },
    {
      "name": "keywords",
      "arg": {
        "format": "{}",
        "snippet": "keywords{${1:keywords%text}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\documentclass/iscram#c,\\iscramset": [
      "draft",
      "anonymous",
      "first alone",
      "title=${1:title}",
      "short title=${1:short title}",
      "author={short name=${1:short name},full name=${2:full name},affiliation=${3:affiliation}}",
      "footer/line 1=${1:text}",
      "footer/line 2=${1:text}",
      "footer/line 3=${1:text}",
      "iscram 2017 footer",
      "WiPe Paper 2017=${1:track name}",
      "CoRe Paper 2017=${1:track name}",
      "iscram 2018 footer",
      "WiPe Paper 2018=${1:track name}",
      "CoRe Paper 2018=${1:track name}"
    ]
  },
  "args": [
    "\\documentclass/iscram#c,\\iscramset"
  ]
}