{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "kvoptions"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "xcolor"
    },
    {
      "name": "marginnote"
    }
  ],
  "macros": [
    {
      "name": "phfMakeCommentingCommand",
      "arg": {
        "format": "{}",
        "snippet": "phfMakeCommentingCommand{${1:cmd name}}"
      }
    },
    {
      "name": "phfMakeCommentingCommand",
      "arg": {
        "format": "[]{}",
        "snippet": "phfMakeCommentingCommand[${2:options%keyvals}]{${1:cmd name}}",
        "keys": [
          "\\phfMakeCommentingCommand#c,\\phfDefineCommentingStyle#c,\\phfSetDefaultCommentingStyle#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "phfDisableCommentingCommands"
    },
    {
      "name": "phfDefineCommentingStyle",
      "arg": {
        "format": "{}{}",
        "snippet": "phfDefineCommentingStyle{${1:style name}}{${2:options%keyvals}}"
      }
    },
    {
      "name": "phfSetDefaultCommentingStyle",
      "arg": {
        "format": "{}",
        "snippet": "phfSetDefaultCommentingStyle{${1:options%keyvals}}"
      }
    },
    {
      "name": "phfccformatmargininitials",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "phfccformatmargininitials{${1:initials}}"
      }
    },
    {
      "name": "phfccmargininitialssep",
      "unusual": true
    },
    {
      "name": "phfccformatboxinitials",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "phfccformatboxinitials{${1:initials}}"
      }
    },
    {
      "name": "phfCCChangesBy",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "phfCCChangesBy{${1:initials}}"
      }
    },
    {
      "name": "phfCommentingDefaultStartCmds",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultEndCmds",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultSpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultEnd",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCSpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultCEnd",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmSpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultRmEnd",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultIFont",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultISpacing",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultIBegin",
      "unusual": true
    },
    {
      "name": "phfCommentingDefaultIEnd",
      "unusual": true
    },
    {
      "name": "phfccfootcommenttextstyle",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\phfMakeCommentingCommand#c,\\phfDefineCommentingStyle#c,\\phfSetDefaultCommentingStyle#c": [
      "color=",
      "initials={${1:initials}}",
      "formatinitials=${1:format cmds}",
      "formatinitials=${1|default,margin,box,nobox,hide,footnote|}",
      "style=${1|footcomments,rmstrikethrough|}",
      "font={${1:font cmds}}",
      "spacing=",
      "begin=",
      "end=",
      "cfont={${1:font cmds}}",
      "cspacing=",
      "cbegin=",
      "cend=",
      "rmfont={${1:font cmds}}",
      "rmspacing=",
      "rmbegin=",
      "rmend=",
      "ifont={${1:font cmds}}",
      "ispacing=",
      "ibegin=",
      "iend="
    ]
  },
  "args": []
}