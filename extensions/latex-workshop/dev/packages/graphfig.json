{
  "deps": [
    {
      "name": "graphics"
    },
    {
      "name": "subfigure",
      "if": "subfigure"
    },
    {
      "name": "float",
      "if": "AllowH"
    }
  ],
  "macros": [
    {
      "name": "graphfile",
      "arg": {
        "format": "{}",
        "snippet": "graphfile{${1:imagefile}}"
      }
    },
    {
      "name": "graphfile",
      "arg": {
        "format": "{}[]",
        "snippet": "graphfile{${1:imagefile}}[${2:subcaption%text}]"
      }
    },
    {
      "name": "graphfile",
      "arg": {
        "format": "[]{}",
        "snippet": "graphfile[${2:width}]{${1:imagefile}}"
      }
    },
    {
      "name": "graphfile",
      "arg": {
        "format": "[]{}[]",
        "snippet": "graphfile[${2:width}]{${1:imagefile}}[${3:subcaption%text}]"
      }
    },
    {
      "name": "graphfile*",
      "arg": {
        "format": "{}",
        "snippet": "graphfile*{${1:imagefile}}"
      }
    },
    {
      "name": "graphfile*",
      "arg": {
        "format": "{}[]",
        "snippet": "graphfile*{${1:imagefile}}[${2:subcaption%text}]"
      }
    },
    {
      "name": "graphfile*",
      "arg": {
        "format": "[]{}",
        "snippet": "graphfile*[${2:height}]{${1:imagefile}}"
      }
    },
    {
      "name": "graphfile*",
      "arg": {
        "format": "[]{}[]",
        "snippet": "graphfile*[${2:height}]{${1:imagefile}}[${3:subcaption%text}]"
      }
    },
    {
      "name": "FigureDefaultPlacement",
      "arg": {
        "format": "{}",
        "snippet": "FigureDefaultPlacement{${1:placement}}"
      }
    },
    {
      "name": "thesubfigure",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "Figure",
      "arg": {
        "format": "{}",
        "snippet": "{${1:caption%text}}"
      }
    },
    {
      "name": "Figure",
      "arg": {
        "format": "{}[]",
        "snippet": "{${1:caption%text}}[${2:label}]"
      }
    },
    {
      "name": "Figure",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:placement}]{${1:caption%text}}"
      }
    },
    {
      "name": "Figure",
      "arg": {
        "format": "[]{}[]",
        "snippet": "[${2:placement}]{${1:caption%text}}[${3:label}]"
      }
    }
  ],
  "keys": {
    "\\usepackage/graphfig#c": [
      "subfigure",
      "AllowH"
    ]
  },
  "args": [
    "\\usepackage/graphfig#c"
  ]
}