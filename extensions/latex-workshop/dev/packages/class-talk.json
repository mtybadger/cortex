{
  "deps": [
    {
      "name": "multido"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "graphicx"
    },
    {
      "name": "pgf"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "title",
      "arg": {
        "format": "[]{}",
        "snippet": "title[${2:short title%text}]{${1:text}}"
      }
    },
    {
      "name": "author",
      "arg": {
        "format": "[]{}",
        "snippet": "author[${2:short author}]{${1:names}}"
      }
    },
    {
      "name": "slidesmag",
      "arg": {
        "format": "{}",
        "snippet": "slidesmag{${1:factor}}"
      }
    },
    {
      "name": "slidestyle",
      "arg": {
        "format": "{}",
        "snippet": "slidestyle{${1:style%keyvals}}"
      }
    },
    {
      "name": "fromslide",
      "arg": {
        "format": "{}{}",
        "snippet": "fromslide{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "fromslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "fromslide*{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "toslide",
      "arg": {
        "format": "{}{}",
        "snippet": "toslide{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "toslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "toslide*{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "onlyslide",
      "arg": {
        "format": "{}{}",
        "snippet": "onlyslide{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "onlyslide*",
      "arg": {
        "format": "{}{}",
        "snippet": "onlyslide*{${1:slide number}}{${2:material%text}}"
      }
    },
    {
      "name": "tableofcontents",
      "arg": {
        "format": "[]",
        "snippet": "tableofcontents[${1:<fromsec>.<${2:fromsubsec}>-<${3:tosec}>.<${4:tosubsec}>}]"
      }
    },
    {
      "name": "slidewidth",
      "unusual": true
    },
    {
      "name": "slideheight",
      "unusual": true
    },
    {
      "name": "theslide",
      "unusual": true
    },
    {
      "name": "thesubslide",
      "unusual": true
    },
    {
      "name": "theslidelabel",
      "unusual": true
    },
    {
      "name": "slidesadjust",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "slidesadjust{${1:hadjust}}{${2:vadjust}}"
      }
    }
  ],
  "envs": [
    {
      "name": "slide",
      "arg": {
        "format": "{}",
        "snippet": "{${1:title%text}}"
      }
    },
    {
      "name": "slide",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:style%keyvals}]{${1:title%text}}"
      }
    },
    {
      "name": "notes"
    },
    {
      "name": "multislide",
      "arg": {
        "format": "{}{}",
        "snippet": "{${1:sub-slides}}{${2:title%text}}"
      }
    },
    {
      "name": "multislide",
      "arg": {
        "format": "[]{}{}",
        "snippet": "[${3:style%keyvals}]{${1:sub-slides}}{${2:title%text}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/talk#c": [
      "notes",
      "slides",
      "screen",
      "rotate",
      "norotate",
      "a4paper",
      "a5paper",
      "b5paper",
      "letterpaper",
      "legalpaper",
      "executivepaper",
      "landscape",
      "10pt",
      "11pt",
      "12pt",
      "oneside",
      "twoside",
      "draft",
      "final",
      "titlepage",
      "notitlepage",
      "onecolumn",
      "twocolumn",
      "leqno",
      "fleqn",
      "openbib"
    ]
  },
  "args": [
    "\\documentclass/talk#c"
  ]
}