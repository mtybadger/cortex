      "name": "Massstab",
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "Massstab{${1:Zahl1}}{${2:Einheit1%formula}}{${3:Zahl2}}{${4:Einheit2%formula}}"
      }
    },
    {
      "name": "tqty",
      "arg": {
        "format": "{}{}",
        "snippet": "tqty{${1:Zahl}}{${2:Einheit%formula}}"
      }
    },
    {
      "name": "tunit",
      "arg": {
        "format": "{}",
        "snippet": "tunit{${1:Einheit%formula}}"
      }
    },
    {
      "name": "Beschl",
      "arg": {
        "format": "{}",
        "snippet": "Beschl{${1:Zahl}}"
      }
    },
    {
      "name": "Erdb"
    },
    {
      "name": "Ortsf"
    },
    {
      "name": "Elem"
    },
    {
      "name": "Elekm"
    },
    {
      "name": "Lichtg"
    },
    {
      "name": "Planck"
    },
    {
      "name": "EFK"
    },
    {
      "name": "MFK"
    },
    {
      "name": "MFK*"
    },
    {
      "name": "Messschieber",
      "arg": {
        "format": "{}",
        "snippet": "Messschieber{${1:Messwert}}"
      }
    },
    {
      "name": "Messschieber",
      "arg": {
        "format": "[]{}",
        "snippet": "Messschieber[${2:cm|${3:inch}|cm+inch}]{${1:Messwert}}"
      }
    },
    {
      "name": "Messschraube",
      "arg": {
        "format": "{}",
        "snippet": "Messschraube{${1:Messwert}}"
      }
    },
    {
      "name": "Kraftmesser",
      "arg": {
        "format": "{}{}",
        "snippet": "Kraftmesser{${1:Messwert}}{${2:Maximalausschlag}}"
      }
    },
    {
      "name": "Multimeter",
      "arg": {
        "format": "{}{}{}",
        "snippet": "Multimeter{${1:Messwert}}{${2:Maximalausschlag}}{${3:Ma√üeinheit}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/schulma-physik#c": [
      "circuitikz"
    ],
    "\\node#c": [
      "spannungsquelle",
      "messgeraet"
    ]
  },
  "args": [
    "\\usepackage/schulma-physik#c"
  ]
}