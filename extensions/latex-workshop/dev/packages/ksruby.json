{
  "deps": [
    {
      "name": "stackengine"
    }
  ],
  "macros": [
    {
      "name": "ksruby",
      "arg": {
        "format": "{}{}",
        "snippet": "ksruby{${1:below}}{${2:above}}"
      }
    },
    {
      "name": "ksruby*",
      "arg": {
        "format": "{}{}",
        "snippet": "ksruby*{${1:below}}{${2:above}}"
      }
    },
    {
      "name": "ruby",
      "arg": {
        "format": "{}{}",
        "snippet": "ruby{${1:below}}{${2:above}}"
      }
    },
    {
      "name": "ruby*",
      "arg": {
        "format": "{}{}",
        "snippet": "ruby*{${1:below}}{${2:above}}"
      }
    },
    {
      "name": "ksrubycenterdefault",
      "arg": {
        "format": "{}",
        "snippet": "ksrubycenterdefault{${1:options}}"
      }
    },
    {
      "name": "ksrubyheightdefault"
    },
    {
      "name": "ksrubyheightdefault",
      "arg": {
        "format": "[]",
        "snippet": "ksrubyheightdefault[${1:false}]"
      }
    },
    {
      "name": "ksrubywidthdefault"
    },
    {
      "name": "ksrubywidthdefault",
      "arg": {
        "format": "[]",
        "snippet": "ksrubywidthdefault[${1:false}]"
      }
    },
    {
      "name": "ksrubyeachchardefault"
    },
    {
      "name": "ksrubyeachchardefault",
      "arg": {
        "format": "[]",
        "snippet": "ksrubyeachchardefault[${1:false}]"
      }
    },
    {
      "name": "ksrubysep",
      "arg": {
        "format": "{}",
        "snippet": "ksrubysep{${1:dim%l}}"
      }
    },
    {
      "name": "ksrubysize",
      "arg": {
        "format": "{}",
        "snippet": "ksrubysize{${1:size}}"
      }
    },
    {
      "name": "ksrubyextra",
      "arg": {
        "format": "{}",
        "snippet": "ksrubyextra{${1:cmds}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/ksruby#c": [
      "rubysep=",
      "rubysize=${1:size}",
      "rubyeachchar",
      "rubywidth",
      "rubyheight",
      "rubycenter=${1|c,l,r|}"
    ],
    "\\ksrubycenter#c": [
      "c",
      "l",
      "r"
    ]
  },
  "args": [
    "\\usepackage/ksruby#c"
  ]
}