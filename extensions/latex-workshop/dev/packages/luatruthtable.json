{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "amsmath"
    },
    {
      "name": "luacode"
    }
  ],
  "macros": [
    {
      "name": "luaTruthTable",
      "arg": {
        "format": "{}{}",
        "snippet": "luaTruthTable{${1:var1,var2,...}}{${2:expr1,expr2,...}}"
      }
    },
    {
      "name": "luaTruthTable",
      "arg": {
        "format": "[]{}{}",
        "snippet": "luaTruthTable[${3:options%keyvals}]{${1:var1,var2,...}}{${2:expr1,expr2,...}}",
        "keys": [
          "\\luaTruthTable"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\luaTruthTable": [
      "trtext=${1:text}",
      "fltext=${1:text}"
    ]
  },
  "args": []
}