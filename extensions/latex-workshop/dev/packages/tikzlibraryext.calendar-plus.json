{
  "deps": [
    {
      "name": "tikzlibrarycalendar"
    },
    {
      "name": "pgfcalendar-ext"
    }
  ],
  "macros": [
    {
      "name": "pgfmathweeksinmonthofyear",
      "unusual": true,
      "arg": {
        "format": "{}{}{}",
        "snippet": "pgfmathweeksinmonthofyear{${1:first weekday}}{${2:month}}{${3:year}}"
      }
    },
    {
      "name": "pgfmathlastdayinmonthofyear",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "pgfmathlastdayinmonthofyear{${1:month}}{${2:year}}"
      }
    },
    {
      "name": "tikzweekcode",
      "unusual": true
    },
    {
      "name": "tikzweektext",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\calendar#c": [
      "day xshift=",
      "day yshift=",
      "month xshift=",
      "month yshift=",
      "week code=${1:code}",
      "week text=${1:text}",
      "every week/.append style={${1:TikZ keys}}",
      "week label left"
    ]
  },
  "args": []
}