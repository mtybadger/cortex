{
  "deps": [
    {
      "name": "natbib"
    }
  ],
  "macros": [
    {
      "name": "multibibliography",
      "arg": {
        "format": "{}",
        "snippet": "multibibliography{${1:bibfile1,bibfile2,...}}"
      }
    },
    {
      "name": "multibibliographystyle",
      "arg": {
        "format": "{}{}",
        "snippet": "multibibliographystyle{${1:type%keyvals}}{${2:style}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "citealn",
      "arg": {
        "format": "{}",
        "snippet": "citealn{${1:keylist}}"
      }
    },
    {
      "name": "citeall",
      "arg": {
        "format": "{}",
        "snippet": "citeall{${1:keylist}}"
      }
    },
    {
      "name": "citeall*",
      "arg": {
        "format": "{}",
        "snippet": "citeall*{${1:keylist}}"
      }
    },
    {
      "name": "printbibliography",
      "arg": {
        "format": "{}",
        "snippet": "printbibliography{${1:type%keyvals}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "nmbibRedirectLinks",
      "arg": {
        "format": "{}{}",
        "snippet": "nmbibRedirectLinks{${1:source type%keyvals}}{${2:target type}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "nmbibLink",
      "arg": {
        "format": "{}{}{}",
        "snippet": "nmbibLink{${1:keylist}}{${2:type%keyvals}}{${3:text}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "timelinerefname",
      "unusual": true
    },
    {
      "name": "sequencerefname",
      "unusual": true
    },
    {
      "name": "authorsrefname",
      "unusual": true
    },
    {
      "name": "timelinebibname",
      "unusual": true
    },
    {
      "name": "sequencebibname",
      "unusual": true
    },
    {
      "name": "authorsbibname",
      "unusual": true
    },
    {
      "name": "multibibliographyfilename",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "multibibliographyfilename{${1:type%keyvals}}{${2:filename%file}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "nmbibBasetype",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "nmbibBasetype{${1:type%keyvals}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "nmbibSetCiteall",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "nmbibSetCiteall{${1:pattern}}"
      }
    },
    {
      "name": "nmbibKEY",
      "unusual": true
    },
    {
      "name": "nmbibNAME",
      "unusual": true
    },
    {
      "name": "nmbibDATE",
      "unusual": true
    },
    {
      "name": "nmbibNUM",
      "unusual": true
    },
    {
      "name": "nmbibSetBiblabel",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "nmbibSetBiblabel{${1:type%keyvals}}{${2:pattern}}",
        "keys": [
          "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "nmbibYearSuffixOff",
      "unusual": true
    },
    {
      "name": "nmbibYearSuffixOn",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/nmbib#c": [
      "round",
      "square",
      "curly",
      "angle",
      "semicolon",
      "colon",
      "comma",
      "authoryear",
      "numbers",
      "super",
      "sort",
      "sort&compress",
      "compress",
      "longnamesfirst",
      "sectionbib",
      "nonamebreak",
      "merge",
      "elide",
      "mcite"
    ],
    "\\multibibliographystyle,\\printbibliography,\\nmbibRedirectLinks,\\nmbibLink,\\multibibliographyfilename,\\nmbibBasetype,\\nmbibSetBiblabel": [
      "timeline",
      "sequence",
      "authors"
    ]
  },
  "args": [
    "\\usepackage/nmbib#c"
  ]
}