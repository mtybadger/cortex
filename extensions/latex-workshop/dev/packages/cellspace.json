{
  "deps": [
    {
      "name": "ifthen"
    },
    {
      "name": "array"
    },
    {
      "name": "calc"
    },
    {
      "name": "xkeyval"
    },
    {
      "name": "amsmath",
      "if": "math"
    }
  ],
  "macros": [
    {
      "name": "cellspacetoplimit"
    },
    {
      "name": "cellspacebottomlimit"
    },
    {
      "name": "bcolumn",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "bcolumn{${1:arg}}"
      }
    },
    {
      "name": "ecolumn",
      "unusual": true
    },
    {
      "name": "addparagraphcolumntypes",
      "arg": {
        "format": "{}",
        "snippet": "addparagraphcolumntypes{${1:col type}}"
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/cellspace#c": [
      "math",
      "nomath",
      "column=${1:column type}"
    ]
  },
  "args": [
    "\\usepackage/cellspace#c"
  ]
