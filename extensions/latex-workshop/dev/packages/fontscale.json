      "footnotesize=${1:size}/${2:baselineskip}",
      "small/scale=${1:number}",
      "small/size=${1:size}",
      "small/baselineskip=",
      "small=${1:size}",
      "small=${1:size}/${2:baselineskip}",
      "normalsize/scale=${1:number}",
      "normalsize/size=${1:size}",
      "normalsize/baselineskip=",
      "normalsize=${1:size}",
      "normalsize=${1:size}/${2:baselineskip}",
      "large/scale=${1:number}",
      "large/size=${1:size}",
      "large/baselineskip=",
      "large=${1:size}",
      "large=${1:size}/${2:baselineskip}",
      "Large/scale=${1:number}",
      "Large/size=${1:size}",
      "Large/baselineskip=",
      "Large=${1:size}",
      "Large=${1:size}/${2:baselineskip}",
      "LARGE/scale=${1:number}",
      "LARGE/size=${1:size}",
      "LARGE/baselineskip=",
      "LARGE=${1:size}",
      "LARGE=${1:size}/${2:baselineskip}",
      "huge/scale=${1:number}",
      "huge/size=${1:size}",
      "huge/baselineskip=",
      "huge=${1:size}",
      "huge=${1:size}/${2:baselineskip}",
      "Huge/scale=${1:number}",
