{
  "deps": [],
  "macros": [
    {
      "name": "fontscalesetup",
      "arg": {
        "format": "{}",
        "snippet": "fontscalesetup{${1:keyvals}}",
        "keys": [
          "\\fontscalesetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "CurrentFontStep"
    },
    {
      "name": "CurrentFontScale"
    },
    {
      "name": "CurrentFontSize"
    },
    {
      "name": "CurrentFontBaselineskip"
    },
    {
      "name": "SetFontStep",
      "arg": {
        "format": "{}",
        "snippet": "SetFontStep{${1:number}}"
      }
    },
    {
      "name": "SetFontStep*",
      "arg": {
        "format": "{}",
        "snippet": "SetFontStep*{${1:number}}"
      }
    },
    {
      "name": "SetFontScale",
      "arg": {
        "format": "{}",
        "snippet": "SetFontScale{${1:number}}"
      }
    },
    {
      "name": "SetFontScale*",
      "arg": {
        "format": "{}",
        "snippet": "SetFontScale*{${1:number}}"
      }
    },
    {
      "name": "SetFontSize",
      "arg": {
        "format": "{}",
        "snippet": "SetFontSize{${1:size}}"
      }
    },
    {
      "name": "SetFontSize*",
      "arg": {
        "format": "{}",
        "snippet": "SetFontSize*{${1:size}}"
      }
    },
    {
      "name": "ScaleFont",
      "arg": {
        "format": "{}",
        "snippet": "ScaleFont{${1:number}}"
      }
    },
    {
      "name": "SetFontSizeBaselineskip",
      "arg": {
        "format": "{}{}",
        "snippet": "SetFontSizeBaselineskip{${1:size}}{${2:skip%l}}"
      }
    },
    {
      "name": "PrintFontParameters"
    },
    {
      "name": "PrintAllFontParameters"
    },
    {
      "name": "PrintSampleText"
    },
    {
      "name": "PrintSampleText",
      "arg": {
        "format": "[]",
        "snippet": "PrintSampleText[${1:text}]"
      }
    },
    {
      "name": "PrintSampleText*"
    },
    {
      "name": "PrintSampleText*",
      "arg": {
        "format": "[]",
        "snippet": "PrintSampleText*[${1:text}]"
      }
    },
    {
      "name": "PrintFontSizeCommand"
    }
  ],
  "envs": [],
  "keys": {
    "\\fontscalesetup": [
      "reset",
      "reset=${1|initial,preamble|}",
      "ignore-order",
      "baselineskip-size-ratio=${1:number}",
      "typographic-scale=${1|classic-10pt,classic-11pt,classic-12pt,modular,musical|}",
      "classic-10pt",
      "classic-11pt",
      "classic-12pt",
      "modular",
      "musical",
      "modular/ratio=${1:number}",
      "musical/ratio=${1:number}",
      "musical/notes=${1:integer}",
      "tiny/scale=${1:number}",
      "tiny/size=${1:size}",
      "tiny/baselineskip=",
      "tiny=${1:size}",
      "tiny=${1:size}/${2:baselineskip}",
      "scriptsize/scale=${1:number}",
      "scriptsize/size=${1:size}",
      "scriptsize/baselineskip=",
      "scriptsize=${1:size}",
      "scriptsize=${1:size}/${2:baselineskip}",
      "footnotesize/scale=${1:number}",
      "footnotesize/size=${1:size}",
