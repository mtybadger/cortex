    },
    {
      "name": "CircledViolationsOff"
    },
    {
      "name": "ShadingOn"
    },
    {
      "name": "ShadingOff"
    },
    {
      "name": "FingerBeforeLetter"
    },
    {
      "name": "LetterBeforeFinger"
    },
    {
      "name": "mcand",
      "arg": {
        "format": "{}",
        "snippet": "mcand{${1:candidate%text}}"
      }
    },
    {
      "name": "mcand",
      "arg": {
        "format": "[]{}",
        "snippet": "mcand[${2:symbol}]{${1:candidate%text}}"
      }
    },
    {
      "name": "mcand*",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "mcand*{${1:candidate%text}}"
      }
    },
    {
      "name": "mcand*",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "mcand*[${2:symbol}]{${1:candidate%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "tableau",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    }
  ],
  "keys": {
    "\\usepackage/ot-tableau#c": [
      "notipa",
      "circledviolations",
      "shadedcells",
      "fingerafter",
      "usehhline"
    ]
  },
  "args": [
    "\\usepackage/ot-tableau#c"
  ]
