  "macros": [
    {
      "name": "inp",
      "arg": {
        "format": "{}",
        "snippet": "inp{${1:input%text}}"
      }
    },
    {
      "name": "ips",
      "arg": {
        "format": "{}",
        "snippet": "ips{${1:text}}"
      }
