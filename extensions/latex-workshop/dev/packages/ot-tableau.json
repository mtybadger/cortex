    {
      "name": "cand*",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "cand*{${1:candidate%text}}"
      }
    },
    {
      "name": "cand*",
      "unusual": true,
      "arg": {
        "format": "[]{}",
        "snippet": "cand*[${2:symbol}]{${1:candidate%text}}"
      }
    },
    {
      "name": "vio",
      "arg": {
        "format": "{}",
        "snippet": "vio{${1:violation%text}}"
      }
    },
    {
      "name": "vio*",
      "arg": {
        "format": "{}",
        "snippet": "vio*{${1:violation%text}}"
      }
    },
    {
      "name": "SetCellShading",
      "arg": {
        "format": "{}",
        "snippet": "SetCellShading{${1:factor}}"
      }
    },
    {
      "name": "Optimal"
    },
    {
      "name": "OptimalMarker",
      "unusual": true
    },
    {
      "name": "ViolationMarker",
      "unusual": true
    },
    {
