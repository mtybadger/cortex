{
  "deps": [
    {
      "name": "xstring"
    },
    {
      "name": "amssymb"
    },
    {
      "name": "bbding"
    },
    {
      "name": "suffix"
    },
    {
      "name": "colortbl"
    },
    {
      "name": "rotating"
    },
    {
      "name": "arydshln"
    },
    {
      "name": "hhline",
      "if": "usehhline"
    }
