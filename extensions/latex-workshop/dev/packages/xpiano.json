{
  "deps": [
    {
      "name": "xcolor"
    }
  ],
  "macros": [
    {
      "name": "keyboard",
      "arg": {
        "format": "{}",
        "snippet": "keyboard{${1:notes}}"
      }
    },
    {
      "name": "keyboard",
      "arg": {
        "format": "[]{}",
        "snippet": "keyboard[${2:options%keyvals}]{${1:notes}}",
        "keys": [
          "\\keyboard,\\keyboardsetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "keyboardsetup",
      "arg": {
        "format": "{}",
        "snippet": "keyboardsetup{${1:options%keyvals}}",
        "keys": [
          "\\keyboard,\\keyboardsetup"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\keyboard,\\keyboardsetup": [
      "color=",
      "single",
      "ext",
      "size=",
      "height=${1:number}",
      "ratio=${1:number}",
      "numbers",
      "font=${1:font commands}",
      "numbercolor=",
      "10=${1:code}",
      "11=${1:code}"
    ]
  },
  "args": []
}