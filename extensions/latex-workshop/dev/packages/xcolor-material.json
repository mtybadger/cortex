{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "kvoptions"
    }
  ],
  "macros": [
    {
      "name": "printcolorvalue",
      "arg": {
        "format": "{}",
        "snippet": "printcolorvalue{${1:color}}"
      }
    },
    {
      "name": "printcolorvalue",
      "arg": {
        "format": "[]{}",
        "snippet": "printcolorvalue[${2:model}]{${1:color}}"
      }
    },
    {
      "name": "colorsample",
      "arg": {
        "format": "{}",
        "snippet": "colorsample{${1:color}}"
      }
    },
    {
      "name": "colorsample",
      "arg": {
        "format": "[][][]{}[]",
        "snippet": "colorsample[${2:model}][${3:textcolor%color}][${4:box width%l}]{${1:color}}[${5:title%text}]"
      }
    },
    {
      "name": "colorpalette",
      "arg": {
        "format": "{}",
        "snippet": "colorpalette{${1:color}}"
      }
    },
    {
      "name": "colorpalette",
      "arg": {
        "format": "[]{}",
        "snippet": "colorpalette[${2:options%keyvals}]{${1:color}}",
        "keys": [
          "\\colorpalette"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/xcolor-material#c": [
      "prefix=${1:prefix}"
    ],
    "\\colorpalette": [
      "width=",
      "height=",
      "shape=${1:shape}",
      "title=",
      "title text color=",
      "init text color=",
      "altern text color=",
      "variation font=",
      "value font=",
      "title font=",
      "colorbox sep=",
      "first colorbox sep=",
      "title colorbox height=",
      "primary palette=",
      "secondary palette=",
      "percent char=",
      "primary variation=",
      "primary toggle at=",
      "secondary toggle at=",
      "color model=",
      "both",
      "primary",
      "basic"
    ]
  },
  "args": [
    "\\usepackage/xcolor-material#c"
  ]
}