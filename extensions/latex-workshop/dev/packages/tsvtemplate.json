{
  "deps": [
    {
      "name": "luatex"
    },
    {
      "name": "environ"
    }
  ],
  "macros": [
    {
      "name": "tsvtemplate",
      "arg": {
        "format": "{}{}",
        "snippet": "tsvtemplate{${1:template}}{${2:file}}"
      }
    },
    {
      "name": "tsvtemplate",
      "arg": {
        "format": "[]{}{}",
        "snippet": "tsvtemplate[${3:colname1,colname2,...}]{${1:template}}{${2:file}}"
      }
    },
    {
      "name": "applytemplate",
      "unusual": true,
      "arg": {
        "format": "{}{}{}{}",
        "snippet": "applytemplate{${1:separator}}{${2:colnames}}{${3:template}}{${4:file}}"
      }
    }
  ],
  "envs": [
    {
      "name": "tsv template",
      "arg": {
        "format": "{}",
        "snippet": "{${1:file}}"
      }
    },
    {
      "name": "tsv template",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:colname1,colname2,...}]{${1:file}}"
      }
    }
  ],
  "keys": {},
  "args": []
}