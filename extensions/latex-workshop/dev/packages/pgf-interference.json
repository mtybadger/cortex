{
  "deps": [
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "pgfinterferencepattern",
      "arg": {
        "format": "{}",
        "snippet": "pgfinterferencepattern{${1:options%keyvals}}",
        "keys": [
          "\\pgfinterferencepattern,\\pgfinterferenceoptions"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pgfinterferenceoptions",
      "arg": {
        "format": "{}",
        "snippet": "pgfinterferenceoptions{${1:options%keyvals}}",
        "keys": [
          "\\pgfinterferencepattern,\\pgfinterferenceoptions"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/pgf-interference#c": [
      "draft"
    ],
    "\\pgfinterferencepattern,\\pgfinterferenceoptions": [
      "wavelength=${1:number}",
      "intensity=${1:number}",
      "slits=${1:integer}",
      "slit-distance=${1:number}",
      "slit-width=${1:number}",
      "screen-distance=${1:number}",
      "screen-width=${1:number}",
      "screen-height=${1:number}",
      "scale=${1:factor}",
      "screen-color=",
      "ruler=${1|above,below,screen,none|}"
    ]
  },
  "args": [
    "\\usepackage/pgf-interference#c"
  ]
}