{
  "deps": [
    {
      "name": "xkeyval"
    }
  ],
  "macros": [
    {
      "name": "fitbox",
      "arg": {
        "format": "{}",
        "snippet": "fitbox{${1:content%text}}"
      }
    },
    {
      "name": "fitbox",
      "arg": {
        "format": "[]{}",
        "snippet": "fitbox[${2:options%keyvals}]{${1:content%text}}",
        "keys": [
          "\\fitbox,\\fitbox*,\\fitboxset,\\SetFitboxLayout"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fitbox*",
      "arg": {
        "format": "{}",
        "snippet": "fitbox*{${1:content%text}}"
      }
    },
    {
      "name": "fitbox*",
      "arg": {
        "format": "[]{}",
        "snippet": "fitbox*[${2:options%keyvals}]{${1:content%text}}",
        "keys": [
          "\\fitbox,\\fitbox*,\\fitboxset,\\SetFitboxLayout"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fitboxset",
      "arg": {
        "format": "{}",
        "snippet": "fitboxset{${1:options%keyvals}}",
        "keys": [
          "\\fitbox,\\fitbox*,\\fitboxset,\\SetFitboxLayout"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fitboxnatwidth"
    },
    {
      "name": "fitboxnatheight"
    },
    {
      "name": "SetFitboxLayout",
      "arg": {
        "format": "{}{}",
        "snippet": "SetFitboxLayout{${1:rows}}{${2:columns}}"
      }
    },
    {
      "name": "SetFitboxLayout",
      "arg": {
        "format": "[]{}{}",
        "snippet": "SetFitboxLayout[${3:options%keyvals}]{${1:rows}}{${2:columns}}",
        "keys": [
          "\\fitbox,\\fitbox*,\\fitboxset,\\SetFitboxLayout"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\fitbox,\\fitbox*,\\fitboxset,\\SetFitboxLayout": [
      "maxheight=",
      "maxwidth=",
      "minheight=",
      "minwidth=",
      "belowboxspace=",
      "maxenlargepage=",
      "colsep=",
      "colsepexpr=${1:space macro}",
      "maincapheight=",
      "maincaplines=${1:integer}",
      "subcapheight=",
      "subcaplines=${1:integer}"
    ]
  },
  "args": []
}