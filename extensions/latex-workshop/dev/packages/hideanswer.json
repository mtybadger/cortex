{
  "deps": [
    {
      "name": "color"
    }
  ],
  "macros": [
    {
      "name": "sethideanswer"
    },
    {
      "name": "unsethideanswer"
    },
    {
      "name": "hideanswer",
      "arg": {
        "format": "{}",
        "snippet": "hideanswer{${1:text}}"
      }
    },
    {
      "name": "hideanswer*",
      "arg": {
        "format": "{}",
        "snippet": "hideanswer*{${1:text}}"
      }
    },
    {
      "name": "smashanswer",
      "arg": {
        "format": "{}",
        "snippet": "smashanswer{${1:text}}"
      }
    },
    {
      "name": "smashanswer*",
      "arg": {
        "format": "{}",
        "snippet": "smashanswer*{${1:text}}"
      }
    },
    {
      "name": "hidegraphics",
      "arg": {
        "format": "{}",
        "snippet": "hidegraphics{${1:imagefile}}"
      }
    },
    {
      "name": "hidegraphics",
      "arg": {
        "format": "[]{}",
        "snippet": "hidegraphics[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\hidegraphics,\\hidegraphics*,\\smashgraphics,\\smashgraphics*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "hidegraphics*",
      "arg": {
        "format": "{}",
        "snippet": "hidegraphics*{${1:imagefile}}"
      }
    },
    {
      "name": "hidegraphics*",
      "arg": {
        "format": "[]{}",
        "snippet": "hidegraphics*[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\hidegraphics,\\hidegraphics*,\\smashgraphics,\\smashgraphics*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "smashgraphics",
      "arg": {
        "format": "{}",
        "snippet": "smashgraphics{${1:imagefile}}"
      }
    },
    {
      "name": "smashgraphics",
      "arg": {
        "format": "[]{}",
        "snippet": "smashgraphics[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\hidegraphics,\\hidegraphics*,\\smashgraphics,\\smashgraphics*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "smashgraphics*",
      "arg": {
        "format": "{}",
        "snippet": "smashgraphics*{${1:imagefile}}"
      }
    },
    {
      "name": "smashgraphics*",
      "arg": {
        "format": "[]{}",
        "snippet": "smashgraphics*[${2:options%keyvals}]{${1:imagefile}}",
        "keys": [
          "\\hidegraphics,\\hidegraphics*,\\smashgraphics,\\smashgraphics*"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "switchanswer",
      "arg": {
        "format": "{}{}",
        "snippet": "switchanswer{${1:sethide-text%text}}{${2:unsethide-text%text}}"
      }
    },
    {
      "name": "switchanswer*",
      "arg": {
        "format": "{}{}",
        "snippet": "switchanswer*{${1:unsethide-text%text}}{${2:sethide-text%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "hideanswerdiv"
    },
    {
      "name": "hideanswerdiv*"
    },
    {
      "name": "smashanswerdiv"
    },
    {
      "name": "smashanswerdiv*"
    }
  ],
  "keys": {
    "\\hidegraphics,\\hidegraphics*,\\smashgraphics,\\smashgraphics*": [
      "alt={${1:alt text}}",
      "bb=${1:llx lly urx ury}",
      "bbllx=",
      "bblly=",
      "bburx=",
      "bbury=",
      "natwidth=",
      "natheight=",
      "hiresbb",
      "pagebox=${1|mediabox,cropbox,bleedbox,trimbox,artbox|}",
      "viewport=${1:llx lly urx ury}",
      "trim=${1:llx lly urx ury}",
      "angle=${1:degrees}",
      "origin=",
      "width=",
      "height=",
      "totalheight=",
      "keepaspectratio",
      "scale=${1:factor}",
      "clip",
      "draft",
      "type=${1:file type}",
      "ext=${1:file extension}",
      "read=${1:read-file extension}",
      "command=",
      "quiet",
      "page=${1:page number}",
      "interpolate",
      "decodearray={${1:color array}}"
    ]
  },
  "args": []
}