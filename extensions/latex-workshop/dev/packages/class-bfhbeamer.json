{
  "deps": [
    {
      "name": "l3keys2e"
    },
    {
      "name": "class-beamer"
    },
    {
      "name": "beamerthemeBFH"
    },
    {
      "name": "bfhmodule"
    },
    {
      "name": "bfhpub",
      "if": "beamerarticle"
    },
    {
      "name": "beamerarticle",
      "if": "beamerarticle"
    },
    {
      "name": "bfhpub",
      "if": "beamerarticle=true"
    },
    {
      "name": "beamerarticle",
      "if": "beamerarticle=true"
    },
    {
      "name": "handoutWithNotes",
      "if": "printNotes"
    },
    {
      "name": "handoutWithNotes",
      "if": "paperNotes"
    },
    {
      "name": "colortbl"
    }
  ],
  "macros": [
    {
      "name": "BFHarraystretch",
      "unusual": true
    },
    {
      "name": "BFHarrayrulewidth",
      "unusual": true
    },
    {
      "name": "setupBfhTabular"
    }
  ],
  "envs": [
    {
      "name": "bfhTabular",
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    },
    {
      "name": "bfhTabular",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:pos}]{${1:preamble}}"
      }
    },
    {
      "name": "bfhTblr",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "{${1:preamble}}"
      }
    }
  ],
  "keys": {
    "\\documentclass/bfhbeamer#c": [
      "authorontitle",
      "logofile=${1:file}",
      "enforce-title-logo=",
      "beamerarticle",
      "printNotes",
      "tabletNotes",
      "paperNotes",
      "usepdftitle",
      "envcountsect",
      "notheorems",
      "noamsthm",
      "compress",
      "t",
      "c",
      "leqno",
      "fleqn",
      "handout",
      "trans",
      "pdftex",
      "nativepdf",
      "pdfmark",
      "dvips",
      "dviwindo",
      "dvipsone",
      "vtex",
      "ps2pdf",
      "ignorenonframetext",
      "noamssymb",
      "bigger",
      "smaller",
      "8pt",
      "9pt",
      "10pt",
      "11pt",
      "12pt",
      "14pt",
      "17pt",
      "20pt",
      "draft",
      "CJK",
      "cjk",
      "onlytextwidth",
      "pgf={${1:pgf options}}",
      "hyperref={${1:hyperref options}}",
      "color={${1:color options}}",
      "xcolor={${1:xcolor options}}",
      "ucs",
      "utf8x",
      "utf8",
      "aspectratio=${2|2013,1610,169,149,141,54,43,32,${1:xxxx}|}"
    ]
  },
  "args": [
    "\\documentclass/bfhbeamer#c"
  ]
}