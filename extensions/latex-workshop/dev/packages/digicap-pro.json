  "macros": [
    {
      "name": "digiCap",
      "arg": {
        "format": "{}{}",
        "snippet": "digiCap{${1:imagefile}}{${2:caption%text}}"
      }
    },
    {
      "name": "digiCap",
      "arg": {
        "format": "{}[]{}",
        "snippet": "digiCap{${1:imagefile}}[${3:design keyvals}]{${2:caption%text}}"
      }
    },
    {
      "name": "digiCap",
      "arg": {
        "format": "[]{}{}",
        "snippet": "digiCap[${3:pos keyvals%keyvals}]{${1:imagefile}}{${2:caption%text}}",
        "keys": [
          "\\digiCap,\\digiCap*,\\dcFirstOpt"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "digiCap",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "digiCap[${3:pos keyvals%keyvals}]{${1:imagefile}}[${4:design keyvals}]{${2:caption%text}}",
        "keys": [
          "\\digiCap,\\digiCap*,\\dcFirstOpt"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "digiCap*",
      "arg": {
        "format": "{}{}",
        "snippet": "digiCap*{${1:imagefile}}{${2:caption%text}}"
      }
    },
    {
      "name": "digiCap*",
      "arg": {
        "format": "{}[]{}",
        "snippet": "digiCap*{${1:imagefile}}[${3:design keyvals}]{${2:caption%text}}"
      }
    },
    {
      "name": "digiCap*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "digiCap*[${3:pos keyvals%keyvals}]{${1:imagefile}}{${2:caption%text}}",
        "keys": [
          "\\digiCap,\\digiCap*,\\dcFirstOpt"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "digiCap*",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "digiCap*[${3:pos keyvals%keyvals}]{${1:imagefile}}[${4:design keyvals}]{${2:caption%text}}",
        "keys": [
          "\\digiCap,\\digiCap*,\\dcFirstOpt"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "graphicHeight"
    },
    {
      "name": "graphicWidth"
    },
    {
      "name": "opcolorbox",
      "arg": {
        "format": "{}",
        "snippet": "opcolorbox{${1:text}}"
      }
    },
    {
      "name": "opcolorbox",
      "arg": {
        "format": "[]{}",
        "snippet": "opcolorbox[${2:options%keyvals}]{${1:text}}",
        "keys": [
          "\\opcolorbox,\\dcSecondOpt"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "presentationOrder",
