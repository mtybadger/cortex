        "format": "{}{}",
        "snippet": "digiDisplaySpace{${1:height}}{${2:width}}"
      }
    },
    {
      "name": "insertCaptions",
      "if": "display1"
    },
    {
      "name": "insertThumbs",
      "if": "display1",
      "arg": {
        "format": "{}{}",
        "snippet": "insertThumbs{${1:rows}}{${2:columns}}"
      }
    },
    {
      "name": "dcFirstOpt",
      "if": "display1",
      "arg": {
        "format": "{}",
        "snippet": "dcFirstOpt{${1:pos keyvals%keyvals}}"
      }
    },
    {
      "name": "dcFirstOpt",
      "if": "display1",
      "arg": {
        "format": "[]{}",
        "snippet": "dcFirstOpt[${2:name}]{${1:pos keyvals%keyvals}}"
      }
    },
    {
      "name": "dcSecondOpt",
      "if": "display1",
      "arg": {
        "format": "{}",
        "snippet": "dcSecondOpt{${1:design keyvals%keyvals}}"
      }
    },
    {
      "name": "dcSecondOpt",
      "if": "display1",
      "arg": {
        "format": "[]{}",
        "snippet": "dcSecondOpt[${2:name}]{${1:design keyvals%keyvals}}"
      }
    },
    {
      "name": "useRollovers",
      "if": "display1"
    },
    {
      "name": "noRollovers",
      "if": "display1"
    },
    {
      "name": "longCapFmt",
      "if": "display1",
      "arg": {
        "format": "{}",
        "snippet": "longCapFmt{${1:format cmds}}"
