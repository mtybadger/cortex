    {
      "name": "digiDSWidth",
      "if": "display1",
      "unusual": true
    },
    {
      "name": "digiDSHeight",
      "if": "display1",
      "unusual": true
    },
    {
      "name": "insertPhotos",
      "if": "display1",
      "unusual": true
    },
    {
      "name": "normalAppr",
      "if": "display1",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "normalAppr{${1:arg}}"
      }
    },
    {
      "name": "downAppr",
      "if": "display1",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "downAppr{${1:arg}}"
      }
    },
    {
      "name": "rolloverAppr",
      "if": "display1",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "rolloverAppr{${1:arg}}"
      }
    },
    {
      "name": "digiCapsPresets",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "digiCapsPresets{${1:arg}}"
      }
    },
    {
      "name": "hiddenPresets",
      "unusual": true
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/digicap-pro#c": [
      "display1"
    ],
    "\\digiCap,\\digiCap*,\\dcFirstOpt": [
      "outerboxsep=",
      "vcaption=${1|b,c,t|}",
      "hcaption=${1|l,c,r|}",
      "inclgraphicx={${1:\\includegraphics keys}}",
      "rollovername=${1:name}"
    ],
    "\\opcolorbox,\\dcSecondOpt": [
      "borderwidth=",
      "fboxsep=",
      "width=",
      "bordercolor=",
      "bgcolor=",
      "borderop=${1:factor}",
      "bgop=${1:factor}",
      "textop=${1:factor}",
      "borderblendmode=${1:mode}",
      "bgblendmode=${1:mode}"
    ],
    "\\setThumbAppearances": [
      "normalop=${1:factor}",
      "rolloverop=${1:factor}",
      "downop=${1:factor}",
      "boundarywidth",
      "rgbcolor=${1:r g b}",
      "cmykcolor=${1:c m y k}"
    ]
  },
  "args": [
    "\\usepackage/digicap-pro#c"
  ]
}