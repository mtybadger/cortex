{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "eforms"
    }
  ],
  "macros": [
    {
      "name": "item",
      "arg": {
        "format": "{}",
        "snippet": "item{${1:keyvals}}",
        "keys": [
          "\\item,\\begin{submenu}",
          "\\item"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "puUseMenus",
      "arg": {
        "format": "{}",
        "snippet": "puUseMenus{${1:menu array names}}"
      }
    },
    {
      "name": "popUpMenu",
      "arg": {
        "format": "()",
        "snippet": "popUpMenu(${1:name})"
      }
    },
    {
      "name": "puProcessMenu",
      "arg": {
        "format": "()",
        "snippet": "puProcessMenu(${1:name})"
      }
    },
    {
      "name": "urlPath",
      "arg": {
        "format": "{}{}",
        "snippet": "urlPath{${1:cmd}}{${2:path%URL}}"
      }
    },
    {
      "name": "Esc"
    },
    {
      "name": "cs",
      "arg": {
        "format": "{}",
        "snippet": "cs{${1:csname}}"
      }
    },
    {
      "name": "itemindex",
      "unusual": true
    },
    {
      "name": "submenuLevel",
      "unusual": true
    },
    {
      "name": "puUseTheseMenus",
      "unusual": true
    },
    {
      "name": "puMenuCmds",
      "unusual": true
    },
    {
      "name": "puNone",
      "unusual": true
    },
    {
      "name": "puTracking",
      "unusual": true
    },
    {
      "name": "iftrackingPU",
      "unusual": true
    },
    {
      "name": "trackingPUfalse",
      "unusual": true
    },
    {
      "name": "trackingPUtrue",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "popupmenu",
      "arg": {
        "format": "{}",
        "snippet": "{${1:name%text}}"
      }
    },
    {
      "name": "submenu",
      "arg": {
        "format": "{}",
        "snippet": "{${1:keyvals}}",
        "keys": [
          "\\item,\\begin{submenu}"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\usepackage/popupmenu#c": [
      "tracking",
      "!tracking"
    ],
    "\\item,\\begin{submenu}": [
      "title=${1:string}",
      "marked"
    ],
    "\\item": [
      "enabled",
      "return=${1:string}"
    ]
  },
  "args": [
    "\\usepackage/popupmenu#c"
  ]
}