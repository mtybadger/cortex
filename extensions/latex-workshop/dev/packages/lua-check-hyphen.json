{
  "deps": [
    {
      "name": "ifluatex"
    },
    {
      "name": "luatexbase"
    },
    {
      "name": "keyval"
    }
  ],
  "macros": [
    {
      "name": "LuaCheckHyphen",
      "arg": {
        "format": "{}",
        "snippet": "LuaCheckHyphen{${1:options%keyvals}}",
        "keys": [
          "\\LuaCheckHyphen,\\usepackage/lua-check-hyphen"
        ],
        "keyPos": 0
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\LuaCheckHyphen,\\usepackage/lua-check-hyphen": [
      "compact",
      "mark",
      "nofile",
      "final",
      "whitelist={${1:files}}"
    ]
  },
  "args": [
    "\\LuaCheckHyphen,\\usepackage/lua-check-hyphen"
  ]
}