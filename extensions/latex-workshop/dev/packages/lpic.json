{
  "deps": [
    {
      "name": "epsfig"
    },
    {
      "name": "rotating"
    },
    {
      "name": "calc"
    },
    {
      "name": "ifthen"
    },
    {
      "name": "color"
    }
  ],
  "macros": [
    {
      "name": "lbl",
      "arg": {
        "format": "{}",
        "snippet": "lbl{${1:coords;LaTeX content}}"
      }
    },
    {
      "name": "lbl",
      "arg": {
        "format": "[]{}",
        "snippet": "lbl[${2:pos}]{${1:coords;LaTeX content}}"
      }
    },
    {
      "name": "lpunitlength",
      "unusual": true
    },
    {
      "name": "lpmarginright",
      "unusual": true
    },
    {
      "name": "lpmarginleft",
      "unusual": true
    },
    {
      "name": "lpmargintop",
      "unusual": true
    },
    {
      "name": "lpmarginbottom",
      "unusual": true
    },
    {
      "name": "lpbgsep",
      "unusual": true
    },
    {
      "name": "lpgridthickness",
      "unusual": true
    },
    {
      "name": "lpframethickness",
      "unusual": true
    },
    {
      "name": "lplblframethickness",
      "unusual": true
    },
    {
      "name": "lpfigframethickness",
      "unusual": true
    },
    {
      "name": "thelpgridstep",
      "unusual": true
    },
    {
      "name": "thelpcoordstep",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "lpic",
      "arg": {
        "format": "{}",
        "snippet": "{${1:filename(${2:x},${3:y})}}"
      }
    },
    {
      "name": "lpic",
      "arg": {
        "format": "[]{}",
        "snippet": "[${2:options}]{${1:filename(${3:x},${4:y})}}",
        "keys": [
          "\\begin{lpic}#c"
        ],
        "keyPos": 0
      }
    }
  ],
  "keys": {
    "\\begin{lpic}#c": [
      "l(${1:length})",
      "r(${1:length})",
      "t(${1:length})",
      "b(${1:length})",
      "grid(${1:step})",
      "coords(${1:step})",
      "frames(${1:length})",
      "frame(${1:length})",
      "figframe(${1:length})",
      "draft",
      "clean",
      "nolbl",
      "nofigure"
    ]
  },
  "args": []
}