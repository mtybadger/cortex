{
  "deps": [
    {
      "name": "xcolor"
    },
    {
      "name": "l3keys2e"
    },
    {
      "name": "longtable"
    },
    {
      "name": "booktabs"
    },
    {
      "name": "caption"
    },
    {
      "name": "fontspec"
    }
  ],
  "macros": [
    {
      "name": "displayfonttable",
      "arg": {
        "format": "{}{}",
        "snippet": "displayfonttable{${1:font name}}{${2:font features}}"
      }
    },
    {
      "name": "displayfonttable",
      "arg": {
        "format": "[]{}{}",
        "snippet": "displayfonttable[${3:options%keyvals}]{${1:font name}}{${2:font features}}",
        "keys": [
          "\\displayfonttable,\\displayfonttable*,\\fonttablesetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "displayfonttable*",
      "arg": {
        "format": "{}{}",
        "snippet": "displayfonttable*{${1:font name}}{${2:font features}}"
      }
    },
    {
      "name": "displayfonttable*",
      "arg": {
        "format": "[]{}{}",
        "snippet": "displayfonttable*[${3:options%keyvals}]{${1:font name}}{${2:font features}}",
        "keys": [
          "\\displayfonttable,\\displayfonttable*,\\fonttablesetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fonttablesetup",
      "arg": {
        "format": "{}",
        "snippet": "fonttablesetup{${1:options%keyvals}}",
        "keys": [
          "\\displayfonttable,\\displayfonttable*,\\fonttablesetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "fonttableglyphcount"
    }
  ],
  "envs": [],
  "keys": {
    "\\displayfonttable,\\displayfonttable*,\\fonttablesetup": [
      "header",
      "noheader",
      "title-format=${1:code}",
      "title-format-cont=${1:code}",
      "display-block=${1|titles,rules,none|}",
      "hex-digits=${1|block,foot,head,head+foot,none|}",
      "hex-digits-font=${1:font commands}",
      "hex-digits-row-format=${1:code}",
      "color=",
      "statistics",
      "nostatistics",
      "statistics-font=${1:font commands}",
      "statistics-format=${1:code}",
      "glyph-width=",
      "missing-glyph=${1:code}",
      "missing-glyph-font=${1:font commands}",
      "missing-glyph-color=",
      "compare-with=${1:font name}",
      "compare-color=",
      "compare-bgcolor=",
      "statistics-compare-format=${1:code}",
      "range-start=${1:slot}",
      "range-end=${1:slot}"
    ]
  },
  "args": []
}