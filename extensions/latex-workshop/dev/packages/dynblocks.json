{
  "deps": [
    {
      "name": "tikz"
    },
    {
      "name": "etoolbox"
    },
    {
      "name": "tikzlibraryshadows",
      "if": "shadow"
    }
  ],
  "macros": [
    {
      "name": "opaqueblock<overlay specification>",
      "arg": {
        "format": "{}",
        "snippet": "opaqueblock<overlay specification>{${1:text}}"
      }
    },
    {
      "name": "opaqueblock<overlay specification>",
      "arg": {
        "format": "[]{}",
        "snippet": "opaqueblock<overlay specification>[${2:width}]{${1:text}}"
      }
    },
    {
      "name": "invblock<overlay specification>"
    },
    {
      "name": "setalignment",
      "arg": {
        "format": "{}",
        "snippet": "setalignment{${1:alignment%keyvals}}",
        "keys": [
          "\\setalignment#c"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "setvisopacity",
      "arg": {
        "format": "{}",
        "snippet": "setvisopacity{${1:opacity}}"
      }
    },
    {
      "name": "setinvopacity",
      "arg": {
        "format": "{}",
        "snippet": "setinvopacity{${1:opacity}}"
      }
    },
    {
      "name": "dynalert<overlay specification>",
      "arg": {
        "format": "{}",
        "snippet": "dynalert<overlay specification>{${1:text}}"
      }
    },
    {
      "name": "setwordscolor",
      "arg": {
        "format": "{}",
        "snippet": "setwordscolor{${1:color}}"
      }
    },
    {
      "name": "setshadowopacity",
      "arg": {
        "format": "{}",
        "snippet": "setshadowopacity{${1:opacity}}"
      }
    },
    {
      "name": "setblockcolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setblockcolor{${1:color}}"
      }
    },
    {
      "name": "setbordercolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setbordercolor{${1:color}}"
      }
    },
    {
      "name": "setinnercolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setinnercolor{${1:color}}"
      }
    },
    {
      "name": "setoutercolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setoutercolor{${1:color}}"
      }
    },
    {
      "name": "settopcolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "settopcolor{${1:color}}"
      }
    },
    {
      "name": "setbottomcolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setbottomcolor{${1:color}}"
      }
    },
    {
      "name": "setleftcolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setleftcolor{${1:color}}"
      }
    },
    {
      "name": "setrightcolor",
      "if": "customcolors",
      "arg": {
        "format": "{}",
        "snippet": "setrightcolor{${1:color}}"
      }
    },
    {
      "name": "fancyblock<overlay specification>",
      "arg": {
        "format": "{}",
        "snippet": "fancyblock<overlay specification>{${1:text}}"
      }
    },
    {
      "name": "fancyblock<overlay specification>",
      "arg": {
        "format": "[]{}",
        "snippet": "fancyblock<overlay specification>[${2:width}]{${1:text}}"
      }
    },
    {
      "name": "vshadeblock<overlay specification>",
      "arg": {
        "format": "{}",
        "snippet": "vshadeblock<overlay specification>{${1:text}}"
      }
    },
    {
      "name": "vshadeblock<overlay specification>",
      "arg": {
        "format": "[]{}",
        "snippet": "vshadeblock<overlay specification>[${2:width}]{${1:text}}"
      }
    },
    {
      "name": "oshadeblock<overlay specification>",
      "arg": {
        "format": "{}",
        "snippet": "oshadeblock<overlay specification>{${1:text}}"
      }
    },
    {
      "name": "oshadeblock<overlay specification>",
      "arg": {
        "format": "[]{}",
        "snippet": "oshadeblock<overlay specification>[${2:width}]{${1:text}}"
      }
    },
    {
      "name": "thecol",
      "unusual": true
    },
    {
      "name": "thebordercol",
      "unusual": true
    },
    {
      "name": "myalert",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "myalert{${1:text}}"
      }
    },
    {
      "name": "printthistext",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "printthistext{${1:id}}"
      }
    },
    {
      "name": "savetext",
      "unusual": true,
      "arg": {
        "format": "{}{}",
        "snippet": "savetext{${1:id}}{${2:text}}"
      }
    },
    {
      "name": "thethistext",
      "unusual": true
    },
    {
      "name": "intdimension",
      "unusual": true
    }
  ],
  "envs": [
    {
      "name": "dynblock"
    }
  ],
  "keys": {
    "\\usepackage/dynblocks#c": [
      "customcolors",
      "getthemecolors",
      "roundedcorners",
      "shadow"
    ],
    "\\setalignment#c": [
      "left",
      "flush left",
      "right",
      "flush right",
      "center",
      "flush center",
      "justify",
      "none"
    ]
  },
  "args": [
    "\\usepackage/dynblocks#c"
  ]
}