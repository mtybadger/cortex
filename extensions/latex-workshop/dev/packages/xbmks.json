{
  "deps": [
    {
      "name": "xkeyval"
    },
    {
      "name": "ifpdf"
    },
    {
      "name": "ifxetex"
    },
    {
      "name": "hyperref"
    }
  ],
  "macros": [
    {
      "name": "xbmksetup",
      "arg": {
        "format": "{}",
        "snippet": "xbmksetup{${1:options%keyvals}}",
        "keys": [
          "\\xbmksetup"
        ],
        "keyPos": 0
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "pdfbookmarkx{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "[]{}{}",
        "snippet": "pdfbookmarkx[${3:level}]{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "pdfbookmarkx{${1:text}}[${3:keyvals}]{${2:name}}",
        "keys": [
          "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "pdfbookmarkx",
      "arg": {
        "format": "[]{}[]{}",
        "snippet": "pdfbookmarkx[${3:level}]{${1:text}}[${4:keyvals}]{${2:name}}",
        "keys": [
          "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx"
        ],
        "keyPos": 2
      }
    },
    {
      "name": "currentpdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "currentpdfbookmarkx{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "currentpdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "currentpdfbookmarkx{${1:text}}[${3:keyvals}]{${2:name}}",
        "keys": [
          "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "subpdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "subpdfbookmarkx{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "subpdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "subpdfbookmarkx{${1:text}}[${3:keyvals}]{${2:name}}",
        "keys": [
          "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx"
        ],
        "keyPos": 1
      }
    },
    {
      "name": "belowpdfbookmarkx",
      "arg": {
        "format": "{}{}",
        "snippet": "belowpdfbookmarkx{${1:text}}{${2:name}}"
      }
    },
    {
      "name": "belowpdfbookmarkx",
      "arg": {
        "format": "{}[]{}",
        "snippet": "belowpdfbookmarkx{${1:text}}[${3:keyvals}]{${2:name}}",
        "keys": [
          "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx"
        ],
        "keyPos": 1
      }
    }
  ],
  "envs": [],
  "keys": {
    "\\usepackage/xbmks#c": [
      "dvipsone",
      "dvips",
      "pdftex",
      "luatex",
      "xetex"
    ],
    "\\xbmksetup": [
      "docbundle={${1:name1,name2,...}}",
      "colors={${1:int=<color>,ext=<color>}}",
      "styles={${1:style1,style2,...}}"
    ],
    "\\pdfbookmarkx,\\currentpdfbookmarkx,\\subpdfbookmarkx,\\belowpdfbookmarkx": [
      "action={${1:PDF action}}",
      "color=",
      "style="
    ]
  },
  "args": [
    "\\usepackage/xbmks#c"
  ]
}