{
  "deps": [
    {
      "name": "class-beamer"
    },
    {
      "name": "framed"
    },
    {
      "name": "tikz"
    }
  ],
  "macros": [
    {
      "name": "fitem",
      "arg": {
        "format": "{}",
        "snippet": "fitem{${1:text}}"
      }
    },
    {
      "name": "pitem",
      "arg": {
        "format": "{}",
        "snippet": "pitem{${1:text}}"
      }
    },
    {
      "name": "customtextcol",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "customtextcol{${1:text}}"
      }
    },
    {
      "name": "slogan",
      "unusual": true,
      "arg": {
        "format": "{}",
        "snippet": "slogan{${1:slogan%text}}"
      }
    },
    {
      "name": "fbckg",
      "arg": {
        "format": "{}",
        "snippet": "fbckg{${1:imagefile}}"
      }
    },
    {
      "name": "thankyou"
    },
    {
      "name": "pointedsl",
      "arg": {
        "format": "{}",
        "snippet": "pointedsl{${1:slogan%text}}"
      }
    },
    {
      "name": "framedsl",
      "arg": {
        "format": "{}",
        "snippet": "framedsl{${1:slogan%text}}"
      }
    },
    {
      "name": "itemized",
      "arg": {
        "format": "{}",
        "snippet": "itemized{${1:itemize contents%text}}"
      }
    },
    {
      "name": "startingslide"
    },
    {
      "name": "misc",
      "arg": {
        "format": "{}",
        "snippet": "misc{${1:content%text}}"
      }
    },
    {
      "name": "sources",
      "arg": {
        "format": "{}",
        "snippet": "sources{${1:list of resources%text}}"
      }
    }
  ],
  "envs": [
    {
      "name": "mybox",
      "unusual": true
    },
    {
      "name": "mybox2",
      "unusual": true
    }
  ],
  "keys": {
    "\\documentclass/fancyslides#c": [
      "usepdftitle",
      "envcountsect",
      "notheorems",
      "noamsthm",
      "compress",
      "t",
      "c",
      "leqno",
      "fleqn",
      "handout",
      "trans",
      "pdftex",
      "nativepdf",
      "pdfmark",
      "dvips",
      "dviwindo",
      "dvipsone",
      "vtex",
      "ps2pdf",
      "ignorenonframetext",
      "noamssymb",
      "bigger",
      "smaller",
      "8pt",
      "9pt",
      "10pt",
      "11pt",
      "12pt",
      "14pt",
      "17pt",
      "20pt",
      "draft",
      "CJK",
      "cjk",
      "onlytextwidth",
      "pgf={${1:pgf options}}",
      "hyperref={${1:hyperref options}}",
      "color={${1:color options}}",
      "xcolor={${1:xcolor options}}",
      "ucs",
      "utf8x",
      "utf8",
      "aspectratio=${2|2013,1610,169,149,141,54,43,32,${1:xxxx}|}"
    ]
  },
  "args": [
    "\\documentclass/fancyslides#c"
  ]
}